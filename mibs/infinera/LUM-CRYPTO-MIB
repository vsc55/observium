        cryptoPmadminIndex                        Unsigned32,
        cryptoPmadminName                         MgmtNameString,
        cryptoPmadminUId						  Unsigned32,
        cryptoPmadminConnAdminIfIndex       	  Unsigned32WithNA,
        cryptoPmadminUpId                         Unsigned32}

