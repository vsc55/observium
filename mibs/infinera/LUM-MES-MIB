    SYNTAX      INTEGER {
                strict (1),
                rr (2),
                wrr (3) }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The scheduler type for the queue.

        strict - strict priority

        rr - round robin

        wrr - weighted round robin

"
    DEFVAL { strict }
    ::= { mesCosProfileEntry 4 }

