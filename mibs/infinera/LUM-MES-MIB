"
    DEFVAL { 0 }
    ::= { mesEvcEntry 38 }

mesEvcClientEgressTagVlanIdAssignment OBJECT-TYPE
    SYNTAX      INTEGER {
                    set (1),
                    copy (2) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Defines how the VLAN id
         of a service frame shall be set
         at the client egress.

         set - Set to specified value.

         copy - Use the VLAN id from the
         original tag.

"
    DEFVAL { copy }
