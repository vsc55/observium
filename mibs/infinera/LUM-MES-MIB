    DEFVAL { enabled }
    ::= { mesMiscEntry 35 }

mesMiscCreateVlan OBJECT-TYPE
    SYNTAX      CommandString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Dialog command for create and configuring a VLAN Map Entry
