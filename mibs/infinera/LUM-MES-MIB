mesPortAutoNegotiationStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                         incomplete (1),
                         halfDuplex (2),
                         fullDuplex (3),
                         halfDuplexRxPauseOn (4),
                         halfDuplexTxPauseOn (5),
                         halfDuplexRxTxPauseOn (6),
                         fullDuplexRxPauseOn (7),
