    ::= { mesPortTable 1 }

MesPortEntry ::=
    SEQUENCE {
        mesPortIndex                      Unsigned32,
        mesPortName                       MgmtNameString,
        mesPortDescr                      DisplayString,
        mesPortSubrack                    SubrackNumber,
        mesPortSlot                       SlotNumber,
