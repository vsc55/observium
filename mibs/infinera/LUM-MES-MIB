    ::= { mesCosProfileTable 1 }

MesCosProfileEntry ::=
    SEQUENCE {
        mesCosProfileIndex             Unsigned32,
        mesCosProfileName              MgmtNameString,
        mesCosProfilePortmask          Unsigned32,
