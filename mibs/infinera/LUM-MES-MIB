"
    DEFVAL { "" }
    ::= { mesEvcEntry 37 }

mesEvcDefaultCeVlanPriority OBJECT-TYPE
    SYNTAX      Unsigned32 (0..7)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The priority assigned at client egress if
