        auxNodeTime                 DateAndTime,
        auxNodeNtpPrimary           IpAddress,
        auxNodeNtpSecondary         IpAddress,
        auxNodeChangeLocalTime      CommandString,
        auxNodeHostUnreachable      FaultStatus}

