roadmLineList OBJECT IDENTIFIER ::= { lumRoadmMIBObjects 6 }
roadmChannelList OBJECT IDENTIFIER ::= { lumRoadmMIBObjects 7 }
-- lumentisRoadmNotifications  OBJECT IDENTIFIER ::= { lumRoadmMIBObjects nn }


