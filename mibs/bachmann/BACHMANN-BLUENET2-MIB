    blueNet2MPGroup         OBJECT-GROUP
        OBJECTS             {
                                blueNet2MPGuid,
                                blueNet2MPName,
                                blueNet2MPFriendlyName,
                                blueNet2MPState,
                                blueNet2MPTimeStamp
                            }
        STATUS              current
        DESCRIPTION         "Conformance group for BlueNet2 Measurement Point information."
        ::=                 { blueNet2Groups    20 }                     
                                
    blueNet2SpdGroup        OBJECT-GROUP
