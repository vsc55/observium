BACHMANN-BLUENET2-MIB DEFINITIONS ::= BEGIN

        IMPORTS
                                MODULE-IDENTITY,
                                NOTIFICATION-TYPE,
                                OBJECT-TYPE,
                                Integer32,
                                Unsigned32,
                                TimeTicks
        FROM                SNMPv2-SMI
                                MODULE-COMPLIANCE,
                                NOTIFICATION-GROUP,
                                OBJECT-GROUP
        FROM                SNMPv2-CONF
                                DateAndTime,
                                DisplayString,
                                RowStatus,
                                TEXTUAL-CONVENTION
        FROM                SNMPv2-TC
                                sysObjectID
        FROM                SNMPv2-MIB
                                bachmann
        FROM                BACHMANN-SMI-MIB;


blueNet2Mib MODULE-IDENTITY
        LAST-UPDATED        "201507010000Z"
        ORGANIZATION        "Bachmann GmbH"
        CONTACT-INFO        "www.bachmann.com

