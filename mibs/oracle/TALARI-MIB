IMPORTS
        MODULE-IDENTITY,
        OBJECT-IDENTITY,
        OBJECT-TYPE,
        NOTIFICATION-TYPE,
        enterprises
                FROM SNMPv2-SMI
        Integer32,
        Counter32,
        Gauge32,
        Counter64,
        IpAddress,
        TimeTicks
                FROM SNMPv2-SMI
        TEXTUAL-CONVENTION,
        DisplayString,
        PhysAddress,
        TruthValue,
        RowStatus,
        TimeStamp,
        AutonomousType,
        TestAndIncr
                FROM SNMPv2-TC;

talari MODULE-IDENTITY
