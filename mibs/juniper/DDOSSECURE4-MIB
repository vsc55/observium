apHighAvailabilityInfo OBJECT IDENTIFIER ::= { apState 4 }

apHaState OBJECT-TYPE
    SYNTAX INTEGER {
	unknown(1),
	probe(2),
	standby(3),
	active(4),
	cripple(5),
	standalone(6),
	activesec(7)
    }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Current high availability state"
    ::= { apHighAvailabilityInfo 1 }

