    SYNTAX  INTEGER {
        tagged(1), 
        untagged(2)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
    "This is VLAN tag state of the port"
    ::= { portInfoTableEntry 6 }

