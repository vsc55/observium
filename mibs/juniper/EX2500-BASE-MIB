    SYNTAX INTEGER {
        no(0),
        yes(1)
        }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The IGMPv3 forwarding for this source/group IP for this host."
    ::= { bosSnoopInfoEntry 9 }

