    SYNTAX INTEGER {
      true(1),
      false(0)
      }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "When set to true aging of fdb table entries is disabled. 
         '1' Fdb aging is disabled
         '0' Fdb aging is enabled"
    ::= { fdb 6 }

