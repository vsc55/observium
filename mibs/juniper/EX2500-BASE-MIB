    SYNTAX  INTEGER {
        true(1),
        false(2)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Value indicating whether lacp is enabled (1) or disabled
         (2) on this port"
    ::= { bosLaLacpInfoPortGeneralTableEntry 2 }

