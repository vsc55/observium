   --------------------------E1端口的对象---------------------------------       
           
    rcftE1PortTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftE1PortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The E1 PORT table.
       "
       ::= { rcftE1PortObjects 1 }
   
    rcftE1PortEntry OBJECT-TYPE
       SYNTAX  RcftE1PortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the E1 PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,rcftE1PortIndex }
       ::= { rcftE1PortTable 1 }
                 
                 
    RcftE1PortEntry ::=
       SEQUENCE {
       rcftE1PortIndex		 	Integer32,
       rcftE1PortAlarmStatus	INTEGER,
       rcftE1PortStatus			INTEGER,
       rcftE1TimeSlots			INTEGER,
       rcftE1TS0Mode			INTEGER,
       rcftE1IdleCode			INTEGER,
       rcftE1LoopStatus			INTEGER,
       rcftE1Order				INTEGER,
       rcftE1PortType			INTEGER,
       rcftE1BertStatus			INTEGER,
	   rcftE1BertTime			Unsigned32, 
	   rcftE1BertErrCode		Unsigned32,
	   rcftE1BertUnusedTime		Unsigned32,
	   rcftE1BertPortSpeed		Unsigned32,
	   rcftE1BertCodeType		INTEGER,
	   rcftE1BertCodeNum		INTEGER,
	   rcftE1AlarmRejest 		INTEGER,
	   rcfT1PortAlarmStatus		INTEGER, 
	   rcftE1PortVCGNumber		INTEGER,
	   rcftE1PortErrorRate		INTEGER,
	   rcftE1PortESCont         INTEGER,
	   rcftE1PortSESCont        INTEGER,
	   rcftE1PortToRNumber      INTEGER,	   
	   rcftE1CVCnt			    INTEGER	                       
       }                               
       
    rcftE1PortIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		E1口的索引
    	"             
        ::= { rcftE1PortEntry 1 }   
        
     rcftE1PortAlarmStatus OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS read-only
     	STATUS	current
    	DESCRIPTION
    	"
    		E1口的告警状态  
    		其他位：保留                                        
    		
    		Bit23	从本地设备上看到的对端设备E1的E-3告警			0：告警； 1：正常
    		Bit22 	从本地设备上看到的对端设备E1的信号劣化TSD告警  	0：告警   1：正常
    		BIT21  	从本地设备上看到的对端设备E1的CRC告警：			0：告警； 1：正常。
    		BIT20  	从本地设备上看到的对端设备E1的LOMF告警：		0：告警； 1：正常。
    		BIT19  	从本地设备上看到的对端设备E1的LOF告警：			0：告警； 1：正常。
    		BIT18  	从本地设备上看到的对端设备E1的CV告警：			0：告警； 1：正常。
    		BIT17  	从本地设备上看到的对端设备E1的AIS告警：			0：告警； 1：正常。    		    		    		    		    		
    		BIT16  	从本地设备上看到的对端设备E1的LOS告警：			0：告警； 1：正常。 
    		            
            bit7：	RDI，0: 远端缺陷告警。     1: 正常
    		Bit6 	TSD  0：本地E1信号劣化告警 1：本地E1信号正常
    		BIT5	CRC  0: 本地E1 CRC告警；   1：本地E1 CRC正常；
			BIT4	LOMF 0：本地E1 复帧失步；  1：本地E1 复帧正常。
			BIT3	LOF	 0：本地E1 帧失步；	   1：本地E1 帧正常。
			BIT2	CV	 0：本地HDB3 编码违例；1：本地HDB3 编码正常。			
			BIT1	AIS	 0：本地AIS 告警；     1：无本地AIS 告警。
			BIT0	LOS	 0：本地信号消失告警； 1：本地信号正常。
    	"             
        ::= { rcftE1PortEntry 2 }   
     	   
     rcftE1PortStatus OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS read-write
     	STATUS	current
    	DESCRIPTION
    	"
    		E1口的状态    
    		其他位：保留
    		BIT21	网管通道模式	 0：不启用	   1：启用独立时隙                           
    		BIT20	E1环回检测结果	 0：无环	   1：有环
    		BIT19	E1环回检测		 0: 禁止	   1：使能
    		BIT18   E1协议版本       0：V2         1：V3     （网管只用来做逻辑，不显示）
			BIT17	请求时隙跟随	 0：禁止	1：使能          
			BIT16	对端跟随         0：无响应，1：响应	 
    		BIT15-14 EX_LBK-Test  00：没有进行过外环测试； 01：保留；10：外环测试成功；11：外环测试失败。
    		BIT13-12 IN_LBK-Test  00：没有进行过内环测试； 01：保留；10：内环测试成功；11：内环测试失败。
    		BIT11    IDLE_MODE 0：固定使用IDLE_CODE；   1：返回。
    		BIT10-9 PORTTYPE  00：BNC；                01：RJ45插座；    其他：保留         
    		BIT8-7  BALANCE	  00：75欧姆非平衡；       01：120欧姆平衡； 其他：保留    
			BIT6    SABIT	  0：本地E1 未使用SA BIT； 1：本地E1 使用SA BIT；
			BIT5    PCM31     0：本地E1 PCM30模式；	   1：本地E1 PCM31模式；
			BIT4    FRAME     0：本地E1透明模式；      1：本地E1成帧模式；		
			BIT3    CRCAUTO   0: 本地E1 CRC自协商使能  1：禁止
			BIT2    CRC       0：本地CRC未使能；       1：本地CRC使能。 
			BIT1    HDB3      0: 本地E1码型NRZ；       1: 本地E1码型HDB3；
			BIT0    OPEN      0：本地E1端口关闭；      1：本地E1端口打开； 
    	"             
        ::= { rcftE1PortEntry 3 }     
        
      rcftE1TimeSlots  OBJECT-TYPE
      	SYNTAX	INTEGER
      	MAX-ACCESS  read-write
      	STATUS	current
      	DESCRIPTION
      	"
      		E1口在成帧模式下所占用的时隙,bit0-bit30每位分别表示时隙1到时隙31的占用情况。
      		时隙0的占用与否是通过透明成帧模式判断的。
      	"
        ::= { rcftE1PortEntry 4 }    
        
      rcftE1TS0Mode  OBJECT-TYPE
      	SYNTAX  INTEGER
      	MAX-ACCESS read-write  
      	STATUS	current
      	DESCRIPTION
		"
			工作模式（TS0）：
			1000：工作于透明模式，透传TS0-32，TS0=1；
			0001：成帧模式0，发送端不发送TS0、接收端重新计算整个TS0； 
			1001：成帧模式1，发送端透传TS0、接收端也使用透传来的TS0（IDLE CODE一致时才可以使用此模式）；
			0010：成帧模式2，发送端不发送TS0、接收端返回TS0（CRC除外，重新计算）； 
			1010：成帧模式3，发送端透传TS0、但接收端丢弃CRC使用重新计算的CRC；其它：保留。		
		"    
        ::= { rcftE1PortEntry 5 }    
        
      rcftE1IdleCode OBJECT-TYPE
      	SYNTAX  INTEGER
      	MAX-ACCESS read-write  
      	STATUS	current
      	DESCRIPTION
		"
           未用时隙的码字
		"    
        ::= { rcftE1PortEntry 6 }   
        
       rcftE1LoopStatus  OBJECT-TYPE
       	SYNTAX	INTEGER
       	{
       		  localDoubleLoopEnable(1),
       		  localDoubleLoopDisable(2),
       		  remoteDoubleLoopEnable(3),
       		  remoteDoubleLoopDisable(4)
       	}
       	MAX-ACCESS read-only
       	STATUS	current
       	DESCRIPTION
       	"
       	     E1的环回状态：1:本地环回，2：无本地环回，3：远端环回，4：无远端环回，
						   5:无环回，
						   6：本地内环使能，7：本地外环使能，8：本地双向环使能， 
						   9:远端内环使能，10：远端外环使能，11：远端双向环使能，
						   12：本地内环禁止，13:本地外环禁止，14：本地双向环禁止，
						   15：远端内环禁止，16：远端外环禁止，17：远端双向环禁止，       	     			   
       	"     
        ::= { rcftE1PortEntry 7 }   

		rcftE1Order	OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS	read-write
			STATUS	current
			DESCRIPTION
			"
			    1：硬复位
				2：软复位
				3：开始查询统计信息
				4：结束查询统计信息
				5：内环使能
				6：内环禁止
				7：外环使能
				8：外环禁止
				9: 无环回
				10: 本地内环使能
				11: 本地外环使能
				12: 本地双向环使能
				13: 远端内环使能
				14: 远端外环使能
				15: 远端双向环使能
				16: 本地内环禁止
				17: 本地外环禁止
				18: 本地双向环禁止
				19: 远端内环禁止
				20: 远端外环禁止
				21: 远端双向环禁止
 				22: 端口BERT测试开始
 				23: 端口BERT测试结束
				24：统计信息清零
				25：告警屏蔽
				26：告警屏蔽解除
				27：端口未用支路告警屏蔽
				28：端口未用支路告警屏蔽解除
				29：环回检测使能
				30：环回检测停止
				31：开始查询SFP扩展信息
				32；停止查询SFP扩展信息
				33：开始查询SFP数字诊断信息
				34：停止查询SFP数字诊断信息
				注：31-34尚没有对网管开放
					
				
			注意：对于约定5，枚举值5-9废弃，
			rcftE1Order的枚举值要和其他接口级命令节点始终保持完全一致。						
			"
		::= { rcftE1PortEntry 8 }	
             
		rcftE1PortType	OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS	read-only
			STATUS	current
			DESCRIPTION
			"    
			    表示E1的类型。这个节点主要是针对约定四而增加，用来区分多种E1子块
			    下各个E1的具体类型。
			    
			    1：既可以为成帧，又可为非成帧（对应约定四中的0x10-0x04子块）
				2：非成帧（对应约定四中的0x10-0x06子块）		
			"
		::= { rcftE1PortEntry 9 }	       
		
	    rcftE1BertStatus		OBJECT-TYPE
	    	SYNTAX  INTEGER
	    	MAX-ACCESS	read-write
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	  BIT4			BERT方向：		0：线路侧； 1：客户侧
	    	  BIT3			BERT_STATUS		0:正常工作；	1:正在bert
	    	  BIT2-1		BERT_RESULT     00:未测试      01:测试正常；	10:测试失败
	    	  BIT0			BERT_TYPE   	0:误码率方式；  1:误码时间方式
	    	"
	    	::= {rcftE1PortEntry 10}
	    	
	    rcftE1BertTime		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	误码率方式:测试时间，时间随测试的进行而增长,单位 S.  		
	    	"				                           
	    	::= {rcftE1PortEntry  11}
	    
	    rcftE1BertErrCode		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	 误码率方式:误码数，单位是bit
	    	"	                         
	    	::= {rcftE1PortEntry 12}
	    	
	    rcftE1BertUnusedTime		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	 误码率方式:测试不可用时间，超过255秒后维持255秒不变，单位是S.
	    	"
	    	::= {rcftE1PortEntry 13} 
	    	
	    rcftE1BertPortSpeed		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	 误码率方式:测试的端口速率，单位：kbps。
	    	"
	    	::= {rcftE1PortEntry 14}    
	    	
	    rcftE1BertCodeType		OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-write
	   		STATUS	current
	   		DESCRIPTION
	   		"
	   		误码率方式:测试码型，最大值为127   
	   		0000001'b：2的15次方-1；
			其它：保留。

	   		"         
	   		::= {rcftE1PortEntry 15}       		
	   		
	    rcftE1BertCodeNum		OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-write
	   		STATUS	current
	   		DESCRIPTION
	   		"
	   		误码率方式:测试码数    
	   		0010001'b：2的31次方个码；
			0010010'b：2的28次方个码；
			0010011'b：2的25次方个码；
			0010100'b：2的21次方个码；
			0100001'b：测试不停；
			其它：保留。

	   		"         
	   		::= {rcftE1PortEntry 16}  
	   		
	   rcftE1AlarmRejest 		OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-only
	   		STATUS	current
	   		DESCRIPTION   
	   		"     
	   			本地E1 告警屏蔽
	   			 1：屏蔽
	   			 2：不屏蔽
	   		"         
	   		::= {rcftE1PortEntry 17}  
	   			   		
	   rcfT1PortAlarmStatus		OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-only
	   		STATUS	current
	   		DESCRIPTION   
	   		"        
	   			本地T1的告警
	   			bit1：AIS		0：告警，1：正常     
	   			bit0：LOS		0：告警，1：正常

	   		"         
	   		::= {rcftE1PortEntry 18} 
	   		
	   	rcftE1PortVCGNumber   OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-only
	   		STATUS	current
	   		DESCRIPTION   
	   		"        
	   			接口属于哪个VCG成员。
					0：不是VCG成员。
					1~N：表示属与哪个VCG成员。  
	   		"         
	   		::= {rcftE1PortEntry 19}   
	   		 
	   	rcftE1PortErrorRate  OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-only
	   		STATUS	current
	   		DESCRIPTION   
	   		"        
	   			E1的误码率 
	   			
	   			1：无误码
	   			2：大于e-6
	   			3：大于e-3
	   		"         
	   		::= {rcftE1PortEntry 20}  
	   		
	   	
	   	rcftE1PortESCont   OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-only
	   		STATUS	current
	   		DESCRIPTION   
	   		"        
	   			E1的误码秒 
	   			
	   		"         
	   		::= {rcftE1PortEntry 21}  
	   	
	   	rcftE1PortSESCont   OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-only
	   		STATUS	current
	   		DESCRIPTION   
	   		"        
	   			E1的严重误码秒 
	   		"         
	   		::= {rcftE1PortEntry 22}  
        rcftE1PortToRNumber   OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-only
	   		STATUS	current
	   		DESCRIPTION   
	   		"        
	   			对端E1编号。 
	   		"         
	   		::= {rcftE1PortEntry 23}             

        rcftE1CVCnt   OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-only
	   		STATUS	current
	   		DESCRIPTION   
	   		"        
	   			E1编码违例统计。	0~65535，累加值，非读清 
	   		"         
	   		::= {rcftE1PortEntry 24}           

	   			      
