   --------------------------DS3E3端口的对象---------------------------------  
      
   rcftDS3E3PortTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftDS3E3PortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The DS3E3 PORT table.
       "
       ::= { rcftDS3E3PortObjects 1 }
   
    rcftDS3E3PortEntry OBJECT-TYPE
       SYNTAX  RcftDS3E3PortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the DS3E3 PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,rcftDS3E3PortIndex }
       ::= { rcftDS3E3PortTable 1 }
                 
                 
    RcftDS3E3PortEntry ::=
       SEQUENCE {
       rcftDS3E3PortIndex		 	Integer32,
       rcftDS3E3PortAlarmStatus		INTEGER,
       rcftDS3E3PortStatus			INTEGER,
       rcftDS3E3PortESCont			INTEGER,
       rcftDS3E3PortLoopStatus		INTEGER, 
       rcftDS3E3PortOrder			INTEGER
       }                               
       
    rcftDS3E3PortIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		DS3E3口的索引
    	"             
        ::= { rcftDS3E3PortEntry 1 }   
 
    rcftDS3E3PortAlarmStatus	OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"  
    		BIT24	OOF    从本地设备上看对端设备电口帧失步告警		 	1：正常    0：告警
            BIT23	RAI	   从本地设备上看对端设备RAI告警    			1:	正常	0：告警     
    		BIT22   LOF    从本地设备上看对端设备电口帧丢失告警         1: 正常    0: 告警
    		BIT21	CRC	   从本地设备上看对端设备电口CRC校验错告警      1：正常	   0：告警       
			BIT20   RLCV   从本地设备上看对端设备电口编码冲突告警       1：正常    0：告警
			BIT19   RDMO   从本地设备上看对端设备电口发送信号无输出     1：正常    0：告警
			BIT18   RLOL   从本地设备上看对端设备电口接收信号失锁告警   1：正常    0：告警
			BIT17   RLOS   从本地设备上看对端设备电口接收信号丢失告警   1：正常    0：告警
			BIT16   RAIS   从本地设备上看对端设备电口接收信号AIS 告警   1：正常    0：告警
    	                                                      
            BIT8	OOF   帧失步告警		 1：正常    0：告警
            BIT7	RAI	  RAI告警			 1:	正常	0：告警                               
    	    BIT6    LOF	  帧丢失告警         1: 正常    0: 告警   
    	    BIT5	CRC	  CRC校验错告警      1：正常	0：告警
			BIT4    LCV   编码冲突告警       1：正常    0：告警
			BIT3    DMO   发送信号无输出     1：正常    0：告警
			BIT2    LOL   接收信号失锁告警   1：正常    0：告警
			BIT1    LOS   接收信号丢失告警   1：正常    0：告警
			BIT0    AIS   接收信号AIS 告警   1：正常    0：告警
    	"
        ::= { rcftDS3E3PortEntry 2 }   
    	                 
    	                 
	rcftDS3E3PortStatus		OBJECT-TYPE
		SYNTAX	INTEGER
		MAX-ACCESS read-write
		STATUS	current
		DESCRIPTION                                         
		"           
			 BIT10:  线路编码	
			 		0:AMI
			 		1:B3ZS		--------------DS3
			 		1:HDB3      --------------E3                                  
			 BIT9-8：帧结构:	
			 		00:G.751	01:G.832   ------------E3
			 		00:M23		01:C-bit   ------------DS3
			 		  
			 BIT7: 是否成帧				1: 成帧,	 0:非成帧
			 BIT6: DS3/E3传输距离		1：长距，	 0：短距  
			 BIT5：保留
			 BIT4：DS3/E3误码仪状态，   1：打开，0：关闭
			 BIT3：FaultPass故障转移    1：使能，0：禁止

			 BIT2-1 端口的环回状态： 0：无环回，1：内环，2：外环，3：双向环
		     BIT0   端口模式选择     0：E3    1：DS3  
		"
		::= { rcftDS3E3PortEntry 3 }  
	
	rcftDS3E3PortESCont	  OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS	read-only
		STATUS  current
		DESCRIPTION
		"
			误码秒个数
		"             
        ::= { rcftDS3E3PortEntry 4 }  
        
        

	
	rcftDS3E3PortLoopStatus		OBJECT-TYPE
		SYNTAX	INTEGER
		MAX-ACCESS read-only
		STATUS	current
		DESCRIPTION
		"
		    1:本地环回，
			2：无本地环回，
			3：远端环回，
			4：无远端环回，
			5: 无环回，
			6：本地内环使能，
			7：本地外环使能，
			8：本地双向环使能， 
			9：远端内环使能，
			10：远端外环使能，
			11：远端双向环使能，
			12：本地内环禁止，
			13: 本地外环禁止，
			14：本地双向环禁止，
			15：远端内环禁止，
			16：远端外环禁止，
			17：远端双向环禁止
		"
		::= { rcftDS3E3PortEntry 5 }
		
	rcftDS3E3PortOrder	  OBJECT-TYPE
		SYNTAX	INTEGER
		MAX-ACCESS read-write
		STATUS  current
		DESCRIPTION
		"
			    1：硬复位
				2：软复位
				3：开始查询统计信息
				4：结束查询统计信息
				5：内环使能
				6：内环禁止
				7：外环使能
				8：外环禁止
				9: 无环回
				10: 本地内环使能
				11: 本地外环使能
				12: 本地双向环使能
				13: 远端内环使能
				14: 远端外环使能
				15: 远端双向环使能
				16: 本地内环禁止
				17: 本地外环禁止
				18: 本地双向环禁止
				19: 远端内环禁止
				20: 远端外环禁止
				21: 远端双向环禁止
 				22: 端口BERT测试开始
 				23: 端口BERT测试结束
				24：统计信息清零
				25：告警屏蔽
				26：告警屏蔽解除
				27：端口未用支路告警屏蔽
				28：端口未用支路告警屏蔽解除
				29：环回检测使能
				30：环回检测停止
				31：开始查询SFP扩展信息
				32；停止查询SFP扩展信息
				33：开始查询SFP数字诊断信息
				34：停止查询SFP数字诊断信息
				注：31-34尚没有对网管开放
					
				
			注意：对于约定5，枚举值5-9废弃，
		    rcftDS3E3PortOrder的枚举值要和其他接口级命令节点始终保持完全一致。  		
		"   
	    ::= { rcftDS3E3PortEntry 6 }  
	   
	   --------------------------DS3E3电口的流量统计-------------------------- 
    
         rcftDS3E3StatisticTable OBJECT-TYPE
	        SYNTAX SEQUENCE OF RcftDS3E3StatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	          局端设备DS3E3端口的统计表.
	          "
            ::= {rcftDS3E3PortPerformance 1}

	    rcftDS3E3StatisticEntry OBJECT-TYPE
	        SYNTAX RcftDS3E3StatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	             以rcftDS3E3Index为索引
	          "
	        AUGMENTS { rcftDS3E3PortEntry }
	        ::= {rcftDS3E3StatisticTable 1}

	    RcftDS3E3StatisticEntry ::= SEQUENCE {
				rcftDS3E3TxPackets         Counter32, 
				rcftDS3E3TxBytes           Counter32, 
				rcftDS3E3TxFailurePackets  Counter32,					
				rcftDS3E3RxPackets         Counter32,
				rcftDS3E3RxBytes           Counter32,
				rcftDS3E3RxErrorPackets    Counter32,
				rcftDS3E3FluxTimer		   Counter32		
	         }
					

    	  rcftDS3E3TxPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS3E3口发送以太网包数
						"
					::= { rcftDS3E3StatisticEntry 1 }
					
		 rcftDS3E3TxBytes OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS3E3口发送以太网字节数
						"
					::= { rcftDS3E3StatisticEntry 2 }   
					

	    rcftDS3E3TxFailurePackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS3E3电口发送以太网丢包数
						"
					::= { rcftDS3E3StatisticEntry 3 } 
										
		 rcftDS3E3RxPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS3E3口接收以太网包数
						"
					::= { rcftDS3E3StatisticEntry 4 }
					
		 rcftDS3E3RxBytes OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS3E3口接收以太网字节数
						"
					::= { rcftDS3E3StatisticEntry 5 }
					
	    rcftDS3E3RxErrorPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS3E3电口接收以太网丢包数
						"
					::= { rcftDS3E3StatisticEntry 6 }

        rcftDS3E3FluxTimer	OBJECT-TYPE
        			SYNTAX		Counter32
        			MAX-ACCESS 	read-only
        			STATUS		current
        			DESCRIPTION
        				"
        					流量统计的时间
        				"			
                    ::= { rcftDS3E3StatisticEntry 7 } 
	    
	 	
	                               
    --------------------------DS3E3端口的告警------------------------ 
                                       
