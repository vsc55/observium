   --------------------------PDH端口的对象---------------------------------       
           
    rcftPdhPortTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftPdhPortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The Pdh PORT table.
       "
       ::= { rcftPdhPortObjects 1 }
   
    rcftPdhPortEntry OBJECT-TYPE
       SYNTAX  RcftPdhPortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the Pdh PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,rcftPdhPortIndex }
       ::= { rcftPdhPortTable 1 }
                 
                 
    RcftPdhPortEntry ::=
       SEQUENCE {
       rcftPdhPortIndex		 	Integer32,
       rcftPdhPortAlarmStatus	INTEGER,
       rcftPdhPortStatus		INTEGER,
       rcftPdhPortECSCnt		INTEGER,
       rcftPdhPortSECSCnt       INTEGER,
       rcftPdhPortModuleType	INTEGER,
       rcftPdhPortLoopStatus    INTEGER,
       rcftPdhPortOrder		INTEGER,
       rcftPdhPortBertStatus	INTEGER,
       rcftPdhPortBertErrCode	Unsigned32       
       }                               
       
    rcftPdhPortIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		Pdh口的索引
    	"             
        ::= { rcftPdhPortEntry 1 }   
        
     rcftPdhPortAlarmStatus OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS read-only
     	STATUS	current
    	DESCRIPTION
    	"
    		Pdh口的告警状态  
    		其他位：保留
			BIT20	从本地设备上查看到的对端设备掉电告警	 0：告警； 1：正常    		   
			BIT19	从本地设备上查看到的对端设备光口的E-6	 0：告警； 1：正常
			BIT18	从本地设备上查看到的对端设备光口的E-3	 0：告警； 1：正常
			BIT17	从本地设备上查看到的对端设备光口的LOF	 0：告警； 1：正常
			BIT16	从本地设备上查看到的对端设备光口的LOS	 0：告警； 1：正常  
			
			BIT3	E-6 pdh光口的E-6告警 0：告警； 1：正常
			BIT2	E-3	pdh光口的E-3告警 0：告警； 1：正常
			BIT1    LOF pdh光口LOF告警   0：告警   1：正常    
			BIT0    LOS pdh光口LOS告警   0：告警   1：正常    		
		"             
        ::= { rcftPdhPortEntry 2 }   
     	   
     rcftPdhPortStatus OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS read-write
     	STATUS	current
    	DESCRIPTION
    	"
    		Pdh口的状态    
    		其他位：保留     
    		BIT10  光口故障转移           0：禁止   1：使能       
			BIT9   光口所接的对端设备的掉电告警	 0：告警； 1：正常		       		
			BIT8   光口ALS开关设置    	0:禁止       1: 使能    		     
            BIT7-6 光口的环回状态 0：无环回，1：内环，2：外环 3：双向环     
			BIT5   光口ALS    	0:禁止       1: 使能  
			BIT4   从本地设备上查看到的对端设备光口的工作状态 0:不工作     1：工作  
			BIT3   光口的工作状态 0:不工作     1：工作  
			
			BIT2-1 光口因故障转移功能使能时的开关状态：
			       0：开
			       1：因电-光Faultpass功能使能时关闭
			       2：因光口ALS功能使能关闭
		    BIT0   光口的打开关闭：1:open, 0:close  
			
    	"             
        ::= { rcftPdhPortEntry 3 }  
        
     rcftPdhPortECSCnt	OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-only
     	STATUS	current
     	DESCRIPTION
     	"
     		pdh光口误码秒
     	"   
     	::= { rcftPdhPortEntry 4 }  
     	
     rcftPdhPortSECSCnt	OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-only
     	STATUS	current
     	DESCRIPTION
     	"
     		pdh光口严重误码秒
     	"   
     	::= { rcftPdhPortEntry 5 }     
     	
     	
     rcftPdhPortModuleType  OBJECT-TYPE
      	  SYNTAX INTEGER
      	  {   
      	     optical-M(1),
      	     optical-S1(2),
      	     optical-S2(3),
      	     optical-S3(4),
      	     optical-SS13(5), 
      	     optical-SS15(6),
      	     optical-SS23(7),
      	     optical-SS25(8),
      	     --optical-SS3(9),    kuanglili 20090519 根据公司规范取消ss3光头类型
      	     optical-SS34(9),
      	     optical-SS35(10),
      	     optical-S15(12),   --kuanglili 20090909 S15光模块类型枚举值为12
      	     optical-SFP(15),
      	     optical-SS24(23),       	     
 	         optical-S1FC(50), 
 	         optical-S1A(51), 
 	         optical-S2A(52), 
 	         optical-S3A(53),      	     
      	     unknown-type(100)
      	  }   
      	  MAX-ACCESS read-only
      	  STATUS current
      	  DESCRIPTION
      	  "
      	    远端设备pdh光口的光模块类型
      	  "
          ::= { rcftPdhPortEntry 6 } 
          
    rcftPdhPortLoopStatus	OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-only
     	STATUS	current
     	DESCRIPTION
     	"
     		pdh光口环回状态  
     		1:本地环回，2：无本地环回，3：远端环回，4：无远端环回，
 			5:无环回，
 			6：本地内环使能，7：本地外环使能，8：本地双向环使能， 
 			9:远端内环使能，10：远端外环使能，11：远端双向环使能，
 			12：本地内环禁止，13:本地外环禁止，14：本地双向环禁止，
 			15：远端内环禁止，16：远端外环禁止，17：远端双向环禁止，     		 
     	"   
     	::= { rcftPdhPortEntry 7 }  
    
    rcftPdhPortOrder	OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS	read-write
    	STATUS	current
    	DESCRIPTION
    	"
			    1：硬复位
				2：软复位
				3：开始查询统计信息
				4：结束查询统计信息
				5：内环使能
				6：内环禁止
				7：外环使能
				8：外环禁止
				9: 无环回
				10: 本地内环使能
				11: 本地外环使能
				12: 本地双向环使能
				13: 远端内环使能
				14: 远端外环使能
				15: 远端双向环使能
				16: 本地内环禁止
				17: 本地外环禁止
				18: 本地双向环禁止
				19: 远端内环禁止
				20: 远端外环禁止
				21: 远端双向环禁止
 				22: 端口BERT测试开始
 				23: 端口BERT测试结束
				24：统计信息清零
				25：告警屏蔽
				26：告警屏蔽解除
				27：端口未用支路告警屏蔽
				28：端口未用支路告警屏蔽解除
				29：环回检测使能
				30：环回检测停止
				31：开始查询SFP扩展信息
				32；停止查询SFP扩展信息
				33：开始查询SFP数字诊断信息
				34：停止查询SFP数字诊断信息
				注：31-34尚没有对网管开放
					
				
			注意：对于约定5，枚举值5-9废弃，
		    rcftPdhPortOrder的枚举值要和其他接口级命令节点始终保持完全一致。    	
		    "  
    	::= { rcftPdhPortEntry 8 }  

  	rcftPdhPortBertStatus    OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS	read-only
    	STATUS	current
    	DESCRIPTION  
    	"
    	  bit0：1：误码仪使能		0：误码仪禁止
    	"
        ::= { rcftPdhPortEntry 9 }  	 		         	               

	rcftPdhPortBertErrCode		OBJECT-TYPE
	    SYNTAX	Unsigned32
	    MAX-ACCESS	read-only
	    STATUS	current
	    DESCRIPTION
	    "
	      误码率方式:误码数，单位是bit
	    "	                         
	    ::= {rcftPdhPortEntry 10}

    
