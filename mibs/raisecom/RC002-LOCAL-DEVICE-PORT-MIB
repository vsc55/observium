     --------------------------DS1端口的对象---------------------------------  
      
   rcftDS1PortTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftDS1PortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The DS1 PORT table.
       "
       ::= { rcftDS1PortObjects 1 }
   
    rcftDS1PortEntry OBJECT-TYPE
       SYNTAX  RcftDS1PortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the DS1 PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,rcftDS1PortIndex }
       ::= { rcftDS1PortTable 1 }
                 
                 
    RcftDS1PortEntry ::=
       SEQUENCE {
       rcftDS1PortIndex		 	Integer32,
       rcftDS1PortAlarmStatus		INTEGER,
       rcftDS1PortStatus			INTEGER, 
       rcftDS1PortBertStatus		INTEGER,
       rcftDS1PortESCont			INTEGER,  
       rcftDS1PortSESCont			INTEGER,
       rcftDS1PortLoopStatus		INTEGER, 
       rcftDS1PortOrder			INTEGER,
       rcftDS1PortTranLength		INTEGER,
       rcftDS1PortFaultPassIndicator	INTEGER,
       rcftDS1PortframeType		INTEGER,  
       rcftDS1PortChannel	INTEGER       
       }                               
       
    rcftDS1PortIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		DS1口的索引
    	"             
        ::= { rcftDS1PortEntry 1 }   
 
    rcftDS1PortAlarmStatus	OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"       
    		BIT22	LOF	   从本地设备上看对端DS1电口的LOF	            1：正常	   0：告警 
    		BIT21	CRC	   从本地设备上看对端设备电口CRC校验错告警      1：正常	   0：告警       
			BIT20   RLCV   从本地设备上看对端设备电口编码冲突告警       1：正常    0：告警
			BIT19   RDMO   从本地设备上看对端设备电口发送信号无输出     1：正常    0：告警
			BIT18   RLOL   从本地设备上看对端设备电口接收信号失锁告警   1：正常    0：告警
			BIT17   RLOS   从本地设备上看对端设备电口接收信号丢失告警   1：正常    0：告警
			BIT16   RAIS   从本地设备上看对端设备电口接收信号AIS 告警   1：正常    0：告警
    	                                                       
    	    BIT8	RED						  1:正常    0：告警
    	 	BIT7    YEL   					  1:正常    0：告警
    	    BIT6	LOF   接收信号帧失步告警  1：正常	0：告警
    	    BIT5	CRC	  CRC校验错告警      1：正常	0：告警
			BIT4    LCV   编码冲突告警       1：正常    0：告警
			BIT3    DMO   发送信号无输出     1：正常    0：告警
			BIT2    LOL   接收信号失锁告警   1：正常    0：告警
			BIT1    LOS   接收信号丢失告警   1：正常    0：告警
			BIT0    AIS   接收信号AIS 告警   1：正常    0：告警
    	"
        ::= { rcftDS1PortEntry 2 }   
    	                 
    	                 
	rcftDS1PortStatus		OBJECT-TYPE
		SYNTAX	INTEGER
		MAX-ACCESS read-write
		STATUS	current
		DESCRIPTION
		"     
			BIT0：线路编码类型   1:B8ZS       0:AMI   
			BIT1: 故障转移		 1：使能      0：禁止  
			BIT2: DS1是否成帧	 1：成帧	  0：非成帧    
			BIT3: ds1是否打开,	 1:打开		  0:关闭
		"
		::= { rcftDS1PortEntry 3 }   
		
	rcftDS1PortBertStatus	OBJECT-TYPE
		SYNTAX INTEGER
		MAX-ACCESS	read-only
		STATUS	current
		DESCRIPTION
		"
			BIT0   DS1误码仪状态，   1：打开，0：关闭

		"      
		::= { rcftDS1PortEntry 4 }   

	
	rcftDS1PortESCont	  OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS	read-only
		STATUS  current
		DESCRIPTION
		"
			误码秒个数
		"             
        ::= { rcftDS1PortEntry 5 }      
        
    rcftDS1PortSESCont     OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS	read-only
		STATUS  current
		DESCRIPTION
		"
			严重误码秒个数
		"             
        ::= { rcftDS1PortEntry 6 }     
        
	rcftDS1PortLoopStatus		OBJECT-TYPE
		SYNTAX	INTEGER
		MAX-ACCESS read-only
		STATUS	current
		DESCRIPTION
		"
		    1:本地环回，
			2：无本地环回，
			3：远端环回，
			4：无远端环回，
			5: 无环回，
			6：本地内环使能，
			7：本地外环使能，
			8：本地双向环使能， 
			9：远端内环使能，
			10：远端外环使能，
			11：远端双向环使能，
			12：本地内环禁止，
			13: 本地外环禁止，
			14：本地双向环禁止，
			15：远端内环禁止，
			16：远端外环禁止，
			17：远端双向环禁止
		"
		::= { rcftDS1PortEntry 7 }
		
	rcftDS1PortOrder	  OBJECT-TYPE
		SYNTAX	INTEGER
		MAX-ACCESS read-write
		STATUS  current
		DESCRIPTION
		"
		 	1：硬复位
			2：软复位
			3：开始查询统计信息
			4：结束查询统计信息
			5：内环使能
			6：内环禁止
			7：外环使能
			8：外环禁止
			9: 无环回
			10: 本地内环使能
			11: 本地外环使能
			12: 本地双向环使能
			13: 远端内环使能
			14: 远端外环使能
			15: 远端双向环使能
			16: 本地内环禁止
			17: 本地外环禁止
			18: 本地双向环禁止
			19: 远端内环禁止
			20: 远端外环禁止
			21: 远端双向环禁止
			22: 端口BERT测试开始
			23: 端口BERT测试结束
		"   
	    ::= { rcftDS1PortEntry 8 }  
	   
	rcftDS1PortTranLength	OBJECT-TYPE
	    SYNTAX	INTEGER
		MAX-ACCESS read-write
		STATUS  current
		DESCRIPTION 
		"
		      DS1  传输距离    
		      
		    2：短距，DS1传输距离为0~133ft
			3：短距，DS1传输距离为133~266ft      	
			4：短距，DS1传输距离为266~399ft
			5：短距，DS1传输距离为399~533ft
			6：短距，DS1传输距离为533~655ft
			8：长距，0dB
			9：长距，-7.5dB 
			10：长距，-15.0dB 
			11：长距，-22.5dB
		      
		"  
		::= { rcftDS1PortEntry 9 }  
		
	rcftDS1PortFaultPassIndicator OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS read-only
		STATUS  current
		DESCRIPTION
		"
			DS1故障转移指示灯
			１：灭，故障转移禁止
			２：绿常亮，故障转移使能，但没有故障发生
			３：黄闪亮，故障转移使能，且发生故障导致以太口关闭
		"  
		::= { rcftDS1PortEntry 10 }  
		
	rcftDS1PortframeType	OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS read-write
		STATUS  current
		DESCRIPTION   
		"    
		DS1帧格式
		1：SF帧格式
		2：ESF
		3：DM
		4：SLC-96
		"
		::= { rcftDS1PortEntry 11 }   
		
		
    rcftDS1PortChannel	OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS read-write
		STATUS  current
		DESCRIPTION     
		"        
		DS1成帧时所用的时隙，bit0-bit323每位分别表示时隙１到24的占用情况。
		"
        ::= { rcftDS1PortEntry 12 } 
		
		  --------------------------DS1电口的流量统计-------------------------- 
    
         rcftDS1StatisticTable OBJECT-TYPE
	        SYNTAX SEQUENCE OF RcftDS1StatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	          局端设备DS1端口的统计表.
	          "
            ::= {rcftDS1PortPerformance 1}

	    rcftDS1StatisticEntry OBJECT-TYPE
	        SYNTAX RcftDS1StatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	             以rcftDS1Index为索引
	          "
	        AUGMENTS { rcftDS1PortEntry }
	        ::= {rcftDS1StatisticTable 1}

	    RcftDS1StatisticEntry ::= SEQUENCE {
				rcftDS1PortTxPackets         Counter32, 
				rcftDS1PortTxBytes           Counter32, 
				rcftDS1PortTxFailurePackets  Counter32,					
				rcftDS1PortRxPackets         Counter32,
				rcftDS1PortRxBytes           Counter32,
				rcftDS1PortRxErrorPackets    Counter32,
				rcftDS1PortFluxTimer		   Counter32		
	         }
					

    	  rcftDS1PortTxPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS1口发送以太网包数
						"
					::= { rcftDS1StatisticEntry 1 }
					
		 rcftDS1PortTxBytes OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS1口发送以太网字节数
						"
					::= { rcftDS1StatisticEntry 2 }   
					

	    rcftDS1PortTxFailurePackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS1电口发送以太网丢包数
						"
					::= { rcftDS1StatisticEntry 3 } 
										
		 rcftDS1PortRxPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS1口接收以太网包数
						"
					::= { rcftDS1StatisticEntry 4 }
					
		 rcftDS1PortRxBytes OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS1口接收以太网字节数
						"
					::= { rcftDS1StatisticEntry 5 }
					
	    rcftDS1PortRxErrorPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS1电口接收以太网错误包数						"
					::= { rcftDS1StatisticEntry 6 }

        rcftDS1PortFluxTimer	OBJECT-TYPE
        			SYNTAX		Counter32
        			MAX-ACCESS 	read-only
        			STATUS		current
        			DESCRIPTION
        				"
        					流量统计的时间
        				"			
                    ::= { rcftDS1StatisticEntry 7 } 


	    
	
