   --------------------------G.端口的对象---------------------------------    
 
    rcftSHDSLPortTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftSHDSLPortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The SDHSL PORT table.
       "
       ::= { rcftSHDSLPortObjects 1 }
   
    rcftSHDSLPortEntry OBJECT-TYPE
       SYNTAX  RcftSHDSLPortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the SHDSL PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,rcftSHDSLPortIndex }
       ::= { rcftSHDSLPortTable 1 }
                 
                 
    RcftSHDSLPortEntry ::=
       SEQUENCE {
       rcftSHDSLPortIndex		 	Integer32,
       rcftSHDSLPortAlarmStatus		INTEGER,
       rcftSHDSLPortStatus			INTEGER, 
       rcftSHDSLPortCapableSpeed	INTEGER,
       rcftSHDSLPortWorkSpeed		INTEGER,
       rcftSHDSLPortProbeMaxSpeed	INTEGER,       
       rcftSHDSLPortProbeMinSpeed	INTEGER,   
       rcftSDHSLPortSNR				INTEGER,
       rcftSHDSLPortConfigSNR		INTEGER,
       rcftSHDSLPortSNRThreshold   	INTEGER,
       rcftSHDSLPortAttenuation 	INTEGER, 
       rcftSHDSLPortAttenuationThreshold		INTEGER, 
       rcftSHDSLPortPBO   			INTEGER, 
       rcftSHDSLPortLOSThreshold	INTEGER,
       rcftSHDSLPortLOSWThreshold	INTEGER,
       rcftSHDSLPortLOLKThreshold	INTEGER,
       rcftSHDSLPortESThreshold		INTEGER,
       rcftSHDSLPortLoopStatus		INTEGER,
       rcftSHDSLPortAttenuationInitThreshhold	INTEGER,
       rcftSHDSLPortBertStatus					INTEGER,
	   rcftSHDSLPortBertTime			    	Unsigned32, 
	   rcftSHDSLPortBertErrCode					Unsigned32,
	   rcftSHDSLPortBertUnusedTime				Unsigned32,
	   rcftSHDSLPortBertPortSpeed				Unsigned32,
	   rcftSHDSLPortBertCodeType				INTEGER,
	   rcftSHDSLPortBertCodeNum					INTEGER,
	   rcftSHDSLPortBertCodeNum					INTEGER,
	   rcftSHDSLPortOrder						INTEGER,
	   rcftSHDSLPortOrderTimeParameter			INTEGER,
	   rcftSHDSLPortOrderModeParameter			INTEGER	   	   	   	                
       } 
           
    rcftSHDSLPortIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		G.口的索引
    	"             
        ::= { rcftSHDSLPortEntry 1 }   
        
     rcftSHDSLPortAlarmStatus   OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-only
     	STATUS		current
     	DESCRIPTION
     	"
     		G.口的告警状态。
     		
     		BIT5: SHDSL-LOS	SHDSL端口LOS时该位清0，否则置1。
            BIT4: SHDSL-LOSW	SHDSL端口LOSW时该位清0，否则置1。
            BIT3: SHDSL-Link	SHDSL端口为link up状态该位置1，否则清0。
            BIT2: 保留	固定为0'b。
            BIT1: FEC	SHDSL端口有FEC错误时该位清0，否则置1。
            BIT0: CRC	SHDSL端口有CRC错误时该位清0，否则置1。
     	"
 	    ::= { rcftSHDSLPortEntry 2 }   
 	    
 	    
      rcftSHDSLPortStatus   OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-write
     	STATUS		current
     	DESCRIPTION
     	"
     		G.口的状态。   
			BIT23-22 IN_LBK-Test 00：没有进行过环回测试； 01：保留；10：内环测试成功；11：内环测试失败。
			BIT21-20 EX_LBK-Test 00：没有进行过环回测试； 01：保留；10：外环测试成功；11：外环测试失败。     
            BIT19-16 保留
     		BIT15-14 Port_En 	00：端口开；01：端口关；10：因故障转移关（无）；11：保留。
     		BIT13    LOC_FP_EN	1：使能本地xx端口故障向SHDSL口转移；  0：禁止。
			BIT12    REM_FP_EN	1：使能远端xx端口故障向SHDSL口转移；  0：禁止。    
			BIT11	 BOOT状态	0：BOOT失败；1：BOOT成功。 
			BIT10-9 保留   
			BIT8	SHDSL-synClock:   0:同步时钟， 1：准同步时钟
			BIT7-6	编码方式	00'b：TC-4PAM； 01'b：TC-16PAM；10'b：TC-32PAM。11'b：保留。
			BIT5-4	SHDSL-Annex	SHDSL端口采用AnnexA时：01b；为AnnexB时：10b；为AnnexF时：11b。
			BIT3	SHDSL-PSD	SHDSL端口采用对称PSD时该位置1，非对称PSD时清0。
			BIT2	SHDSL-Port-En	SHDSL端口为打开状态该位置1，为关闭状态则清0。
			BIT1	SHDSL-LineProbe	SHDSL端口为线路探测使能方式该位置1；否则清0。网管不需要处理！
			BIT0	STU-C	SHDSL端口工作于STU-C模式时该位置1，否则清0。
     	"
 	    ::= { rcftSHDSLPortEntry 3 }   
	      
	  rcftSHDSLPortCapableSpeed	OBJECT-TYPE
	  	SYNTAX	INTEGER 
     	MAX-ACCESS	read-only
     	STATUS		current
     	DESCRIPTION
     	"
     		G.口最大可用速率,单位为64Kbps。
	  	"   
	    ::= { rcftSHDSLPortEntry 4 }    
	      
	      
	  rcftSHDSLPortWorkSpeed OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-only
     	STATUS		current
     	DESCRIPTION
     	"
     		G.口的工作速率,单位为64Kbps。     	
     	"
 	    ::= { rcftSHDSLPortEntry 5 }  	  
 	    
	  rcftSHDSLPortProbeMaxSpeed OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-write
     	STATUS		current
     	DESCRIPTION
     	"
     		G.口处于线路探测方式时配置的允许最高速率,单位为64Kbps。  	
     	"
 	    ::= { rcftSHDSLPortEntry 6 }  	  
 	    
	  rcftSHDSLPortProbeMinSpeed OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-write
     	STATUS		current
     	DESCRIPTION
     	"
     		G.口处于线路探测方式时配置的允许最低速率,单位为64Kbps。  	
     	"
 	    ::= { rcftSHDSLPortEntry 7 }   	    
 	    
 	    
 		rcftSDHSLPortSNR    OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-only
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口正常工作时的SNR（信噪比）,单位为0.5dB。是有符号数，有可能是负数。  	
	     	"
	 	    ::= { rcftSHDSLPortEntry 8 }   	  
 	    
 		rcftSHDSLPortConfigSNR    OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口初始设置的SNR（信噪比）,单位为0.5dB。当linkup的瞬间G.口工作的SNR必须大于该
	     		初始值，之后rcftSDHSLPortSNR可以小于该初始值。是有符号数，有可能是负数。  	
	     	"
	 	    ::= { rcftSHDSLPortEntry 9 }    	    
 	     
 	    rcftSHDSLPortSNRThreshold	OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口SNR（信噪比）的门限值,单位为0.5dB。
	     		当rcftSDHSLPortSNR小于该值时,AGENT发送告警。是有符号数，有可能是负数。  	
	     	"
	 	    ::= { rcftSHDSLPortEntry 10 }    
	 	    
	 	    
 	     
 	    rcftSHDSLPortAttenuation	OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-only
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口的线路衰减值,单位为0.5dB。  	
	     	"
	 	    ::= { rcftSHDSLPortEntry 11 }    
	 	    	    
 	     
 	    rcftSHDSLPortAttenuationThreshold	OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口线路衰减值的门限值,单位为0.5dB。当rcftSHDSLPortAttenuation大于该门限值时
	     		AGENT发送告警。  	
	     	"
	 	    ::= { rcftSHDSLPortEntry 12 }    
	     
 	   	rcftSHDSLPortPBO  OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口线路功率回退结果        
				BIT7	Valid	结果值有意义时为1，否则清0（0-6位无意义）。
				BIT6	PBO_AUTO	PBO自动设置时该位置1，PBO固定设置则清0。
				BIT5-4	保留	固定为00'b。
				BIT3-0	PBO值	PBO实际值，单位为dB。
	     		
	     	"
	 	    ::= { rcftSHDSLPortEntry 13 }    
	 	    
	 	 rcftSHDSLPortLOSThreshold OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		当前15分钟LOS次数告警门限。取值范围是0-255。  		
	     	"
	 	    ::= { rcftSHDSLPortEntry 14 }  
	 	    
	 	    
	 	 rcftSHDSLPortLOSWThreshold OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		当前15分钟LOSW次数告警门限。取值范围是0-255。  		
	     	"
	 	    ::= { rcftSHDSLPortEntry 15 }   
	 	    
	 	    
	 	 rcftSHDSLPortLOLKThreshold OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		当前15分钟LOLK(linkdown)次数告警门限。取值范围是0-255。   		
	     	"
	 	    ::= { rcftSHDSLPortEntry 16 }  
	 	    
	 	 rcftSHDSLPortESThreshold OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		当前15分钟误码秒告警门限。取值范围是0-255。   		
	     	"
	 	    ::= { rcftSHDSLPortEntry 17 }  
	 	    
	 	               
	 	  rcftSHDSLPortLoopStatus	OBJECT-TYPE
	 	  	SYNTAX	INTEGER
	 	  	{
	 	  		insideLoop(1),
	 	  		outsideLoop(2),
	 	  		doubleloop(3),
	 	  		normal(100)	 	  		
	 	  	}             
	     	MAX-ACCESS	read-only
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口的环回状态:1：内环，2：外环，3：内环外环同时存在,100：正常		 
	     		对于约定四，统一为以下枚举值：
	     		E1的环回状态：1:本地环回，2：无本地环回，3：远端环回，4：无远端环回，
 				5:无环回，
 				6：本地内环使能，7：本地外环使能，8：本地双向环使能， 
 				9:远端内环使能，10：远端外环使能，11：远端双向环使能，
 				12：本地内环禁止，13:本地外环禁止，14：本地双向环禁止，
 				15：远端内环禁止，16：远端外环禁止，17：远端双向环禁止，
	     	"
	 	    ::= { rcftSHDSLPortEntry 18 }  
	 	               
 	    rcftSHDSLPortAttenuationInitThreshhold	OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口的环路衰减初始门限值，单位为0.5dB。 	
	     	"
	 	    ::= { rcftSHDSLPortEntry 19 }   	 	  
	 	                                  
	    rcftSHDSLPortBertStatus		OBJECT-TYPE
	    	SYNTAX  INTEGER
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	  BIT3			BERT_STATUS		0:正常工作；	1:正在bert
	    	  BIT2-1		BERT_RESULT     00:未测试      01:测试正常；	10:测试失败
	    	  BIT0			BERT_TYPE   	0:误码率方式；  1:误码时间方式
	    	"
	    	::= {rcftSHDSLPortEntry 20}
	    	
	    rcftSHDSLPortBertTime		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	误码率方式:测试时间，时间随测试的进行而增长,单位 S.  		
	    	"				                           
	    	::= {rcftSHDSLPortEntry  21}
	    
	    rcftSHDSLPortBertErrCode		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	 误码率方式:误码数，单位是bit
	    	"	                         
	    	::= {rcftSHDSLPortEntry 22}
	    	
	    rcftSHDSLPortBertUnusedTime		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	 误码率方式:测试不可用时间，超过255秒后维持255秒不变，单位是S.
	    	"
	    	::= {rcftSHDSLPortEntry 23} 
	    	
	    rcftSHDSLPortBertPortSpeed		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	 误码率方式:测试的端口速率，单位：kbps。
	    	"
	    	::= {rcftSHDSLPortEntry 24}    
	    	
	    rcftSHDSLPortBertCodeType		OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-write
	   		STATUS	current
	   		DESCRIPTION
	   		"
	   		误码率方式:测试码型，最大值为127   
	   		0000001'b：2的15次方-1；
			其它：保留。

	   		"         
	   		::= {rcftSHDSLPortEntry 25}       		
	   		
	    rcftSHDSLPortBertCodeNum		OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-write
	   		STATUS	current
	   		DESCRIPTION
	   		"
	   		误码率方式:测试码数    
	   		0000001'b：2的31次方个码；
			0000010'b：2的28次方个码；
			0000011'b：2的25次方个码；
			0000100'b：2的21次方个码；
			1111111'b：测试不停；
			其它：保留。

	   		"         
	   		::= {rcftSHDSLPortEntry 26}  	   	 	                                  
	 	                                  
		rcftSHDSLPortOrder	OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS	read-write
			STATUS	current
			DESCRIPTION
			"
			    1：硬复位
				2：软复位
				3：开始查询统计信息
				4：结束查询统计信息
				5：内环使能
				6：内环禁止
				7：外环使能
				8：外环禁止
				9: 无环回
				10: 本地内环使能
				11: 本地外环使能
				12: 本地双向环使能
				13: 远端内环使能
				14: 远端外环使能
				15: 远端双向环使能
				16: 本地内环禁止
				17: 本地外环禁止
				18: 本地双向环禁止
				19: 远端内环禁止
				20: 远端外环禁止
				21: 远端双向环禁止
				22: 端口BERT测试开始
				23: 端口BERT测试结束
				24：统计信息清零
				25：告警屏蔽
				26：告警屏蔽解除
				27：端口未用支路告警屏蔽
				28：端口未用支路告警屏蔽解除
				29：环回检测使能
				30：环回检测停止
				31：开始查询SFP扩展信息
				32；停止查询SFP扩展信息
				33：开始查询SFP数字诊断信息
				34：停止查询SFP数字诊断信息
				注：31-34尚没有对网管开放
					
				
			注意：对于约定5，枚举值5-9废弃，
			rcftSHDSLPortOrder的枚举值要和其他接口级命令节点始终保持完全一致。	
	
			"
		::= { rcftSHDSLPortEntry 27 }		 	               
	 	       
		rcftSHDSLPortOrderTimeParameter	OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS	read-write
			STATUS	current
			DESCRIPTION
			"
	  		    给局端模块G.口发送命令时所需要赋的时长参数。 
	  		    	
	  		    1: 测试时长1分钟
	  		    2: 测试时长2分钟
	  		    3: 测试时长4分钟
	  		    4: 测试时长8分钟
	  		    5: 测试时长15分钟
	  		    6: 测试时长30分钟
	  		    7: 测试时长60分钟  
	  		    	
	  		    18: 测试2帧
	  		    19: 测试5帧
	  		    20: 测试10帧
	  		    21: 测试20帧
	  		    22: 测试50帧
	  		    23: 测试100帧 	
	  		    
	  		    32: 测试终止
	  		    33：测试时间/帧数不限									
			"
		::= { rcftSHDSLPortEntry 28 }		 	       
	 	           
		rcftSHDSLPortOrderModeParameter	OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS	read-write
			STATUS	current
			DESCRIPTION
			"
	  		    给局端模块G.口发送命令时所需要赋的模式参数。 
                                                      
		    	  BIT5		MAC_ADDR_EXCH	0:正常环回；	1:交换 MAC 地址                                                      
		    	  BIT4		CRC_RECAL		0:正常环回；	1:重新计算CRC	  		    	
		    	  BIT3		DATA_OUT		0:不输出；	1:环回数据同时在另一侧输出  
		    	  BIT2		DATA_GENERATE   0:环回输入数据  1:产生测试数据；
		    	  BIT1		UTILITY	        0:速率为10%     1:速率为100%
		    	  BIT0		FRAME_LENGTH   	0:帧长62字节；  1:帧长1518字节
					
				  注意：BIT0和BIT1只有在BIT2为1的时候有效。					
			"
		::= { rcftSHDSLPortEntry 29 }		 	           
	 	           
