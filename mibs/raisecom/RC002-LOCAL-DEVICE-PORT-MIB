   --------------------------以太网光口的对象-----------------------------       
    rcftEthFxPortTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftEthFxPortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The EthFx PORT table.
       "
       ::= { rcftEthFxPortObjects 1 }
   
    rcftEthFxPortEntry OBJECT-TYPE
       SYNTAX  RcftEthFxPortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the EthFx PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,rcftEthFxPortIndex }
       ::= { rcftEthFxPortTable 1 }
                              
    RcftEthFxPortEntry ::=
       SEQUENCE {
       rcftEthFxPortIndex		 	Integer32,   
       rcftEthFxPortStatus			INTEGER,  
       rcftEthFxPortModuleMaxSpeed INTEGER,  
       rcftEthFxPortConnectorType INTEGER,
       rcftEthFxPortTransmitMedia INTEGER,
       rcftEthFxPortModuleWaveLen INTEGER,
       rcftEthFxPortModuleManufacturer OCTET STRING,
       rcftEthFxPortModuleDescr OCTET STRING,
       rcftEthFxPortModuleVersion OCTET STRING,
       rcftEthFxPortModuleSerialNumber OCTET STRING,  
       rcftEthFxPortModuleType INTEGER,   
       rcftEthFxPortRxRestrictSpeed	INTEGER,
       rcftEthFxPortTxRestrictSpeed	INTEGER,
       rcftEthFxPortRestrictSpeedStep INTEGER,
       rcftEthFxPortLoopOrder	INTEGER,
       rcftEthFxPortLoopStatus  INTEGER,
       rcftEthFxPortSFPDiagnoInfo INTEGER,    
       rcftEthFxPortSFPDiagnoAlarmStatus INTEGER,
       rcftEthFxPortSFPDiagnoWarningStatus INTEGER,
       rcftEthFxPortTranDistance INTEGER,
       rcftEthFxPortSFPType INTEGER,
       rcftEthFxPortSFPInfo INTEGER,
       rcftEthFxPortPVID	INTEGER,
       rcftEthFxPorttag		INTEGER,
       rcftEthFxPortCOS		INTEGER	  
       }           
       
       
    rcftEthFxPortIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		EthFx口的索引
    	"             
        ::= { rcftEthFxPortEntry 1 }
        
    rcftEthFxPortStatus OBJECT-TYPE
    	SYNTAX		INTEGER
    	MAX-ACCESS	read-write
    	STATUS		current
    	DESCRIPTION
    	"      
      bit23：光口LOS告警					1:恢复    0：告警
      bit22：光口LOL告警。				    1：恢复    0：告警	
      Bit21：以太网光口OAM网管。			1：使能；  0：禁止。
      Bit20：LBK-Test-OK（Bit19为1时有效）。1：环回测试成功，0：环回测试失败 
      Bit19：LBK-Test_Valid。1：环回测试过， 0：没有进行过环回测试                             
      Bit18：远端掉电。1：掉电， 0：未掉电
      Bit17：发送使能。1：使能， 0：禁止
      Bit16：流量控制。1：使能， 0：禁止 
      Bit15：与bit3 bit2 配合使用表示EthFx端口的状态
      		bit15 bit3 bit2
      		000：光口开
      		001：光口关
      		010：因本端故障转移关
      		011：因ALS关
      		100：因环回关
      		     
      Bit14：激光管灵敏度：1：异常，0：正常    	
      Bit13：激光管发送功率：1：异常，0：正常    	
      Bit12：激光管工作状态：1：异常，0：正常    	
      Bit11：远端xx端口故障向光口转移：1：使能，0：禁止    	
      Bit10：本地xx端口故障向光口转移：1：使能，0：禁止     
      Bit9：光口SD信号：1：有效，0：无效
      Bit8：光口双工：1：全双工，0：半双工       
      Bit7-6：光口速率：00：10M， 01：100M，
      		  10：1000M， 11：保留             
      Bit5：光口自协商：1：使能，0：禁止              	 
      Bit4：ALS功能：1：打开，0：禁止     
      Bit3-2：光口使能：00：开启，01：关闭，
      		  10：光口因为电口故障关，11：光口因为无光收信号关
      Bit1：接口Link状态： 1：Linkup，0：Linkdown
      Bit0：模块不在位通知Exist：1：在位，0：不在位  	
    	"      
 		::= { rcftEthFxPortEntry 2 } 
 		
 		rcftEthFxPortModuleMaxSpeed OBJECT-TYPE
      SYNTAX INTEGER
      {
        stm16(1),
        stm8(2),
        stm4(3),
        stm1(4)
      }
      MAX-ACCESS read-only  
      STATUS current
      DESCRIPTION
      "
       	SFP模块接口模块的最大速率:(1 : 2.5Gbit/s; 
       	                    2 : 1.25Gbit/s; 
       	                    3 : 622Mbit/s; 
       	                    4 : 155Mbit/s 
       	                    5 : 125Mbit/s)
      "  
    ::={ rcftEthFxPortEntry 3 } 
    
    rcftEthFxPortConnectorType OBJECT-TYPE
      SYNTAX INTEGER
      {
        unkkown(1),
        rj45(2),
        sc(3),
        style1(4),
        style2(5),
        bnctnc(6),
        coaheader(7),
        jack(8),
        lc(9),
        mtrj(10),
        mu(11), 
        sg(12),  
        opticalpigtail(13),
        hssdc2(14),
        copperpigtail(15)
             
      }
      MAX-ACCESS read-only  
      STATUS current
      DESCRIPTION
      "
       	SFP模块连接头类型:(1 : 未知类型; 
       	                   2 : rj45; 
       	                   3 : sc; 
       	                   4 : style1;
       	                   5 : style2;
       	                   6 : bnctnc;
       	                   7 : coaheader;
       	                   8 : jack;
       	                   9 : lc;
       	                   10 : mtrj;
       	                   11 : mu;
       	                   12 : sg;
       	                   13 : opticalpigtail;
       	                   14 : hssdc2;
       	                   15 : copperpigtail)
      "  
    ::={ rcftEthFxPortEntry 4 }   
    
    rcftEthFxPortTransmitMedia OBJECT-TYPE
      SYNTAX INTEGER
      {
        unkkown(1),
        singleMode9um(2),
        multiMode50um(3),
        multiMode62point5um(4),
        copperline(15)
        
      }
      MAX-ACCESS read-only  
      STATUS current
      DESCRIPTION
      "
       	SFP模块传输介质:(1 : 未知类型; 
       	                 2 : 单模9um; 
       	                 3 : 多模50um; 
       	                 4 : 多模62.5um
       	                 15：铜线)
      "  
    ::={ rcftEthFxPortEntry 5 }
    
    rcftEthFxPortModuleWaveLen OBJECT-TYPE
      SYNTAX INTEGER   
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
      "
       	SFP模块工作波长(单位为nm) 
      " 
    ::={rcftEthFxPortEntry 6}  
    
    rcftEthFxPortModuleManufacturer  OBJECT-TYPE
      SYNTAX OCTET STRING(SIZE (0..30))   
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
      "
       SFP模块的模块制造厂商
      " 
    ::={rcftEthFxPortEntry 7}

    rcftEthFxPortModuleDescr OBJECT-TYPE
      SYNTAX OCTET STRING(SIZE (0..30))
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
      "
        SFP模块的产品型号
      "	 
    ::={rcftEthFxPortEntry 8}   
        
    rcftEthFxPortModuleVersion OBJECT-TYPE
      SYNTAX OCTET STRING(SIZE (0..10))
      MAX-ACCESS read-only
      STATUS current           
      DESCRIPTION
      "
        SFP模块的版本号
      "	 
    ::={rcftEthFxPortEntry 9}  
        
    rcftEthFxPortModuleSerialNumber OBJECT-TYPE
      SYNTAX OCTET STRING(SIZE (0..30))
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
      "
        SFP模块的序列号
      "	 
    ::={rcftEthFxPortEntry 10}
               
    rcftEthFxPortModuleType  OBJECT-TYPE
      SYNTAX INTEGER
      {   
      	 optical-M(1),
      	 optical-S1(2),
      	 optical-S2(3),
      	 optical-S3(4),
      	 optical-SS13(5), 
      	 optical-SS15(6),
      	 optical-SS23(7),
      	 optical-SS25(8),
      	 --optical-SS3(9),  kuanglili 20090519 根据公司规范取消ss3光头类型
      	 optical-SS34(9),
      	 optical-SS35(10),
      	 optical-S15(12),  --kuanglili 20090909 S15光模块类型枚举值为12
      	 optical_SFP(15), 
      	 unknown-type(100)
      }   
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
      "
      	 以太网光口的光模块类型
      "
	::= { rcftEthFxPortEntry 11 }               
               
 	rcftEthFxPortRxRestrictSpeed  OBJECT-TYPE
    	SYNTAX		INTEGER
    	MAX-ACCESS	read-write
    	STATUS		current
    	DESCRIPTION 
    	"
             接收带宽
    	"
 	::= { rcftEthFxPortEntry 12 }       
	
 	rcftEthFxPortTxRestrictSpeed  OBJECT-TYPE
    	SYNTAX		INTEGER
    	MAX-ACCESS	read-write
    	STATUS		current
    	DESCRIPTION 
    	"
           发送带宽
       	"
 	::= { rcftEthFxPortEntry 13 }    
 		
 	rcftEthFxPortRestrictSpeedStep	OBJECT-TYPE
 		SYNTAX	INTEGER
 		MAX-ACCESS	read-only
 		STATUS		current
 		DESCRIPTION
 		"
 			带宽粒度 
 			0x00：64Kbps；0x01：32Kbps（无）；其余：保留。 		
 		"   
 	::= { rcftEthFxPortEntry 14 }  
 	     
 	rcftEthFxPortLoopOrder	OBJECT-TYPE
 	     SYNTAX	INTEGER   
 	     {
 	     	ethFxinsideLoopEnable(1), 
 	     	ethFxinsideLoopDisable(2),
 	     	ethFxLoopbackTest(3)
 	     }
 	     MAX-ACCESS	read-write
 		 STATUS		current 
 		 DESCRIPTION
 		 "
 		   光口环回命令: 
 		   	1:本地内环环回使能，
 		   	2:本地内环环回禁止 ,
 		   	3:环回检测（一次完成自动停止）     
 		   	4:远端外环使能
 		   	5：远端外环禁止
 		   	6：OAM环使能
 		   	7：OAM环禁止
 		   
 		 "
 	::= { rcftEthFxPortEntry 15 }
 	
 	rcftEthFxPortLoopStatus		OBJECT-TYPE
 	     SYNTAX	INTEGER   
 	     {
 	     	ethFxPortoutsideLoop(1),
 	     	ethFxPortnormal(100)
 	     }
 	     MAX-ACCESS	read-only
 	     STATUS		current
         DESCRIPTION
         "
            光口环回: 
            	1: 光口本地外环  
            	2：光口远端外环
       		3：光口本地内环 
            	4：OAM环
            	
            	100:正常
         "                          
 	::= { rcftEthFxPortEntry 16 }   
 	
 	rcftEthFxPortSFPDiagnoInfo	OBJECT-TYPE	 		      
  		 SYNTAX	INTEGER
  		 MAX-ACCESS 	read-only
  		 STATUS	current
  		 DESCRIPTION  
  		 "
     		局端模块数字诊断的相关信息   
    		bit3: 监测到sfp模块         1：有告警   0：无     
     		bit2: 采用内部校准：  		1：有效，  	0：无效
      		bit1: 采用外部校准：  		1：有效，  	0：无效    		     
     		bit0: 是否具有数字诊断功能：1：有 		0：无 
  		 "
 	     ::={rcftEthFxPortEntry 17}

 	 rcftEthFxPortSFPDiagnoAlarmStatus	OBJECT-TYPE	 		      
  		 SYNTAX	INTEGER
  		 MAX-ACCESS 	read-only
  		 STATUS	current
  		 DESCRIPTION  
  		 "
     		局端模块数字诊断的相关告警        
            
            bit9: 收光功率下限告警：  	1：有告警，0：没有告警     		                       
     		bit8: 收光功率上限告警：  	1：有告警，0：没有告警    		     
     		bit7: 发光功率下限告警：  	1：有告警，0：没有告警
     		bit6: 发光功率上限告警：  	1：有告警，0：没有告警  	                       	                       
     		bit5: 电流下限告警：  		1：有告警，0：没有告警    		     
     		bit4: 电流上限告警：  		1：有告警，0：没有告警
     		bit3: 电压下限告警：  		1：有告警，0：没有告警    		     
     		bit2: 电压上限告警：  		1：有告警，0：没有告警
     		bit1: 温度下限告警：  		1：有告警，0：没有告警    		     
     		bit0: 温度上限告警：  		1：有告警，0：没有告警

 	     "
 	     ::={rcftEthFxPortEntry 18}     
 	      
 	  rcftEthFxPortSFPDiagnoWarningStatus   OBJECT-TYPE	 		      
  		 SYNTAX	INTEGER
  		 MAX-ACCESS 	read-only
  		 STATUS	current
  		 DESCRIPTION  
  		 "
     		局端模块数字诊断的相关警告   
     		
            bit9: 收光功率下限警告：  	1：有警告，0：没有警告     		                       
     		bit8: 收光功率上限警告：  	1：有警告，0：没有警告     		     
     		bit7: 发光功率下限警告：  	1：有警告，0：没有警告 
     		bit6: 发光功率上限警告：  	1：有警告，0：没有警告   	                       	                       
     		bit5: 电流下限警告：  		1：有警告，0：没有警告     		     
     		bit4: 电流上限警告：  		1：有警告，0：没有警告 
     		bit3: 电压下限警告：  		1：有警告，0：没有警告    		     
     		bit2: 电压上限警告：  		1：有警告，0：没有警告 
     		bit1: 温度下限警告：  		1：有警告，0：没有警告     		     
     		bit0: 温度上限警告：  		1：有警告，0：没有警告 
          "
 	     ::={rcftEthFxPortEntry 19}    
 	     
