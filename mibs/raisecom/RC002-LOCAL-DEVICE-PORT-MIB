   --------------------------V35端口的对象---------------------------------       
           
    rcftV35PortTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftV35PortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The V35 PORT table.
       "
       ::= { rcftV35PortObjects 1 }
   
    rcftV35PortEntry OBJECT-TYPE
       SYNTAX  RcftV35PortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the V35 PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,rcftV35PortIndex }
       ::= { rcftV35PortTable 1 }
                 
                 
    RcftV35PortEntry ::=
       SEQUENCE {
       rcftV35PortIndex		 	Integer32,
       rcftV35PortAlarmStatus	INTEGER,
       rcftV35PortStatus		INTEGER,  
       rcftV35PortSpeed			Unsigned32,
       rcftV35PortBertStatus			INTEGER,
	   rcftV35PortBertTime			    Unsigned32, 
	   rcftV35PortBertErrCode			Unsigned32,
	   rcftV35PortBertUnusedTime		Unsigned32,
	   rcftV35PortBertPortSpeed			Unsigned32,
	   rcftV35PortBertCodeType			INTEGER,
	   rcftV35PortBertCodeNum			INTEGER,
	   rcftV35PortLoopStatus			INTEGER,
	   rcftV35PortOrder					INTEGER	   	   	         
       }                               
       
    rcftV35PortIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		V35口的索引
    	"             
        ::= { rcftV35PortEntry 1 }   
 
    rcftV35PortAlarmStatus	OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
			其它位	保留	固定000'b。 
			BIT12	DSR_I		1：DSR信号UP；        0：DSR信号DOWN。		
			
			BIT11	RAIS    站在本地设备上看对端V35口所占那一路E1的AIS告警 1：正常 	0：告警  	
			BIT10	RCV     站在本地设备上看对端V35口所占那一路E1的CV告警  1：正常 	0：告警  
			BIT9	RLOF	站在本地设备上看对端V35口所占那一路E1的LOF告警 1：正常 	0：告警  
			BIT8	AIS     V35口所占那一路E1的AIS告警 1：正常 	0：告警  	   
			BIT7	CV      V35口所占那一路E1的CV告警  1：正常 	0：告警  
			BIT6	LOF	    V35口所占那一路E1的LOF告警 1：正常 	0：告警        
			BIT5	PATT		1：误码测试正常       0：误码测试告警 			
			BIT4    CRC_ALARM   1：正常 			  0：告警        
			         		
			BIT3	DCD_O		1：DCD信号UP；        0：DCD信号DOWN。
			BIT2	CTS_O		1：CTS信号UP；        0：CTS信号DOWN。
			BIT1	DTR_I		1：DTR信号UP；        0：DTR信号DOWN。
			BIT0	RTS_I		1：RTS信号UP；        0：RTS信号DOWN。
    	"
        ::= { rcftV35PortEntry 2 }   
    	
 	rcftV35PortStatus     OBJECT-TYPE
 		SYNTAX	INTEGER
    	MAX-ACCESS read-write
    	STATUS		current
    	DESCRIPTION
    	"
    		BIT25	 V35口带宽跟随：1：跟随； 0：不跟随    	   
    		BIT24	 V35口占用时隙的成帧模式：1：成帧； 0：透明 
			BIT23-22 IN_LBK-Test 00：没有进行过内环测试； 01：保留；10：内环测试成功；11：内环测试失败。
			BIT21-20 EX_LBK-Test 00：没有进行过环回测试； 01：保留；10：外环测试成功；11：外环测试失败。
			BIT19-18 CLOCK   	 00：从时钟  01：V35终端时钟，并且V35带宽不跟随 
								 10：V35终端时钟，并且V35带宽跟随  11：主时钟   
			BIT17-16 LOOP_STATE  00:不环回   01：本地环回  	 10：远端环回  11：保留
    	
			BIT15-14 Port_En  	00：端口开(DCD=0、CTS=0)；01：端口关(DCD=1、CTS=1)；10：因故障转移关；        11：保留。
			BIT13    LOC_FP_EN	1：使能本地其它端口故障向V35口转移；  0：禁止。
			BIT12    REM_FP_EN	1：使能远端V35口故障向V35口转移；0：禁止。
			BIT11    Remote CRC_EN 站在本地上看远端V35口CRC功能  1：使能      0：禁止
			BIT10	 Local CRC_EN   1：使能      0：禁止
			BIT9-8   接口方式	00：M34M；01：HDB26M；其它：保留。
    	
			BIT7    RCLK_AN		0：RXCLK时钟固定；    1：RXCLK时钟相位适应。
			BIT6    RCLK_INV	0：RXCLK时钟正相；1：RXCLK时钟反相。
			BIT5    TCLK_INV	0：TXCLK时钟正相；1：TXCLK时钟反相。
			BIT4    SA_DISABLE	1：不使用SA bit   0：使用SA bit
			BIT3	Rate_CFG   	1：时隙带宽主控   0：时隙带宽被控  
			BIT2	BERT_EN		1：误码测试使能   0：误码测试禁止
			BIT1    DATA_V		0：输入数据中断； 1：输入数据正常。
			BIT0    CLK_V		0：输入时钟异常； 1：输入时钟正常。
    	"
        ::= { rcftV35PortEntry 3 }   
        
    rcftV35PortSpeed	OBJECT-TYPE
 		SYNTAX	Unsigned32
    	MAX-ACCESS read-write
    	STATUS		current
    	DESCRIPTION
		"
		   V35口的速率	单位为64Kbps。
		"  
        ::= { rcftV35PortEntry 4 }  		
		                               
	rcftV35PortBertStatus		OBJECT-TYPE
	    SYNTAX  INTEGER
	    MAX-ACCESS	read-only
	    STATUS	current
	    DESCRIPTION
	    "
	    	 BIT3			BERT_STATUS		0:正常工作；	1:正在bert
	    	 BIT2-1		BERT_RESULT     00:未测试      01:测试正常；	10:测试失败
	    	 BIT0			BERT_TYPE   	0:误码率方式；  1:误码时间方式
	    "
	    ::= {rcftV35PortEntry 5 }
	    	
	rcftV35PortBertTime		OBJECT-TYPE
	    SYNTAX	Unsigned32
	    MAX-ACCESS	read-only
	    STATUS	current
	    DESCRIPTION
	    "
	    	误码率方式:测试时间，时间随测试的进行而增长,单位 S.  		
	    "				                           
	    ::= {rcftV35PortEntry  6 }
	    
	rcftV35PortBertErrCode		OBJECT-TYPE
	    SYNTAX	Unsigned32
	    MAX-ACCESS	read-only
	    STATUS	current
	    DESCRIPTION
	    "
	    	误码率方式:误码数，单位是bit
	    "	                         
	    ::= {rcftV35PortEntry 7 }
	    	
	rcftV35PortBertUnusedTime		OBJECT-TYPE
	    SYNTAX	Unsigned32
	    MAX-ACCESS	read-only
	    STATUS	current
	    DESCRIPTION
	    "
	    	误码率方式:测试不可用时间，超过255秒后维持255秒不变，单位是S.
	    "
	    ::= {rcftV35PortEntry 8 } 
	    	
	rcftV35PortBertPortSpeed		OBJECT-TYPE
	    SYNTAX	Unsigned32
	    MAX-ACCESS	read-only
	    STATUS	current
	    DESCRIPTION
	    "
	    	误码率方式:测试的端口速率，单位：kbps。
	 	"   	
	    ::= {rcftV35PortEntry 9 }    
	    	
	rcftV35PortBertCodeType		OBJECT-TYPE
	   	SYNTAX INTEGER
	   	MAX-ACCESS	read-write
	   	STATUS	current
	   	DESCRIPTION
	   	"
	   		误码率方式:测试码型，最大值为127   
	   		0000001'b：2的15次方-1；
			其它：保留。
	   	"         
	   	::= {rcftV35PortEntry 10 }       		
	   		
	rcftV35PortBertCodeNum		OBJECT-TYPE
	   	SYNTAX INTEGER
	   	MAX-ACCESS	read-write
	   	STATUS	current
	   	DESCRIPTION
	   	"
	   		误码率方式:测试码数    
	   		0010001'b：2的31次方个码；
			0010010'b：2的28次方个码；
			0010011'b：2的25次方个码；
			0010100'b：2的21次方个码；
			0100001'b：测试不停；
			其它：保留。
	   	"         
	   	::= {rcftV35PortEntry 11 }  	  
	   	
	 rcftV35PortLoopStatus	OBJECT-TYPE
	 	SYNTAX INTEGER           
	    MAX-ACCESS read-only
	    STATUS current
	    DESCRIPTION
	     "	 
	     	E1的环回状态：1:本地环回，2：无本地环回，3：远端环回，4：无远端环回，
 			5:无环回，
 			6：本地内环使能，7：本地外环使能，8：本地双向环使能， 
 			9:远端内环使能，10：远端外环使能，11：远端双向环使能，
 			12：本地内环禁止，13:本地外环禁止，14：本地双向环禁止，
 			15：远端内环禁止，16：远端外环禁止，17：远端双向环禁止，
	     "
	 	 ::= { rcftV35PortEntry 12 }  	 
	 	 
	rcftV35PortOrder	OBJECT-TYPE
		SYNTAX	INTEGER
		MAX-ACCESS	read-write
		STATUS	current
		DESCRIPTION
		"
			
		    1：硬复位
			2：软复位
			3：开始查询统计信息
			4：结束查询统计信息
			5：内环使能
			6：内环禁止
			7：外环使能
			8：外环禁止
			9: 无环回
			10: 本地内环使能
			11: 本地外环使能
			12: 本地双向环使能
			13: 远端内环使能
			14: 远端外环使能
			15: 远端双向环使能
			16: 本地内环禁止
			17: 本地外环禁止
			18: 本地双向环禁止
			19: 远端内环禁止
			20: 远端外环禁止
			21: 远端双向环禁止
			22: 端口BERT测试开始
			23: 端口BERT测试结束
			24：统计信息清零
			25：告警屏蔽
			26：告警屏蔽解除
			27：端口未用支路告警屏蔽
			28：端口未用支路告警屏蔽解除
			29：环回检测使能
			30：环回检测停止
			31：开始查询SFP扩展信息
			32；停止查询SFP扩展信息
			33：开始查询SFP数字诊断信息
			34：停止查询SFP数字诊断信息
			注：31-34尚没有对网管开放
					
				
			注意：对于约定5，枚举值5-9废弃，
			rcftV35PortOrder的枚举值要和其他接口级命令节点始终保持完全一致。						
		"
		::= { rcftV35PortEntry 13 }		 	   	 			                               
