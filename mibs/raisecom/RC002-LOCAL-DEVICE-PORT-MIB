  -----------------------VCG MIB----------------------------            
     rcftSlotVCGTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftSlotVCGEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The VCG table.
       "
       ::= { rcftSlotVCGObjects 1 }
   
    rcftSlotVCGEntry OBJECT-TYPE
       SYNTAX  RcftSlotVCGEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the VCG table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,rcftSlotVCGIndex }
       ::= { rcftSlotVCGTable 1 }
                 
                 
    RcftSlotVCGEntry ::=
       SEQUENCE {
       rcftSlotVCGIndex		 	Integer32,    
       rcftSlotVCGStatus		INTEGER, 
       rcftSlotVCGLoopStatus	INTEGER,  
       rcftSlotVCGLcasXPR		INTEGER,
       rcftSlotVCGLcasXAR		INTEGER,
       rcftSlotVCGLcasXPT		INTEGER,
       rcftSlotVCGLcasXAT		INTEGER,
       rcftSlotVCGAlarmStatus	INTEGER, 
       rcftSlotVCGTxISPTPID     INTEGER,  
       rcftSlotVCGRxISPTPID     INTEGER,
       rcftSlotVCGBaseCoS 		INTEGER,
       rcftSlotVCGVLANID		INTEGER,
       rcftSlotVCGMemberList	OCTET STRING,    
       rcftSlotToRVCGMemberList	  OCTET STRING,  
       rcftSlotVCGMemberStatus    OCTET STRING, 
       rcftSlotVCGMemberRxCode    OCTET STRING, 
       rcftSlotVCGMemberTxCode    OCTET STRING, 
       rcftSlotVCGMemberAlarmStatus  	OCTET STRING,  
       rcftSlotToRVCGMemberAlarmStatus  OCTET STRING
                        
       }          
                                                 
       
    rcftSlotVCGIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		VCG 的索引
    	"             
        ::= { rcftSlotVCGEntry 1 }    
        
        rcftSlotVCGStatus OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
        "    
        	bit10:CSF发送				1：使能，0：禁止 
        	bit9：对端VLI。				1：使能，0：禁止
        	bit8：对端Vcat。			1：使能，0：禁止
            bit7：对端Lcas。			1：使能，0：禁止   
                         
        	bit6：是否带tag 				1：使能，0：禁止	
        	bit5：CheckLoop。				1：使能，0：禁止 
        	bit4：Fcs。						1：使能，0：禁止
        	bit3：GFP净荷扰码Scrb。			1：使能，0：禁止
        	bit2：VLI。						1：使能，0：禁止
        	bit1：Vcat。					1：使能，0：禁止
        	bit0：Lcas。					1：使能，0：禁止   
        	
        	LCAS on时，VCAT不可配（内部强制使能）。
        	只有LCAS off时，网管可配VCAT。
			VLI始终可配，但是只在LCAS off、VCAT 禁止时配置生效。      	
        "                                                  
        ::= {rcftSlotVCGEntry 2}
        
        rcftSlotVCGLoopStatus	OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
           0：无环，1~63：表示有环的VCG号
        " 
        ::= {rcftSlotVCGEntry 3}
        rcftSlotVCGLcasXPR  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
           	接收配置成员数（1-E1接口数目）
        " 
        ::= {rcftSlotVCGEntry 4}   
        
        rcftSlotVCGLcasXAR  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
           	接收实际成员数（1-E1接口数目）
        " 
        ::= {rcftSlotVCGEntry 5}   
        
        rcftSlotVCGLcasXPT  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
           	 发送配置成员数（1-E1接口数目）
        " 
        ::= {rcftSlotVCGEntry 6}   
        
        rcftSlotVCGLcasXAT  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
           	 发送实际成员数（1-E1接口数目）
        " 
        ::= {rcftSlotVCGEntry 7}
        rcftSlotVCGAlarmStatus	OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
           	 VCG 告警    0：告警，1：正常
           	 bit0：GID      	
           	 bit1：LOA
			 bit2：LFD
			 bit3：CFS         	            	            	                                      
             bit4：TLCT
           	 bit5：PLCT	
           	 bit6：TLCR
			 bit7：PLCR
			 
			 bit8：对端GID：
			 bit9：对端LOA
			 bit10：对端LFD
			 bit11：对端TLCT
		 	 bit12：对端PLCT	
			 bit13：对端TLCR
			 bit14：对端PLCR 
                            
        " 
        ::= {rcftSlotVCGEntry 8}     
        
       rcftSlotVCGTxISPTPID  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current 
        DESCRIPTION
       	"
           	发送侧 TPID
       	" 
       	::= {rcftSlotVCGEntry 9}   
        
       rcftSlotVCGRxISPTPID  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current 
        DESCRIPTION
       	"
           	接收侧 TPID
       	" 
       	::= {rcftSlotVCGEntry 10}    
       	
       	
       rcftSlotVCGBaseCoS 		OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current 
        DESCRIPTION
       	"
           	COS：0~7
       	" 
       	::= {rcftSlotVCGEntry 11}  
       	
       rcftSlotVCGVLANID  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current 
        DESCRIPTION
       	"
           	VLANID
       	" 
       	::= {rcftSlotVCGEntry 12}  
       	
       	rcftSlotVCGMemberList  OBJECT-TYPE
	  		SYNTAX	OCTET STRING(SIZE (0..15))
			MAX-ACCESS	read-write
			STATUS	current
			DESCRIPTION
			"
			  E1成员列表，每个字节表示1个成员对应的E1端口号。
				此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
				每个字节为0表示该成员无效。
			" 	  	  
            ::={rcftSlotVCGEntry 13}   
        
       rcftSlotToRVCGMemberList	  OBJECT-TYPE
	  		SYNTAX	OCTET STRING(SIZE (0..15))
			MAX-ACCESS	read-write
			STATUS	current
			DESCRIPTION
			"
			  对端E1成员列表，每个字节表示1个成员对应的E1端口号。
				此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
				每个字节为0表示该成员无效。
			" 	  	  
            ::={rcftSlotVCGEntry 14}   
  
       rcftSlotVCGMemberStatus    OBJECT-TYPE
	  		SYNTAX	OCTET STRING(SIZE (0..15))
			MAX-ACCESS	read-write
			STATUS	current
			DESCRIPTION
			"
			  此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
				每个字节的：
				bit0：接收成员状态。		1：OK，0：Error。
				bit1：发送成员状态。		1：OK，2：Error。
			" 	  	  
            ::={rcftSlotVCGEntry 15}   

       rcftSlotVCGMemberRxCode    OBJECT-TYPE
	  		SYNTAX	OCTET STRING(SIZE (0..15))
			MAX-ACCESS	read-only
			STATUS	current
			DESCRIPTION
			"
			  成员接收控制字，此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
				每个字节的枚举值如下：
					1：FIXED：固定带宽，非LCAS模式。
					2：ADD：将当前成员添加到VCG组中。
					3：NORM：正在添加， 通道良好。
					4：EOS：添加结束。
					5：IDLE：当前成员没有添加到VCG中。
					6：DNU：已添加但是通道故障。

			" 	  	  
            ::={rcftSlotVCGEntry 16}   
 
       rcftSlotVCGMemberTxCode    OBJECT-TYPE
	  		SYNTAX	OCTET STRING(SIZE (0..15))
			MAX-ACCESS	read-only
			STATUS	current
			DESCRIPTION
			"
			  成员发送控制字，此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
				每个字节的枚举值如下：
					1：FIXED 
					2：ADD
					3：NORM
					4：EOS
					5：IDLE
					6：DNU 

			" 	  	  
            ::={rcftSlotVCGEntry 17}   
 
       rcftSlotVCGMemberAlarmStatus  	OBJECT-TYPE
	  		SYNTAX	OCTET STRING(SIZE (0..15))
			MAX-ACCESS	read-only
			STATUS	current
			DESCRIPTION
			"
			  成员告警状态，此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
				每个字节的bit位定义如下：
					Bit0：LOM：复帧失步告警
					Bit1：CRC：
					Bit2：SQM：序列指示失配告警
					Bit3：LOA：成员定位丢失

			" 	  	  
            ::={rcftSlotVCGEntry 18}   
  
       rcftSlotToRVCGMemberAlarmStatus  OBJECT-TYPE
	  		SYNTAX	OCTET STRING(SIZE (0..15))
			MAX-ACCESS	read-only
			STATUS	current
			DESCRIPTION
			"
			  对端成员告警状态，此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
				每个字节的bit位定义如下：
					Bit0：对端LOM
					Bit1：对端CRC
					Bit2：对端SQM
					Bit3：对端LOA 
			" 	  	  
            ::={rcftSlotVCGEntry 19}   


        
         --------------------------VCG GFP帧流量统计--------------------------   
         rcftSlotVCGStatisticTable OBJECT-TYPE
	        SYNTAX SEQUENCE OF RcftSlotVCGStatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	          局端设备GFP的统计表.
	          "
            ::= {rcftSlotVCGPerformance 1}
	    rcftSlotVCGStatisticEntry OBJECT-TYPE
	        SYNTAX RcftSlotVCGStatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	             以rcftSlotVCGIndex为索引
	          "
	        AUGMENTS { rcftSlotVCGEntry }
	        ::= {rcftSlotVCGStatisticTable 1}
	    RcftSlotVCGStatisticEntry ::= SEQUENCE {
				rcftVCGRxClientPackets    	  Counter32,  
				rcftVCGRxIdlePackets		   Counter32, 
				rcftVCGRxMgmntPackets		  Counter32, 			
				rcftVCGRxFCSErrMgmntPackets   Counter32,
				rcftVCGRxLenErrPackets   	  Counter32,
				rcftVCGRxFCSErrClientPackets  Counter32,
				rcftVCGRxThecErrPackets     Counter32,
				rcftVCGRxEhecErrPackets     Counter32,
				rcftVCGRxCIDErrPackets      Counter32,
				rcftVCGRxSpareErrPackets    Counter32,
				rcftVCGRxChecCorPackets     Counter32,
				rcftVCGRxThecCorPackets     Counter32,
				rcftVCGRxEhecCorPackets     Counter32,
				rcftVCGRxBytes          	Counter32,
				rcftVCGTxClientPackets        Counter32,
				rcftVCGTxIdlePackets        Counter32, 
				rcftVCGTxMgmntPackets		Counter32,
				rcftVCGTxBytes				Counter32,
				rcftVCGFluxTimer		    Counter32		
	         }
					
          rcftVCGRxClientPackets    OBJECT-TYPE
