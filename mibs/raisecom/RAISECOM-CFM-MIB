--------------------------------------------------------------------------------
rcCfmMipExTable 	OBJECT-TYPE
    SYNTAX  SEQUENCE OF RcCfmMipExEntry
    ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "CFM mip extend Table "  
    ::= { rcCfm 25 }
    
rcCfmMipExEntry     OBJECT-TYPE
	SYNTAX      RcCfmMipExEntry
    ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry in the rcCfmMipExTable."
    INDEX	{ dot1agCfmMaCompPrimaryVlanId, 
    		  rcCfmMipExIfIndex
    		  
    		}
    ::= { rcCfmMipExTable 1 }   
    
RcCfmMipExEntry ::= SEQUENCE {
    rcCfmMipExIfIndex         INTEGER,
	rcCfmMipRowStatus         RowStatus 
}                    

rcCfmMipExIfIndex    OBJECT-TYPE 
	SYNTAX      INTEGER
	MAX-ACCESS  read-create
	STATUS      current
    DESCRIPTION
            "The ifIndex of the manual-created MIP"
	::= { rcCfmMipExEntry 1 }   
	
rcCfmMipRowStatus OBJECT-TYPE
          SYNTAX    RowStatus
          ACCESS    read-create
          STATUS    current
          DESCRIPTION
                 "The status of this entry."
          ::= { rcCfmMipExEntry 2 } 

	
END      


