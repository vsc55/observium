   rc002 OBJECT IDENTIFIER ::= { raiseCom 2 }

    rcftMibObjects OBJECT IDENTIFIER ::= { rc002 1 }

    rcftTraps OBJECT IDENTIFIER ::= { rc002 0 }

--
--	THIS TABLE DEFINE YOUR PRIVATE MIB INFORMATION
--


    rcftSystem OBJECT IDENTIFIER ::= { rcftMibObjects 1 }

    rcftChassis OBJECT IDENTIFIER ::= { rcftMibObjects 2 }

    rcftPower  OBJECT IDENTIFIER ::= { rcftMibObjects 3 }

    rcftFan    OBJECT IDENTIFIER ::= { rcftMibObjects 4 }

    rcftSlotStat OBJECT IDENTIFIER ::= { rcftMibObjects 5 }   
   
    rcftSysTrapTarget OBJECT IDENTIFIER ::= { rcftSystem 7}

    rcftSysTrapTargetEntry OBJECT IDENTIFIER ::= { rcftSysTrapTarget 1}

    rcftSysTrapPort OBJECT IDENTIFIER ::= { rcftSystem 9}

    rcftSysTrapPortEntry OBJECT IDENTIFIER ::= { rcftSysTrapPort 1}


    rcftSysId OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
        "
          设备编号
          Device ID
        "
        ::= {rcftSystem 1}

    rcftSysLevel OBJECT-TYPE
        SYNTAX INTEGER
        {   
            kernal(1),
            convergence(2),
            subordinate(3)
        }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
        "
          描述系统的级别. 1:核心,2:汇接,3:次要
          
        "
        ::= {rcftSystem 2}

    rcftSysChassisNum OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        " 
          实际在位的机箱数量     
        "
        ::= {rcftSystem 3}

    rcftSysAlarm OBJECT-TYPE
        SYNTAX INTEGER
        {   
            normal(1),
            minor(2),
            serious(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
          " 1:正常,2:轻微告警,包括风扇故障.LINK闪烁 
            3:严重告警,包括机箱温度过热.电源故障.背板监控失效"
        ::= {rcftSystem 4}

    rcftSysTmptAlarmThreshold OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
        " 
           机箱温度告警阈值.   
           The object is the temperature threshold of the chassis.
           When the chassis temperature is higer than the value of this object 
           a notification will be sent.
        "
        ::= {rcftSystem 5}

    rcftSysTrapEnable OBJECT-TYPE
        SYNTAX INTEGER
        {   
            enable(1),
            disable(2)
        }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
        "
          告警使能. 
          This object controls if the generation of
	   notifications is enabled. If the value of this object
	   is 'enable(1)', then all notifications are enabled; otherwise they are disabled.
        "
        ::= {rcftSystem 6}

    rcftTrapSink1 OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
          "
           接收告警的第一个服务器的IP地址.
           The value of this object is the first Ip Address of the first server to which
 	       the notification will be sent.
		 "
          ::={rcftSysTrapTargetEntry 1}

    rcftTrapSink2 OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
        " 
           接收告警的第二个服务器的IP地址.       
          The value of this object is the Ip Address of the second server to which
 	      the notification will be sent.
        "    
          ::={rcftSysTrapTargetEntry 2}

    rcftTrapSink3 OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
        " 
          接收告警的第三个服务器的IP地址.
          The value of this object is the Ip Address of the 3rd server to which
 	      the notification will be sent.
        "    
          ::={rcftSysTrapTargetEntry 3}

    rcftTrapSink4 OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
        "
          接收告警的第四个服务器的IP地址.
          The value of this object is the Ip Address of the 4th server to which
 	      the notification will be sent.
	"
         ::={rcftSysTrapTargetEntry 4}

    rcftTrapSink5 OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
        " 
          接收告警的第五个服务器的IP地址.
          The value of this object is the Ip Address of the 5th server to which
 	      the notification will be sent.
        "    
         ::={rcftSysTrapTargetEntry 5}

    rcftTrapSink6 OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
        " 
          接收告警的第六个服务器的IP地址.
          The value of this object is the Ip Address of the 6th server to which
 	      the notification will be sent.
        "    
         ::={rcftSysTrapTargetEntry 6}

    rcftTrapSink7 OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
        "
          接收告警的第七个服务器的IP地址. 
          The value of this object is the Ip Address of the 7th server to which
 	      the notification will be sent. 
        "    
         ::={rcftSysTrapTargetEntry 7}

    rcftTrapSink8 OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
        "
         接收告警的第八个服务器的IP地址. 
         The value of this object is the Ip Address of the 8th server to which
 	     the notification will be sent.
        "    
         ::={rcftSysTrapTargetEntry 8}

    rcftRSlotTmptAlarmThreshold OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
          " 
            远端机箱温度告警阈值. 
          "
        ::= {rcftSystem 8}


    rcftTrapPort1 OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
        " 
          接收告警的第一个服务器的端口号. 
          The value of this object is the first port of the first server to which
 	      the notification will be sent. 
        "    
          ::={rcftSysTrapPortEntry 1}

    rcftTrapPort2 OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
        " 
          接收告警的第二个服务器的端口号.
          The value of this object is the port of the first server to which
 	      the notification will be sent. 
        "    
          ::={rcftSysTrapPortEntry 2}

    rcftTrapPort3 OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
