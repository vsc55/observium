    }

    rcftInterfaceType OBJECT-TYPE
        SYNTAX INTEGER{
               ethport(1),
               audioChannel(2),
               e1port(3)
       }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
          " 
          	接口类型(1:电口,2:语音信道,3:E1口)
          "
        ::= {rcftInterfaceStatEntry 1}

    rcftInterfaceIndex OBJECT-TYPE
        SYNTAX INTEGER(1..16)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
          " 
          	接口索引(1-16)
          "
        ::= {rcftInterfaceStatEntry 2}

    rcftInterfaceRLink OBJECT-TYPE
        SYNTAX INTEGER
        {
            link-up(1),
            link-down(2)            
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
          "
          	远端接口LINK(1:link-up, 2:link-down)
          "
        ::= {rcftInterfaceStatEntry 3}

    rcftInterfaceRAutoNegotiation OBJECT-TYPE
        SYNTAX INTEGER
        {
            auto(1),
            manul(2)
        }
        MAX-ACCESS read-write		
        STATUS current
        DESCRIPTION
          "
          	远端接口速率协商状态(1:自协商,2:手动)
          "
        ::= {rcftInterfaceStatEntry 4}

    rcftInterfaceRDuplex OBJECT-TYPE
        SYNTAX INTEGER
        {
            full-duplex(1),
            half-duplex(2),
            auto(3)
        }
        MAX-ACCESS read-write		
        STATUS current
        DESCRIPTION
          "
          	远端接口双工状态(1:全双工,2:半双工,3:自动)
          "
        ::= {rcftInterfaceStatEntry 5}

    rcftInterfaceRSpeed OBJECT-TYPE
        SYNTAX INTEGER
        {
            rcft10Mbps(1),
            rcft100Mbps(2),
            rcft1000Mbps(3),
            rcft10Gbps(4),
            other(16)
