				INTEGER,
			rcftInterfaceRMDIXAuto
				INTEGER,
			rcftInterfaceRRecvRestrictSpeed
				INTEGER,
			rcftInterfaceRFlowCtrl
				INTEGER,
			rcftInterfaceRAudioType
				INTEGER,
