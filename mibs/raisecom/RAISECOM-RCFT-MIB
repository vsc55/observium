      	  STATUS current
      	  DESCRIPTION
      	  "
      	     局端模块的光模块类型
      	  "
      	  ::={rcftSlotStatEntry 158}     
      	        	                                             
       rcftSlotInformation OBJECT-TYPE
            SYNTAX OCTET STRING(SIZE (0..100))
          	MAX-ACCESS read-only
          	STATUS current
          	DESCRIPTION
          	"
          	    局端板卡的版本信息:版本信息的格式是:A.x-X.x-Ex-Fx.y 
                其中:A.x为硬件版本号;X.x为软件版本号;Ex中的x为以太网交换芯片类型;
                Fx.y中的x为FPGA芯片代号,y为FPGA代码版本号.
          	"     
          	::={rcftSlotStatEntry 159}
          	
     	
       	remoteOpticalModuleType OBJECT-TYPE  
       	SYNTAX INTEGER
      	  {   
      	     optical-M(1),
      	     optical-S1(2),
      	     optical-S2(3),
      	     optical-S3(4),
      	     optical-SS13(5), 
