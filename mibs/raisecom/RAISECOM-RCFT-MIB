            	INTEGER,
            rcftStmBncTxClkLos
            	INTEGER,
            rcftStmBncAnalogLos
            	INTEGER,
            rcftStmBncDigitalLos
            	INTEGER,
            rcftStmBncSyncLos
           		INTEGER
}

    rcftStmAdminStatus OBJECT-TYPE
        SYNTAX INTEGER
