				INTEGER, 
			rcftInterfaceRAudioOutSignalingSymbol
				INTEGER, 
			rcftInterfaceRAudioInSignalingType
				INTEGER, 
			rcftInterfaceRAudioOutSignalingType
				INTEGER,
			rcftInterfaceRAudioInSignalingStatus 
				INTEGER,
			rcftInterfaceRAudioOutSignalingStatus  
				INTEGER,
			rcftInterfaceRAudioUseEnable
				INTEGER,
			rcftInterfaceWANToLANFPEnWANDownLANPortStatus
				INTEGER,
