      	     optical-SS34(9),
      	     optical-SS35(10),
      	     optical-S15(12),   --kuanglili 20090826 S15枚举值为12
      	     optical-SFP(15),   --wangjunduo 20130508 SFP光模块类型枚举值为15
      	     optical-S1FC(50),
      	     optical-S1A(51),
      	     optical-S2A(52),
      	     optical-S3A(53),
      	     unknown-type(100)
      	  } 
