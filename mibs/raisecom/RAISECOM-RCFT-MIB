     ---以下是对CWDM MIB 的定义
            
     --**********************************--
     -- 这一部分是模块盘的配置和状态信息
     --**********************************--       
      	      	
      cwdmClientWorkSpeed OBJECT-TYPE
      	SYNTAX INTEGER
      	{
      	    stm16-C48(1),
      	    ge-Gb(2),
      	    stm4-C12(3),
      	    stm1-C3(4),
      	    fe(5),
      	    auto(6),
      	    reserved(7),
      	    speed2fc(8),
			speedfc(9),
			bypass (10),
			stm16-fec(11)
      	}  
      	MAX-ACCESS read-write
      	STATUS current
      	DESCRIPTION
      	"
      	    模块盘速率
      	"   
      	::={rcftSlotStatEntry 139}     
      	 
      
    --********************************--
    -- cwdm侧接口配置和工作状态信息
    --********************************--   	   
       
       cwdmCWDMWorkStatus OBJECT-TYPE
       	SYNTAX INTEGER
        MAX-ACCESS read-write  
        STATUS current
        DESCRIPTION
        "  
          	模块的本端与远端CWDM侧的状态信息.
  
                注:在对该对象进行配置操作时,在绑定该对象的同时,需要绑定针对某一槽位的板卡类型
                rcftSlotConfCardType,如果是对本端模块进行配置操作,需要本端的板卡类型赋给rcftSlotConfCardType;
                如果是对远端模块进行配置操作,需要将远端的板卡类型赋给rcftSlotConfCardType.
        "
        ::={rcftSlotStatEntry 140}    
        
       
       cwdmCWDMModuleMaxSpeed OBJECT-TYPE
       	 SYNTAX INTEGER
       	 MAX-ACCESS read-only  
       	 STATUS current
       	 DESCRIPTION
       	 "
       	   cwdm侧接口模块的最大速率
         "  
       	 ::={rcftSlotStatEntry 141}
       	 
       cwdmCWDMModuleTransLen OBJECT-TYPE
       	SYNTAX INTEGER
       	MAX-ACCESS read-only
       	STATUS current
       	DESCRIPTION
       	"
       	 cwdm侧SFP模块在单模光纤中的传输距离(单位为Km)
       	"	
       	::={rcftSlotStatEntry 142}
 
       cwdmCWDMModuleWaveLen OBJECT-TYPE
       	SYNTAX INTEGER
       	MAX-ACCESS read-only
       	STATUS current
       	DESCRIPTION
       	"
       	  cwdm侧SFP模块工作波长(单位为nm)
       	" 
       	::={rcftSlotStatEntry 143}

       cwdmCWDMModuleManufacturer  OBJECT-TYPE
       	SYNTAX OCTET STRING(SIZE (0..30))   
       	MAX-ACCESS read-only
       	STATUS current
       	DESCRIPTION
