    RcftStmStatEntry ::= SEQUENCE {

            rcftStmAdminStatus            
                INTEGER,
            rcftStmFiberPort
           	INTEGER,
            rcftStmFiberRxPllUnlck
            	INTEGER,
            rcftStmFiberTxClkLos
            	INTEGER,
            rcftStmFiberAnalogLos
            	INTEGER,
