	   		DESCRIPTION   
	   		"	    
	   		    从时钟选取的E1编号，范围：1-设备E1端口数
			"         
	   		::= {rcftSlotStatEntry 281}  
	   	
	   	rcftSlotRmcChannelType	 OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-write
	   		STATUS	current
	   		DESCRIPTION   
	   		"	    
	   		     收发器网管通道模式：1：802.3ah，2：rclink, 3:TS1000,	10:远端收发器网管禁止
			"         
	   		::= {rcftSlotStatEntry 282}
      
	   	rcftSlotApsE3SwitchDelay OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS read-write
	   		STATUS current
	   		DESCRIPTION
	   		"  
            	设备保护倒换时，E-3告警切换延时时间，以秒为单位
            	1～15，默认为5
	   		"
	   		::= { rcftSlotStatEntry 283 } 
	   		 
 	   	rcftSlotApsE6SwitchDelay OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS read-write
	   		STATUS current
	   		DESCRIPTION
	   		"  
            	设备保护倒换时，E-6告警切换延时时间，以秒为单位
            	1～255，默认为60
	   		"
	   		::= { rcftSlotStatEntry 284 }  
	   		
 	   	rcftE1DS1PortType OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS read-write
	   		STATUS current
	   		DESCRIPTION
	   		" 
	   			E1/DS1（T1）模式。	 0：E1，1：DS1（T1）
	   		"
	   		::= { rcftSlotStatEntry 285 }

 	   rcftSlotManageID OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS read-write
	   		STATUS current
	   		DESCRIPTION
	   		" 
	   			设备管理ID。	 1~255
	   		"
	   		::= { rcftSlotStatEntry 286 }   
	   		
 	   rcftSlotE1PortNumber OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS read-only
	   		STATUS current
	   		DESCRIPTION
	   		" 
	   			E1支路数量。	 1～32
	   		"
	   		::= { rcftSlotStatEntry 287 }

       rcftSlotQoS OBJECT-TYPE
       		SYNTAX INTEGER
       		MAX-ACCESS read-write
       		STATUS current
       		DESCRIPTION
       		"
       			 1：使能，2：禁止
       		"   
       		::= {rcftSlotStatEntry 288}    
       		
        rcftSlotTPIDRemark OBJECT-TYPE
       		SYNTAX INTEGER
       		MAX-ACCESS read-write
       		STATUS current
       		DESCRIPTION
       		"
       			 1：使能，2：禁止
       		"   
       		::= {rcftSlotStatEntry 289}  
       rcftRSlotTPIDRemark OBJECT-TYPE
       		SYNTAX INTEGER
       		MAX-ACCESS read-write
       		STATUS current
       		DESCRIPTION
       		"
       			 1：使能，2：禁止
       		"   
       		::= {rcftSlotStatEntry 290}   
       		
       rcftSlotDeviceMibUse OBJECT-TYPE
       		SYNTAX INTEGER
       		{
	      		mib002(1),
	      		rccomlib(2)
	      	}

       		MAX-ACCESS read-only
       		STATUS current
       		DESCRIPTION
       		"
       			 设备支持的mib类型
       			1:使用002mib
			2:使用组件库mib

       		"   
       		::= {rcftSlotStatEntry 291}       		
      rcftSlotApsSwitchDelay OBJECT-TYPE
	       SYNTAX INTEGER (0..255)    
	       MAX-ACCESS read-write
	       STATUS current
	       DESCRIPTION
	       "  
	   	    -- 设备保护倒换切换延迟时间，有损模式二下有效,以100ms为单位
	      	-- 缺省值为0，可以设置范围为0－60
	       "
	       ::= { rcftSlotStatEntry 292 }
	   		  		  	    
    rcftInterfaceStatTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RcftInterfaceStatEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
          " The table holding information related to the remote eth port status."
        ::= {rcftSlotStat 2}

    rcftInterfaceStatEntry OBJECT-TYPE
        SYNTAX RcftInterfaceStatEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
          "  "
        INDEX { rcftChassisIndex, rcftSlotIndex, rcftInterfaceType, rcftInterfaceIndex}
        ::= {rcftInterfaceStatTable 1}

    RcftInterfaceStatEntry ::= SEQUENCE {

           rcftInterfaceType
                INTEGER,
            rcftInterfaceIndex
                INTEGER,
            rcftInterfaceRLink
                INTEGER,
