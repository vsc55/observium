	     		当前15分钟误码秒告警门限。取值范围是0-255。   		
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 17 }  
	 	    
	 	               
	 	  rcftRemoteSHDSLPortLoopStatus	OBJECT-TYPE
	 	  	SYNTAX	INTEGER
	 	  	{
	 	  		insideLoop(1),
	 	  		outsideLoop(2),
	 	  		doubleloop(3),
	 	  		normal(100)	 	  		
	 	  	}             
	     	MAX-ACCESS	read-only
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口的环回状态:1：内环，2：外环，3：内环外环同时存在,100：正常		
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 18 }   
	 	      
	 	      
 	    rcftRemoteSHDSLPortAttenuationInitThreshhold	OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口的环路衰减初始门限值，单位为0.5dB。 	
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 19 } 
	 	    
		rcftRemoteSHDSLPortOrderTimeParameter OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS read-write
			STATUS	current
			DESCRIPTION
	  		 "
	  		    给远端模块G.口发送命令时所需要赋的时长参数。 
	  		    	
	  		    1: 测试时长1分钟
	  		    2: 测试时长2分钟
	  		    3: 测试时长4分钟
	  		    4: 测试时长8分钟
	  		    5: 测试时长15分钟
	  		    6: 测试时长30分钟
	  		    7: 测试时长60分钟  
	  		    	
	  		    18: 测试2帧
	  		    19: 测试5帧
	  		    20: 测试10帧
	  		    21: 测试20帧
	  		    22: 测试50帧
	  		    23: 测试100帧 		    	  		    			
	  		"                 
            ::= { rcftRemoteSHDSLPortEntry 20 }	    
            
      	rcftRemoteSHDSLPortOrderModeParameter OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS read-write
			STATUS	current
			DESCRIPTION
	  		 "
	  		    给远端模块G.口发送命令时所需要赋的模式参数。  	  		    			
	  		    
	  		"                 
            ::= { rcftRemoteSHDSLPortEntry 21 }            
            
		rcftRemoteSHDSLPortOrder	OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS	read-write
			STATUS	current
			DESCRIPTION
			"
			    1：硬复位
				2：软复位
				3：开始查询统计信息
				4：结束查询统计信息
				5：内环使能
				6：内环禁止
				7：外环使能
				8：外环禁止 
				9: 无环回
				10: 本地内环使能
				11: 本地外环使能
				12: 本地双向环使能
				13: 远端内环使能
				14: 远端外环使能
				15: 远端双向环使能
				16: 本地内环禁止
				17: 本地外环禁止
				18: 本地双向环禁止
				19: 远端内环禁止
				20: 远端外环禁止
				21: 远端双向环禁止
				22: 端口BERT测试开始
				23: 端口BERT测试结束
				
			"
		   ::= { rcftRemoteSHDSLPortEntry 22 }	 	      	 	  
	 	               
 	   	rcftRemoteSHDSLPortPBOAmount  OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
				PBO实际值，单位为dB。	     		
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 23 }   
	 	    
	    rcftRemoteSHDSLBertStatus		OBJECT-TYPE
	    	SYNTAX  INTEGER
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	  BIT3			BERT_STATUS		0:正常工作；	1:正在bert
	    	  BIT2-1		BERT_RESULT     00:未测试      01:测试正常；	10:测试失败
	    	  BIT0			BERT_TYPE   	0:误码率方式；  1:误码时间方式
	    	"
	    	::= {rcftRemoteSHDSLPortEntry 24}
	    	
	    rcftRemoteSHDSLBertTime		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	误码率方式:测试时间，时间随测试的进行而增长,单位 S.  		
	    	"				                           
	    	::= {rcftRemoteSHDSLPortEntry  25}
	    
	    rcftRemoteSHDSLBertErrCode		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	 误码率方式:误码数，单位是bit
	    	"	                         
	    	::= {rcftRemoteSHDSLPortEntry 26}
	    	
	    rcftRemoteSHDSLBertUnusedTime		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	 误码率方式:测试不可用时间，超过255秒后维持255秒不变，单位是S.
	    	"
	    	::= {rcftRemoteSHDSLPortEntry 27} 
	    	
	    rcftRemoteSHDSLBertPortSpeed		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	 误码率方式:测试的端口速率，单位：kbps。
	    	"
	    	::= {rcftRemoteSHDSLPortEntry 28}    
	    	
	    rcftRemoteSHDSLBertCodeType		OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-write
	   		STATUS	current
	   		DESCRIPTION
	   		"
	   		误码率方式:测试码型，最大值为127   
	   		0000001'b：2-15-1；
			其它：保留。

	   		"         
	   		::= {rcftRemoteSHDSLPortEntry 29}       		
	   		
	    rcftRemoteSHDSLBertCodeNum		OBJECT-TYPE
	   		SYNTAX INTEGER
	   		MAX-ACCESS	read-write
	   		STATUS	current
	   		DESCRIPTION
	   		"
	   		误码率方式:测试码数    
	   		0000001'b：2的31次方个码；
			0000010'b：2的28次方个码；
			0000011'b：2的25次方个码；
			0000100'b：2的21次方个码；
			1111111'b：测试不停；
			其它：保留。

	   		"         
	   		::= {rcftRemoteSHDSLPortEntry 30}  	                       

	   	rcftRemoteSHDSLLoopStatus  OBJECT-TYPE
       	SYNTAX	INTEGER
       	{
       		  localDoubleLoopEnable(1),
       		  localDoubleLoopDisable(2),
       		  remoteDoubleLoopEnable(3),
       		  remoteDoubleLoopDisable(4),
       		  normal(5),
       		  localInSideLoopEnale(6),
       		  localOutSideLoopEnable(7)
       		         		         		  
       	}
       	MAX-ACCESS read-only
       	STATUS	current
       	DESCRIPTION
       	"
       	     G.口的环回状态：1:本地环回，2：无本地环回，3：远端环回，4：无远端环回，
       	     5:无环回，
       	     6：本地内环使能，7：本地外环使能，8：本地双向环使能， 
       	     9:远端内环使能，10：远端外环使能，11：远端双向环使能，
       	     12：本地内环禁止，13:本地外环禁止，14：本地双向环禁止，
       	     15：远端内环禁止，16：远端外环禁止，17：远端双向环禁止，
       	"     
        ::= {rcftRemoteSHDSLPortEntry 31}  	   				 	    	 	               
	 	     
