   rcftRemoteVCGTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftRemoteVCGEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The VCG table.
       "
       ::= { rcftRemoteVCGObjects 1 }
   
    rcftRemoteVCGEntry OBJECT-TYPE
       SYNTAX  RcftRemoteVCGEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the VCG table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex, rcftRemoteDeviceIndex, rcftRemoteVCGIndex }
       ::= { rcftRemoteVCGTable 1 }
                 
                 
    RcftRemoteVCGEntry ::=
       SEQUENCE {
       rcftRemoteVCGIndex		 	Integer32,    
       rcftRemoteVCGStatus		INTEGER, 
       rcftRemoteVCGLoopStatus	INTEGER,  
       rcftRemoteVCGLcasXPR		INTEGER,
       rcftRemoteVCGLcasXAR		INTEGER,
       rcftRemoteVCGLcasXPT		INTEGER,
       rcftRemoteVCGLcasXAT		INTEGER,
       rcftRemoteVCGAlarmStatus	INTEGER,
       rcftRemoteVCGRxISPTPID		INTEGER,   
       rcftRemoteVCGTxISPTPID		INTEGER,
       rcftRemoteVCGBaseCoS 		INTEGER,
       rcftRemoteVCGVLANID		INTEGER,
       rcftRemoteVCGMemberList	OCTET STRING,     
       rcftRemoteVCGMemberStatus    OCTET STRING, 
       rcftRemoteVCGMemberRxCode    OCTET STRING, 
       rcftRemoteVCGMemberTxCode    OCTET STRING, 
       rcftRemoteVCGMemberAlarmStatus  	OCTET STRING,  
       rcftRemoteToLVCGMemberAlarmStatus  OCTET STRING,
       rcftRemoteToLVCGMemberAlarmStatus	OCTET STRING

                            
       }          
                                                 
       
    rcftRemoteVCGIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		VCG 的索引
    	"             
        ::= { rcftRemoteVCGEntry 1 }    
        
        rcftRemoteVCGStatus OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
        "   
        	bit10:CSF发送				1：使能，0：禁止                                   
        	bit6：是否带tag 				1：使能，0：禁止	
        	bit5：CheckLoop。				1：使能，0：禁止 
        	bit4：Fcs。						1：使能，0：禁止
        	bit3：GFP净荷扰码Scrb。			1：使能，0：禁止
        	bit2：VLI。						1：使能，0：禁止
        	bit1：Vcat。					1：使能，0：禁止
        	bit0：Lcas。					1：使能，0：禁止   
        	
        	LCAS on时，VCAT不可配（内部强制使能）。
        	只有LCAS off时，网管可配VCAT。
			VLI始终可配，但是只在LCAS off、VCAT 禁止时配置生效。      	
        "                                                  
        ::= {rcftRemoteVCGEntry 2}
        
        rcftRemoteVCGLoopStatus	OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
           0：无环，1~63：表示有环的VCG号
        " 
        ::= {rcftRemoteVCGEntry 3}
        rcftRemoteVCGLcasXPR  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
           	接收配置成员数（1-E1接口数目）
        " 
        ::= {rcftRemoteVCGEntry 4}   
        
        rcftRemoteVCGLcasXAR  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
           	接收实际成员数（1-E1接口数目）
        " 
        ::= {rcftRemoteVCGEntry 5}   
        
        rcftRemoteVCGLcasXPT  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
           	 发送配置成员数（1-E1接口数目）
        " 
        ::= {rcftRemoteVCGEntry 6}   
        
        rcftRemoteVCGLcasXAT  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
           	 发送实际成员数（1-E1接口数目）
        " 
        ::= {rcftRemoteVCGEntry 7}
        rcftRemoteVCGAlarmStatus	OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
           	 VCG 告警 
           	 bit0：GID  
           	 bit1：LOA
			 bit2：LFD
			 bit3：CFS
           	            	            	          	 
           	 LCAS ON时的告警                               
             bit4：TLCT
           	 bit5：PLCT	
           	 bit6：TLCR
			 bit7：PLCR	
			 
			 bit8：对端GID：
			 bit9：对端LOA
			 bit10：对端LFD
                            
        " 
        ::= {rcftRemoteVCGEntry 8}  
        
       rcftRemoteVCGRxISPTPID	OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current 
        DESCRIPTION
       	"
           	发送侧 TPID
       	" 
       	::= {rcftRemoteVCGEntry 9}    
       	
       rcftRemoteVCGTxISPTPID	OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current 
        DESCRIPTION
       	"
           	接收侧 TPID
       	" 
       	::= {rcftRemoteVCGEntry 10}  
       	
       	
       rcftRemoteVCGBaseCoS 		OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current 
        DESCRIPTION
       	"
           	COS：0~7
       	" 
       	::= {rcftRemoteVCGEntry 11}  
       	
       rcftRemoteVCGVLANID  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-write
        STATUS current 
        DESCRIPTION
       	"
           	VLANID
       	" 
       	::= {rcftRemoteVCGEntry 12}      
       	
       	rcftRemoteVCGMemberList  	OBJECT-TYPE
	  	SYNTAX	OCTET STRING(SIZE (0..15))
		MAX-ACCESS	read-write
		STATUS	current
		DESCRIPTION
    	"
    		E1成员列表，每个字节表示1个成员对应的E1端口号。
			此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
			每个字节为0表示该成员无效。   
		
    	"             
        ::= { rcftRemoteVCGEntry 13 }

       	
       	rcftRemoteVCGMemberStatus		OBJECT-TYPE
	  	SYNTAX	OCTET STRING(SIZE (0..15))
		MAX-ACCESS	read-write
		STATUS	current
		DESCRIPTION
    	"   
    		此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
			每个字节的：
    		bit0：成员的接收RX状态。1：OK， 0：Error。
			bit1：成员的发送TX状态。1：OK， 0：Error。
    	"             
        ::= { rcftRemoteVCGEntry 14 }
         
       rcftRemoteVCGMemberRxCode		OBJECT-TYPE
	  	SYNTAX	OCTET STRING(SIZE (0..15))
		MAX-ACCESS	read-only
		STATUS	current
		DESCRIPTION
    	"   
    		此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
			每个字节的枚举值如下：

    		VCG成员接收控制字：
    		1：FIXED  。固定带宽，不支持LCAS
    		2：ADD    。当前成员添加到VCG组中
    		3：NORM   。正常传送，当前成员已配置到VCG组中，且通道良好。
    		4：EOS    。该组中的最后一个成员
    		5：IDLE   。该成员没有配置到VCG组中
    		6：DNU	  。该成员是VCG成员，但是通道故障。	
      	"             
        ::= { rcftRemoteVCGEntry 15 }
          
       rcftRemoteVCGMemberTxCode		OBJECT-TYPE
	  	SYNTAX	OCTET STRING(SIZE (0..15))
		MAX-ACCESS	read-only
		STATUS	current
		DESCRIPTION
    	"    
    		此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
			每个字节的枚举值如下：

    		VCG成员发送控制字：
    		1：FIXED  。固定带宽，不支持LCAS
    		2：ADD    。当前成员添加到VCG组中
    		3：NORM   。正常传送，当前成员已配置到VCG组中，且通道良好。
    		4：EOS    。该组中的最后一个成员
    		5：IDLE   。该成员没有配置到VCG组中
    		6：DNU	  。该成员是VCG成员，但是通道故障。	
       	"             
        ::= { rcftRemoteVCGEntry 16 }
        
       rcftRemoteVCGMemberAlarmStatus	OBJECT-TYPE
	  	SYNTAX	OCTET STRING(SIZE (0..15))
		MAX-ACCESS	read-only
		STATUS	current
		DESCRIPTION
    	"   
    		成员告警状态，此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
			每个字节的bit位定义如下：

    		VCG 成员的告警
    		bit3：   LOA （与MND合并了）
    		bit2：   SQM
    		bit1：   CRC
    		bit0：   LOM
    	"             
        ::= { rcftRemoteVCGEntry 17 }
                                          
      rcftRemoteToLVCGMemberAlarmStatus   OBJECT-TYPE
	  	SYNTAX	OCTET STRING(SIZE (0..15))
		MAX-ACCESS	read-only
		STATUS	current
		DESCRIPTION
    	"   
    		对端VCG 成员告警状态，此节点为字节串，各成员之间有先后顺序，字节串从左到右依次表示1-N个成员。
			每个字节的bit位定义如下：

    		bit3：   LOA （与MND合并了）
    		bit2：   SQM
    		bit1：   CRC
    		bit0：   LOM
    	"             
        ::= { rcftRemoteVCGEntry 18 }
                                 
        
         --------------------------VCG GFP帧流量统计--------------------------   
         rcftRemoteVCGStatisticTable OBJECT-TYPE
	        SYNTAX SEQUENCE OF RcftRemoteVCGStatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	          局端设备GFP的统计表.
	          "
            ::= {rcftRemoteVCGPerformance 1}
	    rcftRemoteVCGStatisticEntry OBJECT-TYPE
	        SYNTAX RcftRemoteVCGStatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	             以rcftRemoteVCGIndex为索引
	          "
	        AUGMENTS { rcftRemoteVCGEntry }
	        ::= {rcftRemoteVCGStatisticTable 1}

	    RcftRemoteVCGStatisticEntry ::= SEQUENCE {
				rcftRemoteVCGRxClientPackets    	  Counter32,
				rcftRemoteVCGRxIdlePackets          Counter32,
				rcftRemoteVCGRxMgmntPackets		  	  Counter32,     
				rcftRemoteVCGRxFCSErrMgmntPackets     Counter32,
				rcftRemoteVCGRxLenErrPackets   	  Counter32,
				rcftRemoteVCGRxFCSErrClientPackets    Counter32,
				rcftRemoteVCGRxThecErrPackets     Counter32,
				rcftRemoteVCGRxEhecErrPackets     Counter32,
				rcftRemoteVCGRxCIDErrPackets      Counter32,
				rcftRemoteVCGRxSpareErrPackets    Counter32,
				rcftRemoteVCGRxChecCorPackets     Counter32,
				rcftRemoteVCGRxThecCorPackets     Counter32,
				rcftRemoteVCGRxEhecCorPackets     Counter32,
				rcftRemoteVCGRxBytes          	  Counter32,
				rcftRemoteVCGTxClientPackets        Counter32,
				rcftRemoteVCGTxIdlePackets        Counter32, 
				rcftRemoteVCGTxMgmntPackets       Counter32,
				rcftRemoteVCGTxBytes			  Counter32,
				rcftRemoteVCGFluxTimer		      Counter32		
	         }
					
          		rcftRemoteVCGRxClientPackets    OBJECT-TYPE
				SYNTAX Counter32
				MAX-ACCESS read-only
				STATUS current
				DESCRIPTION
				"
					接收客户数据包数			
				"
				::= { rcftRemoteVCGStatisticEntry 1 }    
				
