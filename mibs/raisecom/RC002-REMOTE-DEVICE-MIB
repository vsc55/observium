   --------------------------G.端口的对象---------------------------------    
 
    rcftRemoteSHDSLPortTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftRemoteSHDSLPortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The SDHSL PORT table.
       "
       ::= { rcftRemoteSHDSLPortObjects 1 }
   
    rcftRemoteSHDSLPortEntry OBJECT-TYPE
       SYNTAX  RcftRemoteSHDSLPortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the SHDSL PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,rcftRemoteDeviceIndex,rcftRemoteSHDSLPortIndex }
       ::= { rcftRemoteSHDSLPortTable 1 }
                 
                 
    RcftRemoteSHDSLPortEntry ::=
       SEQUENCE {
       rcftRemoteSHDSLPortIndex		 		Integer32,
       rcftRemoteSHDSLPortAlarmStatus		INTEGER,
       rcftRemoteSHDSLPortStatus			INTEGER, 
       rcftRemoteSHDSLPortCapableSpeed		INTEGER,
       rcftRemoteSHDSLPortWorkSpeed			INTEGER,
       rcftRemoteSHDSLPortProbeMaxSpeed		INTEGER,       
       rcftRemoteSHDSLPortProbeMinSpeed		INTEGER,   
       rcftRemoteSDHSLPortSNR				INTEGER,
       rcftRemoteSHDSLPortConfigSNR			INTEGER,
       rcftRemoteSHDSLPortSNRThreshold   	INTEGER,
       rcftRemoteSHDSLPortAttenuation 		INTEGER, 
       rcftRemoteSHDSLPortAttenuationThreshold		INTEGER, 
       rcftRemoteSHDSLPortPBO   			INTEGER, 
       rcftRemoteSHDSLPortLOSThreshold		INTEGER,
       rcftRemoteSHDSLPortLOSWThreshold		INTEGER,
       rcftRemoteSHDSLPortLOLKThreshold		INTEGER,
       rcftRemoteSHDSLPortESThreshold		INTEGER,
       rcftRemoteSHDSLPortLoopStatus		INTEGER,
       rcftRemoteSHDSLPortAttenuationInitThreshhold	INTEGER,
	   rcftRemoteSHDSLPortOrderTimeParameter		INTEGER,
	   rcftRemoteSHDSLPortOrderModeParameter		INTEGER,
	   rcftRemoteSHDSLPortOrder						INTEGER,
       rcftRemoteSHDSLPortPBOAmount   			INTEGER,
       rcftRemoteSHDSLBertStatus				INTEGER,
	   rcftRemoteSHDSLBertTime			    	Unsigned32, 
	   rcftRemoteSHDSLBertErrCode				Unsigned32,
	   rcftRemoteSHDSLBertUnusedTime			Unsigned32,
	   rcftRemoteSHDSLBertPortSpeed				Unsigned32,
	   rcftRemoteSHDSLBertCodeType			INTEGER,
	   rcftRemoteSHDSLBertCodeNum			INTEGER,
	   rcftRemoteSHDSLLoopStatus		INTEGER	   	         
		} 
           
    rcftRemoteSHDSLPortIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		G.口的索引
    	"             
        ::= { rcftRemoteSHDSLPortEntry 1 }   
        
     rcftRemoteSHDSLPortAlarmStatus   OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-only
     	STATUS		current
     	DESCRIPTION
     	"
     		G.口的告警状态。
     		
     		BIT5: SHDSL-LOS	SHDSL端口LOS时该位清0，否则置1。
            BIT4: SHDSL-LOSW	SHDSL端口LOSW时该位清0，否则置1。
            BIT3: SHDSL-Link	SHDSL端口为link up状态该位置1，否则清0。
            BIT2: 保留	固定为0'b。
            BIT1: FEC	SHDSL端口有FEC错误时该位清0，否则置1。
            BIT0: CRC	SHDSL端口有CRC错误时该位清0，否则置1。
     	"
 	    ::= { rcftRemoteSHDSLPortEntry 2 }   
 	    
 	    
      rcftRemoteSHDSLPortStatus   OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-write
     	STATUS		current
     	DESCRIPTION
     	"    
     		G.口的状态。   
			BIT23-22 IN_LBK-Test 00：没有进行过环回测试； 01：保留；10：内环测试成功；11：内环测试失败。
			BIT21-20 EX_LBK-Test 00：没有进行过环回测试； 01：保留；10：外环测试成功；11：外环测试失败。     
            BIT19-16 保留
     		BIT15-14 Port_En 	00：端口开；01：端口关；10：因故障转移关（无）；11：保留。
     		BIT13    LOC_FP_EN	1：使能本地xx端口故障向SHDSL口转移；  0：禁止。
			BIT12    REM_FP_EN	1：使能远端xx端口故障向SHDSL口转移；  0：禁止。    
			BIT11	 BOOT状态	0：BOOT失败；1：BOOT成功。 
			BIT10-9 保留  
			BIT8：	SHDSL-synClock:   0:同步时钟， 1：准同步时钟
			BIT7-6	编码方式	00'b：TC-4PAM； 01'b：TC-16PAM；10'b：TC-32PAM。11'b：保留。
			BIT5-4	SHDSL-Annex	SHDSL端口采用AnnexA时：01b；为AnnexB时：10b；为AnnexF时：11b。
			BIT3	SHDSL-PSD	SHDSL端口采用对称PSD时该位置1，非对称PSD时清0。
			BIT2	SHDSL-Port-En	SHDSL端口为打开状态该位置1，为关闭状态则清0。
			BIT1	SHDSL-LineProbe	SHDSL端口为线路探测使能方式该位置1；否则清0。网管不需要处理！
			BIT0	STU-C	SHDSL端口工作于STU-C模式时该位置1，否则清0。
     	"
 	    ::= { rcftRemoteSHDSLPortEntry 3 }   
	      
	  rcftRemoteSHDSLPortCapableSpeed	OBJECT-TYPE
	  	SYNTAX	INTEGER 
     	MAX-ACCESS	read-only
     	STATUS		current
     	DESCRIPTION
     	"
     		G.口最大可用速率,单位为64Kbps。
	  	"   
	    ::= { rcftRemoteSHDSLPortEntry 4 }    
	      
	      
	  rcftRemoteSHDSLPortWorkSpeed OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-only
     	STATUS		current
     	DESCRIPTION
     	"
     		G.口的工作速率,单位为64Kbps。     	
     	"
 	    ::= { rcftRemoteSHDSLPortEntry 5 }  	  
 	    
	  rcftRemoteSHDSLPortProbeMaxSpeed OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-write
     	STATUS		current
     	DESCRIPTION
     	"
     		G.口处于线路探测方式时配置的允许最高速率,单位为64Kbps。  	
     	"
 	    ::= { rcftRemoteSHDSLPortEntry 6 }  	  
 	    
	  rcftRemoteSHDSLPortProbeMinSpeed OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-write
     	STATUS		current
     	DESCRIPTION
     	"
     		G.口处于线路探测方式时配置的允许最低速率,单位为64Kbps。  	
     	"
 	    ::= { rcftRemoteSHDSLPortEntry 7 }   	    
 	    
 	    
 		rcftRemoteSDHSLPortSNR    OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-only
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口正常工作时的SNR（信噪比）,单位为0.5dB。是有符号数，有可能是负数。  	
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 8 }   	  
 	    
 		rcftRemoteSHDSLPortConfigSNR    OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口初始设置的SNR（信噪比）,单位为0.5dB。当linkup的瞬间G.口工作的SNR必须大于该
	     		初始值，之后rcftSDHSLPortSNR可以小于该初始值。是有符号数，有可能是负数。  	
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 9 }    	    
 	     
 	    rcftRemoteSHDSLPortSNRThreshold	OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口SNR（信噪比）的门限值,单位为0.5dB。
	     		当rcftSDHSLPortSNR小于该值时,AGENT发送告警。是有符号数，有可能是负数。  	
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 10 }    
	 	    
	 	    
 	     
 	    rcftRemoteSHDSLPortAttenuation	OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-only
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口的线路衰减值,单位为0.5dB。  	
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 11 }    
	 	    	    
 	     
 	    rcftRemoteSHDSLPortAttenuationThreshold	OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口线路衰减值的门限值,单位为0.5dB。当RcftRemoteSHDSLPortAttenuation小于该门限值时
	     		AGENT发送告警。  	
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 12 }    
	     
 	   	rcftRemoteSHDSLPortPBO  OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		G.口线路功率回退结果        
				BIT7	Valid	结果值有意义时为1，否则清0（0-6位无意义）。
				BIT6	PBO_AUTO	PBO自动设置时该位置1，PBO固定设置则清0。
				BIT5-4	保留	固定为00'b。
				BIT3-0	PBO值	PBO实际值，单位为dB。
	     		
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 13 }    
	 	    
	 	 rcftRemoteSHDSLPortLOSThreshold OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		当前15分钟LOS次数告警门限。取值范围是0-255。  		
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 14 }  
	 	    
	 	    
	 	 rcftRemoteSHDSLPortLOSWThreshold OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		当前15分钟LOSW次数告警门限。取值范围是0-255。  		
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 15 }   
	 	    
	 	    
	 	 rcftRemoteSHDSLPortLOLKThreshold OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-write
	     	STATUS		current
	     	DESCRIPTION
	     	"
	     		当前15分钟LOLK(linkdown)次数告警门限。取值范围是0-255。   		
	     	"
	 	    ::= { rcftRemoteSHDSLPortEntry 16 }  
	 	    
