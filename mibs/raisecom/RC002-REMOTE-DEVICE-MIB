    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		Data 口的索引
    	"             
        ::= { rcftRemoteDataPortEntry 1 }   
 
    rcftRemoteDataPortStatus	OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS read-write
    	STATUS		current
    	DESCRIPTION
    	"       		    	                                                       		
			BIT1    数据口有无信号			   	1：正常    	0：告警
			BIT0	数据口的开关状态	        0：关闭		1：打开
    	"
        ::= { rcftRemoteDataPortEntry 2 }   
    
    	rcftRemoteDataPortPosition OBJECT-TYPE
    	SYNTAX INTEGER
    	MAX-ACCESS read-only
    	STATUS current
    	DESCRIPTION 
    	"
    		数据口的位置信息：
    		bit7-4：盘
    		bit3-0：槽		
    	"             
    	::={ rcftRemoteDataPortEntry 3}
    		                 
       rcftRemoteDataPortType OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"       		    	                                                       
			数据口类型：
			1：232
			2：422
			3：485
			4：开关量			
    	"
        ::= { rcftRemoteDataPortEntry 4 } 
 	
 	
   
 	
