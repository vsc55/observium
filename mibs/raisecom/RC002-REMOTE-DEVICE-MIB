        rcftRemoteDeviceE1Table OBJECT-TYPE
              SYNTAX  SEQUENCE OF RcftRemoteDeviceE1Entry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                 "
                 The Remote Device System table.
                 "
             ::= { rcftRemoteDeviceE1Objects 1 }

   		rcftRemoteDeviceE1Entry OBJECT-TYPE
              SYNTAX  RcftRemoteDeviceE1Entry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                 "
                 An entry in the E1 config table.
                 索引使用对应的VC12的ifIndex
                 "
              INDEX   { rcftChassisIndex, rcftSlotIndex,rcftRemoteDeviceIndex,rcftRemoteE1Index }
             ::= { rcftRemoteDeviceE1Table 1 }
             
          RcftRemoteDeviceE1Entry ::=
              SEQUENCE {
              			rcftRemoteE1Index					Integer32,
          			    rcftRemoteE1BertEnable            	INTEGER,
						rcftRemoteE1ClockMode             	INTEGER,
          			    rcftRemoteE1FrameEnable			  	INTEGER,  
          			    rcftRemoteE1AlarmStatus			  	INTEGER, 
          			    rcftRemoteE1SubSpeed              	Unsigned32,          			    		
          			    rcftRemoteE1CRCDetectEnable		  	INTEGER,
          			    rcftRemoteE1ErrCodeSecCnt         	Counter32,
          			    rcftRemoteE1SErrCodeSecCnt        	Counter32,
          			    rcftRemoteE1TransErrorCode		  	INTEGER,
          			    rcftRemoteE1CRCStatus				INTEGER,
          			    rcftRemoteE1FaultPass				INTEGER,
          			    rcftRemoteE1LocalLoopEn				INTEGER, 
          			    rcftRemoteE1Location				INTEGER, 
                   		rcftRemoteE1FoundLink		       	INTEGER,
                   		rcftRemoteE1UnUsed		            INTEGER,
                   		rcftRemoteToLocalE1AlarmStatus		INTEGER,
                   		rcftRemoteE1Balance					INTEGER,
                   		rcftRemoteE1PortStatus				INTEGER,
                   		rcftRemoteE1PortTS0Mode				INTEGER,
                   		rcftRemoteE1PortIdleCode			INTEGER,
                   		rcftRemoteE1LoopStatus				INTEGER,
						rcftRemoteE1OrderTimeParameter		INTEGER,
						rcftRemoteE1OrderModeParameter		INTEGER,
						rcftRemoteE1Order					INTEGER,
						rcftRemoteE1PortType				INTEGER,
						rcftRemoteE1BertStatus				INTEGER,
						rcftRemoteE1BertTime				Unsigned32, 
						rcftRemoteE1BertErrCode				Unsigned32,
						rcftRemoteE1BertUnusedTime			Unsigned32,
						rcftRemoteE1BertPortSpeed			Unsigned32,
						rcftRemoteE1BertCodeType			INTEGER,
						rcftRemoteE1BertCodeNum				INTEGER,
                   		rcftRemoteE1LoopSwitchStatus		INTEGER,
                   		rcftRemoteE1AlarmRejest		INTEGER,
                   		rcftRemoteT1AlarmStatus	  INTEGER,
                   		rcftRemoteE1PortVCGNumber	INTEGER,
                   		rcftRemoteE1ToLNumber	INTEGER,
                   		rcftRemoteE1CVCnt		INTEGER									                   		         			             			             			    
                   }  
                   
         rcftRemoteE1Index OBJECT-TYPE
            SYNTAX      Integer32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
            "
          	  远端设备索引
            "
            ::= { rcftRemoteDeviceE1Entry 1 }          
                   
        rcftRemoteE1BertEnable OBJECT-TYPE
          SYNTAX  INTEGER {
                    enable (1),
                    disable (2)
                    }
          MAX-ACCESS  read-write
          STATUS  current
          DESCRIPTION
             "
               远端设备E1误码仪功能
             "
		  ::= { rcftRemoteDeviceE1Entry 2}
         
         
       rcftRemoteE1ClockMode	OBJECT-TYPE
       		SYNTAX INTEGER{
       		         master(1),
       		         secondary(2),
       		         transparent(3),
       		         e1received(4)
       		}
          MAX-ACCESS  read-write
          STATUS  current
          DESCRIPTION
             "
               远端设备E1的时钟模式      1:主	2：从	3：透传   4：E1接收
             "
		  ::= { rcftRemoteDeviceE1Entry 3}  
		  
		              
       rcftRemoteE1FrameEnable 		OBJECT-TYPE
       		SYNTAX INTEGER
	        {   
	              transparent(1),
	    	      pcm(2)
	        }
	        MAX-ACCESS read-write
	        STATUS current
	        DESCRIPTION
	          "
	          	远端设备E1口工作模式：（1：透明方式，2：成帧方式）.
	          "     
             ::= { rcftRemoteDeviceE1Entry 4}  
             
      rcftRemoteE1AlarmStatus		OBJECT-TYPE
      		SYNTAX	INTEGER
      		MAX-ACCESS		read-only   
      		STATUS			current
      		DESCRIPTION
      			"
      				远端设备E1端口的告警信息
      				--------------------------------------------------------------
				    | Bit15 | Bit14 | Bit13 | Bit12 | Bit11 | Bit10 |Bit9 | Bit8 | 
				    --------------------------------------------------------------   
				    ---------------------------------------------------------
				    | Bit7 | Bit6 | Bit5 | Bit4 | Bit3 | Bit2 | Bit1 | Bit0 |
				    --------------------------------------------------------- 
				     bit0:receive signal LOS
				     bit1:receive signal AIS
				     bit2:receive signal LOF 	
				     bit3:receive signal CRC
				     bit4:receive signal LOMF
				     bit5:receive signal CV  
				     bit6:  TSD   
				     
				     bit16:receive signal RLOS
				     bit17:receive signal RAIS
				     bit19:receive signal RLOF
				     bit21:receive signal RCRC
				     				     
				     每一位：0：告警，1：正常
      			"       
            ::= { rcftRemoteDeviceE1Entry 5}     
               
         
     	rcftRemoteE1SubSpeed   OBJECT-TYPE
     			SYNTAX Unsigned32
     			MAX-ACCESS	read-write    
          		STATUS  current
          		DESCRIPTION
               	"
                 	设置成帧E1的子速率
                 	每一比特代表相应的时隙
               	"
          	::= { rcftRemoteDeviceE1Entry 6}
               
               
		  rcftRemoteE1CRCDetectEnable  OBJECT-TYPE
	          SYNTAX  INTEGER {
	                    enable (1),
	                    disable (2)
	                    }
	          MAX-ACCESS  read-write
	          STATUS  current
	          DESCRIPTION
	                 "
	                 	远端设备E1端口CRC自适应(1:使能，2:禁止)
	                 "
			  ::= { rcftRemoteDeviceE1Entry 7 }

		 	   
		 rcftRemoteE1ErrCodeSecCnt  OBJECT-TYPE
		 		SYNTAX 		Counter32
		 		MAX-ACCESS	read-only
		 		STATUS		current
		 	    DESCRIPTION
		 	    		"
		 	    			远端设备E1端口的误码秒计数
		 	    		"
		 	    ::= { rcftRemoteDeviceE1Entry 8 }      
		 	    

		 rcftRemoteE1SErrCodeSecCnt  OBJECT-TYPE
		 		SYNTAX 		Counter32
		 		MAX-ACCESS	read-only
		 		STATUS		current
		 	    DESCRIPTION
		 	    		"
		 	    			远端设备E1端口的严重误码秒计数
		 	    		"
		 	    ::= { rcftRemoteDeviceE1Entry 9 }      
		  
            		     
        rcftRemoteE1TransErrorCode OBJECT-TYPE
        		SYNTAX INTEGER
		    	{
		    	   less10E-6(1),
		    	   more10E-6(2),
		    	   more10E-3(3)
		    	}
		    	MAX-ACCESS read-only
		    	STATUS current
		    	DESCRIPTION
		    	   "
		    	  	远端设备E1传输误码率(1:小于10E-6,2:大于10E-6,小于10E-3,3:大于10E-3).
		    	   "    
              ::= { rcftRemoteDeviceE1Entry 10 }     

        rcftRemoteE1CRCStatus	OBJECT-TYPE
        	  SYNTAX INTEGER
        	  {
        	       enable(1),
        	       disable(2)
        	  }
		      MAX-ACCESS read-only
		      STATUS current
		      DESCRIPTION
		      	"
		      		远端设备E1端口CRC状态
		      	"
			 ::= { rcftRemoteDeviceE1Entry 11 } 		  
	
	  rcftRemoteE1FaultPass	  OBJECT-TYPE
	  		SYNTAX	INTEGER
	  		{
	  			enable(1),
	  			disable(2)
	  		}	          
	  		MAX-ACCESS	read-write
	  		STATUS		current
	  		DESCRIPTION
	  		"
	  			远端设备E1端口故障转移功能(1:使能,2:禁止)	
	  		"
			::= { rcftRemoteDeviceE1Entry 12 } 			                                     
					                                     
       rcftRemoteE1LocalLoopEn  OBJECT-TYPE
           SYNTAX INTEGER
           {
               enable(1),
               disable(2)
           }
           MAX-ACCESS  read-only
           STATUS      current
           DESCRIPTION
           "
             	远端E1口工作在外环:(1:使能,2:不使能)
           "
           ::= { rcftRemoteDeviceE1Entry 13 }
           
       rcftRemoteE1Location  OBJECT-TYPE
           SYNTAX INTEGER
    	   {
    	       e1-1(1),
    	       e1-2(2),
    	       e1-3(3), 
    	       e1-4(4), 
    	       e1-5(5),
    	       e1-6(6),
    	       e1-7(7),
    	       e1-8(8),
    	       unknown(100)
    	   }
    	   MAX-ACCESS  read-write
    	   STATUS      current
    	   DESCRIPTION
    	   "
    	  	远端对应的远端是第几路E1口(1:E1-1,2:E1-2,3:E1-3,4:E1-4).
    	   "
           ::= { rcftRemoteDeviceE1Entry 14 }   
           
       rcftRemoteE1FoundLink OBJECT-TYPE
            SYNTAX INTEGER
            {
                success(1),
                failForDelay(2),
                failForOtherReason(100)
            }
            MAX-ACCESS  read-only
            STATUS      current
        	DESCRIPTION
         	 "
          		远端接口是否建链成功:(1:成功,2:失败)
         	 "
         ::= { rcftRemoteDeviceE1Entry 15 }  		  
		                                       
       rcftRemoteE1UnUsed OBJECT-TYPE
            SYNTAX INTEGER
            {
                 unused(1),
                 used(2)
            }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
             "
             	远端接口是否被使用:(1:没有,2:有)
             "
         ::= { rcftRemoteDeviceE1Entry 16 }  	
        
      rcftRemoteToLocalE1AlarmStatus OBJECT-TYPE
      		SYNTAX	INTEGER
      		MAX-ACCESS		read-only   
      		STATUS			current
      		DESCRIPTION
      			"
      				远端设备E1端口所接对端E1端口的告警信息
      				--------------------------------------------------------------
				    | Bit15 | Bit14 | Bit13 | Bit12 | Bit11 | Bit10 |Bit9 | Bit8 | 
				    --------------------------------------------------------------   
				    ---------------------------------------------------------
				    | Bit7 | Bit6 | Bit5 | Bit4 | Bit3 | Bit2 | Bit1 | Bit0 |
				    --------------------------------------------------------- 
				     bit0:receive signal LOS
				     bit1:receive signal AIS
				     bit2:receive signal LOF 	
				     bit3:receive signal CRC    
				     bit4:receive signal LOMF 
				     bit5:receive signal CV    
				     bit6: TSD, 信号恶化告警
				     bit7：RDI，对端缺陷告警。 
				     
					 bit16：对端LOS
					 bit17：对端AIS
				 	 bit19：对端LOF
					 bit20：对端LOMF
					 bit21：对端CRC
					 bit22：对端TSD
				     				     
				     每一位：0:告警，1：正常
      			"      			                          
		::= { rcftRemoteDeviceE1Entry 17 } 	 
		
		rcftRemoteE1Balance OBJECT-TYPE
			SYNTAX INTEGER
			{
				balance(1),
				imbalance(2)
			}	
      		MAX-ACCESS		read-only   
      		STATUS			current
      		DESCRIPTION
			"
				远端设备某一路E1的平衡标识
			"		
		::= { rcftRemoteDeviceE1Entry 18 } 	         
		
		
		rcftRemoteE1PortStatus	OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS	read-write
			STATUS		current
			DESCRIPTION
			"   
				BIT22 	网管通道模式（可与BIT19组合）     1：启用独立时隙；0：未启用独立时隙
				BIT21	E1环回检测结果	 0：无环   1：有环
				BIT20	E1环回检测		 0: 禁止	   1：使能

				BIT19    SABIT	  0：本地E1 未使用SA BIT； 1：本地E1 使用SA BIT；
				BIT18    E1协议版本       0：V2         1：V3     （网管不显示）
				BIT17	 响应对端跟随	  0：禁止		1：使能          
				BIT16	 对端跟随         0：无请求，	1：请求	，对端有请求后，E1时隙网管不可配
			    BIT14	 E1used:     1：E1口被使用； 0：E1口未被使用。				
			    BIT13	 CRC_EN:     1：CRC使能； 0：CRC禁止。	
				BIT12-11 IN_LBK-Test 00：没有进行过内环测试； 01：保留；10：内环测试成功；11：内环测试失败。
				BIT10-9  EX_LBK-Test 00：没有进行过环回测试； 01：保留；10：外环测试成功；11：外环测试失败。
				BIT8     IDLE_MODE	0：固定使用IDLE_CODE；  1：返回。 				
				BIT7-6	 Port_En 00：端口开；01：端口关；1x：保留。
				BIT5-4	 阻抗	00：75欧姆非平衡；01：120欧姆平衡；其它：保留。
				BIT3-2	 保留	固定00'b。	
				BIT1-0	 接口方式00：BNC； 01：RJ45插座；其它：保留。	
			
			"	
		::= { rcftRemoteDeviceE1Entry 19 } 	  
		
		rcftRemoteE1PortTS0Mode OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS	read-write
			STATUS		current
			DESCRIPTION
			"
				工作模式（TS0）：
				8：工作于透明模式，透传TS0-32，TS0=1；
				1：成帧模式0，发送端不发送TS0、接收端重新计算整个TS0； 
				9：成帧模式1，发送端透传TS0、接收端也使用透传来的TS0（IDLE CODE一致时才可以使用此模式）；
				2：成帧模式2，发送端不发送TS0、接收端返回TS0（CRC除外，重新计算）； 
				10：成帧模式3，发送端透传TS0、但接收端丢弃CRC使用重新计算的CRC；其它：保留。			
			"    		
		::= { rcftRemoteDeviceE1Entry 20 } 	
		
        rcftRemoteE1PortIdleCode	OBJECT-TYPE  
			SYNTAX	INTEGER
			MAX-ACCESS	read-write
			STATUS		current
			DESCRIPTION
			"
		       未用时隙的码字
			"    		
		::= { rcftRemoteDeviceE1Entry 21 } 	  
		
       rcftRemoteE1LoopStatus  OBJECT-TYPE
       	SYNTAX	INTEGER
       	{
       		  localDoubleLoopEnable(1),
       		  localDoubleLoopDisable(2),
       		  remoteDoubleLoopEnable(3),
       		  remoteDoubleLoopDisable(4)
       	}
       	MAX-ACCESS read-only
       	STATUS	current
       	DESCRIPTION
       	"
       	     E1的环回状态：1:本地环回，2：无本地环回，3：远端环回，4：无远端环回，
       	     5:无环回，
       	     6：本地内环使能，7：本地外环使能，8：本地双向环使能， 
       	     9:远端内环使能，10：远端外环使能，11：远端双向环使能，
       	     12：本地内环禁止，13:本地外环禁止，14：本地双向环禁止，
       	     15：远端内环禁止，16：远端外环禁止，17：远端双向环禁止，
       	"     
        ::= { rcftRemoteDeviceE1Entry 22 }  
        
        
		rcftRemoteE1OrderTimeParameter OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS read-write
			STATUS	current
			DESCRIPTION
	  		 "
	  		    给远端模块E1口发送命令时所需要赋的时长参数。 
	  		    	
	  		    1: 测试时长1分钟
	  		    2: 测试时长2分钟
	  		    3: 测试时长4分钟
	  		    4: 测试时长8分钟
	  		    5: 测试时长15分钟
	  		    6: 测试时长30分钟
	  		    7: 测试时长60分钟  
	  		    	
	  		    18: 测试2帧
	  		    19: 测试5帧
	  		    20: 测试10帧
	  		    21: 测试20帧
	  		    22: 测试50帧
	  		    23: 测试100帧 		    	  		    			
	  		"                 
            ::= { rcftRemoteDeviceE1Entry 23 }	    
            
      	rcftRemoteE1OrderModeParameter OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS read-write
			STATUS	current
			DESCRIPTION
	  		 "
	  		    给远端模块E1口发送命令时所需要赋的模式参数。  	  		    			
	  		    
	  		"                 
            ::= { rcftRemoteDeviceE1Entry 24 }            
            
		rcftRemoteE1Order	OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS	read-write
			STATUS	current
			DESCRIPTION
			"
			    1：硬复位
				2：软复位
				3：开始查询统计信息
				4：结束查询统计信息
				5：内环使能
				6：内环禁止
				7：外环使能
				8：外环禁止 
				9: 无环回
				10: 本地内环使能
				11: 本地外环使能
				12: 本地双向环使能
				13: 远端内环使能
				14: 远端外环使能
				15: 远端双向环使能
				16: 本地内环禁止
				17: 本地外环禁止
				18: 本地双向环禁止
				19: 远端内环禁止
				20: 远端外环禁止
				21: 远端双向环禁止
				22: 端口BERT测试开始
				23: 端口BERT测试结束	
				
			注意：rcftRemoteE1Order的枚举值要和rcftRemoteDeviceOrder始终保持完全一致。				
			"
		   ::= { rcftRemoteDeviceE1Entry 25 }	
        
		rcftRemoteE1PortType	OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS	read-only
			STATUS	current
			DESCRIPTION
			"
			    表示E1的类型。这个节点主要是针对约定四而增加，用来区分多种E1子块
			    下各个E1的具体类型。
			    
			    1：既可以为成帧，又可为非成帧（对应约定四中的0x10-0x04子块）
				2：非成帧（对应约定四中的0x10-0x06子块）				
			"
		   ::= { rcftRemoteDeviceE1Entry 26 }
		   			
	    rcftRemoteE1BertStatus		OBJECT-TYPE
	    	SYNTAX  INTEGER
	    	MAX-ACCESS	read-write
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	  BIT3			BERT_STATUS		0:正常工作；	1:正在bert
	    	  BIT2-1		BERT_RESULT     00:未测试    01:测试正常；	10:测试失败
	    	  BIT0			BERT_TYPE   	0:误码率方式；  1:误码时间方式
	    	"
	    	::= {rcftRemoteDeviceE1Entry 27}
	    	
	    rcftRemoteE1BertTime		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	误码率方式:测试时间，时间随测试的进行而增长，单位 S.		
	    	"				                           
	    	::= {rcftRemoteDeviceE1Entry  28}
	    
	    rcftRemoteE1BertErrCode		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	 误码率方式:误码数，单位是bit
	    	"	                         
	    	::= {rcftRemoteDeviceE1Entry 29}
	    	
	    rcftRemoteE1BertUnusedTime		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	 误码率方式:测试不可用时间，超过255秒后维持255秒不变，单位是S.
	    	"
	    	::= {rcftRemoteDeviceE1Entry 30} 
	    	
	    rcftRemoteE1BertPortSpeed		OBJECT-TYPE
	    	SYNTAX	Unsigned32
	    	MAX-ACCESS	read-only
	    	STATUS	current
	    	DESCRIPTION
	    	"
	    	 误码率方式:测试的端口速率，单位：kbps。
	    	"
	    	::= {rcftRemoteDeviceE1Entry 31}   
	    	
	   rcftRemoteE1BertCodeType		OBJECT-TYPE
	   		SYNTAX	 	INTEGER
	   		MAX-ACCESS	read-write
	   		STATUS	current
	   		DESCRIPTION
	   		"
	   		误码率方式:测试码型，最大值为127   
	   		 0000001'b：215-1；
			 其它：保留。

	   		"         
	   		::= {rcftRemoteDeviceE1Entry 32}    
	   		
	   	rcftRemoteE1BertCodeNum		OBJECT-TYPE
	   		SYNTAX	INTEGER
	   		MAX-ACCESS	read-write
	   		STATUS	current
	   		DESCRIPTION
	   		"
	   		误码率方式:测试码数,最大值为127    
	   		0000001'b：2的31次方个码；
			0000010'b：2的28次方个码；
			0000011'b：2的25次方个码；
			0000100'b：2的21次方个码；
			1111111'b：测试不停；
			其它：保留。

	   		"                              
	   		::= {rcftRemoteDeviceE1Entry 33}
	  
       rcftRemoteE1LoopSwitchStatus  OBJECT-TYPE
       	SYNTAX	INTEGER
       	MAX-ACCESS read-only
       	STATUS	current
       	DESCRIPTION
       	"
       	     E1的环回状态（是拨码的状态而非实际状态，通常不用）：
       	     1:本地环回，2：无本地环回，3：远端环回，4：无远端环回，
       	     5:无环回，
       	     6：本地内环使能，7：本地外环使能，8：本地双向环使能， 
       	     9:远端内环使能，10：远端外环使能，11：远端双向环使能，
       	     12：本地内环禁止，13:本地外环禁止，14：本地双向环禁止，
       	     15：远端内环禁止，16：远端外环禁止，17：远端双向环禁止，
       	"     
        ::= { rcftRemoteDeviceE1Entry 34 }    
        
        
