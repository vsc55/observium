        ::= { rcftRemoteDeviceE1Entry 39 }  
	    	                                                             
      rcftRemoteE1StatisticTable OBJECT-TYPE
	        SYNTAX SEQUENCE OF RcftRemoteE1StatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	          远端设备E1端口的统计表.
	          "
            ::= {rcftRemoteDeviceE1Objects 2}

	    rcftRemoteE1StatisticEntry OBJECT-TYPE
	        SYNTAX RcftRemoteE1StatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	            E1接口的性能统计表
	          "
	        AUGMENTS { rcftRemoteDeviceE1Entry }
	        ::= {rcftRemoteE1StatisticTable 1}

	    RcftRemoteE1StatisticEntry ::= SEQUENCE {
	           
				rcftRemoteE1TxPackets         Counter32, 
				rcftRemoteE1TxBytes           Counter32,
				rcftRemoteE1RxPackets         Counter32,
				rcftRemoteE1RxBytes           Counter32,
				rcftRemoteE1RxERRPackets      Counter32,
				rcftRemoteE1FluxTimer		  Counter32,
				rcftRemoteE1LANTxPackets	  Counter32,
				rcftRemoteE1LANRxPackets      Counter32,
				rcftRemoteE1LANRxLosPackets   Counter32

	         }
					
	 	rcftRemoteE1TxPackets OBJECT-TYPE
				SYNTAX Counter32
				MAX-ACCESS read-only
				STATUS current
				DESCRIPTION
					"
						远端设备E1端口发送以太网包数
					"
