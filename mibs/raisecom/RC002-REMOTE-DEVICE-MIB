   --------------------------V35端口的对象---------------------------------       
           
    rcftRemoteMoudleV35Table OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftRemoteMoudleV35Entry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The V35 PORT table.
       "
       ::= { rcftRemoteMoudleV35Objects 1 }
   
    rcftRemoteMoudleV35Entry OBJECT-TYPE
       SYNTAX  RcftRemoteMoudleV35Entry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the V35 PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,
       			 rcftRemoteDeviceIndex, rcftRemoteMoudleIndex,
       			 rcftRemoteMoudleV35Index }
       ::= { rcftRemoteMoudleV35Table 1 }
                 
                 
    RcftRemoteMoudleV35Entry ::=
       SEQUENCE {
       rcftRemoteMoudleV35Index		 	Integer32,
       rcftRemoteMoudleV35AlarmStatus	INTEGER,
       rcftRemoteMoudleV35Status		INTEGER            	   	         
       }                               
       
    rcftRemoteMoudleV35Index OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		V35口的索引
    	"             
        ::= { rcftRemoteMoudleV35Entry 1 }   
 
    rcftRemoteMoudleV35AlarmStatus	OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
			其它位	保留	固定000'b。 
			BIT11	RAIS    站在本地设备上看对端V35口所占那一路E1的AIS告警 1：正常 	0：告警  	
			BIT10	RCV     站在本地设备上看对端V35口所占那一路E1的CV告警  1：正常 	0：告警  
			BIT9	RLOF	站在本地设备上看对端V35口所占那一路E1的LOF告警 1：正常 	0：告警  
			BIT8	AIS     V35口所占那一路E1的AIS告警 1：正常 	0：告警  	   
			BIT7	CV      V35口所占那一路E1的CV告警  1：正常 	0：告警  
			BIT6	LOF	    V35口所占那一路E1的LOF告警 1：正常 	0：告警        
			BIT5	PATT		1：误码测试正常       0：误码测试告警 			
			BIT4    CRC_ALARM   1：正常 			  0：告警        
			         		
			BIT3	DCD_O		1：DCD信号UP；        0：DCD信号DOWN。
			BIT2	CTS_O		1：CTS信号UP；        0：CTS信号DOWN。
			BIT1	DTR_I		1：DTR信号UP；        0：DTR信号DOWN。
			BIT0	RTS_I		1：RTS信号UP；        0：RTS信号DOWN。
    	"
        ::= { rcftRemoteMoudleV35Entry 2 }   
    	
 	rcftRemoteMoudleV35Status     OBJECT-TYPE
 		SYNTAX	INTEGER
    	MAX-ACCESS read-write
    	STATUS		current
    	DESCRIPTION
    	"  
    		BIT15   DSR_O 		0：DSR信号up     1：DSR信号down  
			BIT14   DTR_I 		0：DTR信号up     1：DTR信号down   
			BIT13   RTS_I	 	0：RTS信号up     1：RTS信号down   
			BIT12	故障转移功能（E1到V35） 	0:禁止     1：使能
			BIT11   CTS_O 		0：CTS信号up    1：CTS信号down
			BIT10   DCD_O 		0：DCD信号up    1：DCD信号down
			BIT9	RD状态		0：禁止 1：使能
			BIT8	TD状态		0：禁止 1：使能
	
			BIT7~6  时钟方式		   11：主时钟  00：从时钟  
									   01：V35终端时钟  10：V35终端时钟且带宽跟随  	
			BIT5    时隙带宽控制	   0：从控； 1：主控    	
 			BIT4    ALOOP功能          0：禁止； 1：使能    	
 			BIT3    DLOOP功能  		   0：禁止； 1：使能    	                              
			BIT2    RLOOP功能          0：禁止； 1：使能    	
			BIT1    远端环回状态       0：未成功； 1：成功    
			BIT0    RX_CLK		       0：正相； 1：反相
    	"
        ::= { rcftRemoteMoudleV35Entry 3 }   
          	 			                              	 			                             	 			                               
    --------------------------V35端口的告警-------------------------- 
                                            	
     	
	  rcftRemoteAudioPortTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftRemoteAudioPortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The Audio PORT table.
       "
       ::= { rcftRemoteAudioPortObjects 1 }
   
    rcftRemoteAudioPortEntry OBJECT-TYPE
       SYNTAX  RcftRemoteAudioPortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the Audio PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,
        			rcftRemoteDeviceIndex,rcftRemoteAudioPortIndex }
       ::= { rcftRemoteAudioPortTable 1 }
                 
                 
    RcftRemoteAudioPortEntry ::=
       SEQUENCE {
       rcftRemoteAudioPortIndex		 	Integer32,
       rcftRemoteAudioPortStatus			INTEGER,
       rcftRemoteAudioPortPosition		INTEGER,
       rcftRemoteAudioPortType		INTEGER     
       }                               
       
    rcftRemoteAudioPortIndex OBJECT-TYPE
    	SYNTAX	 Integer32
    	MAX-ACCESS read-only
    	STATUS	current
    	DESCRIPTION
    	"
    		Audio口的索引
    	"             
        ::= { rcftRemoteAudioPortEntry 1 }   
 
    rcftRemoteAudioPortStatus OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS read-write
    	STATUS	current
    	DESCRIPTION
    	"       		    	                                                       		
			BIT1    LOS   音频口有无信号告警   	1：正常    	0：告警  
			BIT0	音频口的开关状态	        0：关闭		1：打开
    	"
        ::= { rcftRemoteAudioPortEntry 2 }   
        
      rcftRemoteAudioPortPosition OBJECT-TYPE
    	SYNTAX INTEGER
    	MAX-ACCESS read-only
    	STATUS current
    	DESCRIPTION 
    	"
    		音频口的位置信息：
    		bit7-4：盘
    		bit3-0：槽		
    	"             
    	::={ rcftRemoteAudioPortEntry 3}

    	                 
      rcftRemoteAudioPortType OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS read-only
    	STATUS	current
    	DESCRIPTION
    	"       		    	                                                       
			音频口的类型：
			1：电话用户端
			2：电话中继端
			3：高保真双向音频
			4：高保真音频编码
			5：高保真音频解码			
    	"
        ::= { rcftRemoteAudioPortEntry 4 }   
   	                                  
   
   
