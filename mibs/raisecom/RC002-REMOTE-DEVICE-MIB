	 	 	"
	 	 		G.口最近若干个24小时的性能统计表
	 	 	"  
 	 	    INDEX{ rcftChassisIndex, rcftSlotIndex,rcftRemoteDeviceIndex
 	 	    		,rcftRemoteSHDSLPortIndex,rcftRemoteSHDSLPortIntervalDayNumber }            
	 		::= { rcftRemoteSHDSLPortIntervalDayTable 1 }   
	 		
	 		
