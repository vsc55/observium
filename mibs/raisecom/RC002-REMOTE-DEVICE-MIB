       rcftRemoteEthFxModuleType  OBJECT-TYPE
      	  SYNTAX INTEGER
      	  {   
      	     optical-M(1),
      	     optical-S1(2),
      	     optical-S2(3),
      	     optical-S3(4),
      	     optical-SS13(5), 
      	     optical-SS15(6),
      	     optical-SS23(7),
      	     optical-SS25(8),
      	     --optical-SS3(9),    kuanglili 20090519 根据公司规范取消ss3光头类型 
      	     optical-SS34(9),    
      	     optical-SS35(10),
      	     optical-S15(12),   	--kuanglili 20090909 S15光模块的枚举为12  
      	     optical-SFP(15),
      	     unknown-type(100)
      	  }   
      	  MAX-ACCESS read-only
      	  STATUS current
      	  DESCRIPTION
      	  "
      	    远端设备以太网光口的光模块类型
      	  "
	      ::= { rcftRemoteEthFxPortEntry 10 }
		
