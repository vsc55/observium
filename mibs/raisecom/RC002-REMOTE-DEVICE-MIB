	     	MAX-ACCESS	read-only
	     	STATUS		current
	     	DESCRIPTION  
	     	"
	     		G.口当前24小时的LINK DOWN的次数
	     	"
            ::= { rcftRemoteSHDSLPortCurrentDayEntry 3 }
            
	 	rcftRemoteSHDSLPortCurrentDayCVTimes OBJECT-TYPE
	     	SYNTAX	INTEGER
	     	MAX-ACCESS	read-only
	     	STATUS		current
	     	DESCRIPTION  
	     	"
	     		G.口当前24小时的CV的次数
	     	"
            ::= { rcftRemoteSHDSLPortCurrentDayEntry 4 }     
            
            
