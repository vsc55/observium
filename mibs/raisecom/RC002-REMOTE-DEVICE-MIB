							以太网口发送以太网字节数
						"
					::= { rcftRemoteEthFeStatisticEntry 2 }
					
		 rcftRemoteEthFeRxPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							以太网口接收以太网包数
						"
					::= { rcftRemoteEthFeStatisticEntry 3 }
					
		 rcftRemoteEthFeRxBytes OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							以太网口接收以太网字节数
						"
					::= { rcftRemoteEthFeStatisticEntry 4 }
					
	    rcftRemoteEthFeRxLostPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							以太网电口接收以太网丢包数
						"
					::= { rcftRemoteEthFeStatisticEntry 5 }

        rcftRemoteEthFeFluxTimer	OBJECT-TYPE
        			SYNTAX		Counter32
        			MAX-ACCESS 	read-only
        			STATUS		current
        			DESCRIPTION
        				"
        					流量统计的时间
        				"			
                    ::= { rcftRemoteEthFeStatisticEntry 6 }

