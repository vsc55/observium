 -- 系统配置表
 		rcftRemoteDeviceSysTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF RcftRemoteDeviceSysEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                 "
                 The Remote Device System table.
                 "
             ::= { rcftRemoteDeviceSysObjects 1 }

    	rcftRemoteDeviceSysEntry OBJECT-TYPE
              SYNTAX  RcftRemoteDeviceSysEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                 "
                 An entry in the rc952 system table.
                
                 "
              INDEX   { rcftChassisIndex, rcftSlotIndex,rcftRemoteDeviceIndex }
             ::= { rcftRemoteDeviceSysTable 1 }

     	RcftRemoteDeviceSysEntry ::=
              SEQUENCE {
                   rcftRemoteDeviceIndex		 	Integer32,
                   rcftRemoteDeviceExist         	INTEGER,
                   rcftRemoteDeviceType    		 	INTEGER,
                   rcftRemoteDeviceLocalPortType	INTEGER,
                   rcftRemoteDeviceLocalPortIndex	INTEGER,
                   rcftRemoteDeviceVersionInfo	 	OCTET STRING,
                   rcftRemoteSysTemperature      	INTEGER,
                   rcftRemoteSysVoltageStatus      	INTEGER,
                   rcftRemoteDeviceWorkMode			INTEGER,  
                   rcftRemoteDeviceFrameLen			INTEGER, 
                   rcftRemoteDeviceOrder			INTEGER,
                   rcftRemoteDeviceConfigFlag		INTEGER, 
                   rcftRemoteSlotAutoCutErrLineEn   INTEGER,
                   rcftRemotePowerSupply			INTEGER,
                   rcftRemoteDevicePowerDown		INTEGER,
                   rcftRemoteDeviceClkMode			INTEGER,
                   rcftRemoteDeviceE1SubCardType	INTEGER,
                   rcftRemoteDeviceGateway			OCTET STRING,
                   rcftRemoteDeviceIP				OCTET STRING,
                   rcftRemoteDeviceSubnetMask		OCTET STRING,
                   rcftRemoteDeviceVLANID			INTEGER, 
                   rcftRemoteDeviceCommunityRW		INTEGER,
                   rcftRemoteDeviceCommunityLength	INTEGER,
                   rcftRemoteDeviceCommunity		OCTET STRING,
                   rcftRemoteDeviceVoltageValue		Unsigned32,
                   rcftRemoteDeviceStatus			INTEGER,
                   rcftRemoteSubModuleExist			INTEGER,
                   rcftRemoteMultiE1LoopOrder		OCTET STRING,
                   rcftRemoteOrderTimeParameter		INTEGER,
                   rcftRemoteOrderModeParameter		INTEGER,
                   rcftRemoteSDRAMBuf				INTEGER,
                   rcftRemoteRLPStatus				INTEGER,
                   rcftRemoteLALStatus				INTEGER,
                   rcftRemoteRALStatus				INTEGER, 
                   rcftRemoteDeviceSwitchStatus		INTEGER, 
                   rcftRemoteDeviceMoudleExist		INTEGER,
                   rcftRemoteCardInformation 		OCTET STRING,      
                   rcftRemoteSwitchType		INTEGER,
                   rcftRemoteConnectMode	INTEGER,
                   rcftRemoteQosEnable		INTEGER,
                   rcftRemoteBaseCOS		INTEGER,
                   rcftRemoteDSCP		OCTET STRING, 
                   rcftRemoteQueuesPolicy 	INTEGER,  
                   rcftRemoteMultiE1AlarmRejectOrder  OCTET STRING,
                   rcftRemoteT1PortPulseWaveForm  INTEGER, 
                   rcftRemoteT1PortCodeType	INTEGER,
                   rcftRemoteDeviceSabitMode		INTEGER,
                   rcftRemoteDeviceApsWaitToRestore INTEGER,
                   rcftRemoteDeviceCLKChannel	INTEGER,
                   rcftRemoteDeviceRmcChannelType	INTEGER,
                   rcftRemoteDeviceProductType	OCTET STRING,
                   rcftRemoteDeviceProtocolVer	INTEGER,
                   rcftRemoteDeviceVenderCode	INTEGER,
                   rcftRemoteDeviceModelID	INTEGER,
                   rcftRemoteE1PortNumber  INTEGER,
                   rcftRemoteDeviceVLANType	INTEGER,
                   rcftRemoteDeviceQoSPolicy	INTEGER,                                                                                                                      
                   rcftRemoteDeviceApsE3SwitchDelay  INTEGER,
                   rcftRemoteDeviceApsE6SwitchDelay  INTEGER,
                   rcftRemoteDeviceVLANTagDirection	 INTEGER, 
                   rcftRemoteDeviceVLANTagModule	INTEGER,
                   rcftRemoteDeviceISPTPID		INTEGER,
                   rcftRemoteE1DS1PortType		INTEGER,
                   rcftRemoteDeviceE1FrameChannel	INTEGER,
                   rcftRemoteDeviceManageID		INTEGER,
                   rcftRemoteDeviceMibUse		INTEGER	                   
                                                                                                                               
                   }


        rcftRemoteDeviceIndex OBJECT-TYPE
           SYNTAX      Integer32
           MAX-ACCESS  read-only
           STATUS      current
           DESCRIPTION
           "
          	远端设备的索引
           "
        	::= { rcftRemoteDeviceSysEntry 1 }
 			
 		rcftRemoteDeviceExist OBJECT-TYPE
            SYNTAX      INTEGER 
            {
          	  exist(1),
          	  noexist(2) 
            }
           MAX-ACCESS  read-only
           STATUS      current
           DESCRIPTION
           "
          	远端设备是否在位
           "
           ::= { rcftRemoteDeviceSysEntry 2 } 

        rcftRemoteDeviceType   OBJECT-TYPE
           SYNTAX      INTEGER
           {
               rcftTypeRCMS2802-240GE-REV-A-REMOTE(5394),   -- RC006设备，只能做rc002的一级远端，RCMS2802-120/240LGE-BL(B)作为局端
               rcftTypeRC501-FE-REV-C(20001),
               rcftTypeRC601-FE-REV-C(20002),
               rcftTypeRC511-FE-REV-A(20003),
               rcftTypeRC952-FEE1-REV-A(20004),
               rcftTypeRC952-FXE1-REV-A(20005),
               rcftTypeRC601-FE-REV-E(20006),
               rcftTypeRC511-4FE-REV-A(20007),
               rcftTypeRC511-FE-C-REV-A(20008),
               rcftTypeRC951-FEE1-REV-A(20009),
               rcftTypeRC513-FE-REV-A(20010), 
               rcftTypeRC513-FE-C-REV-A(20011),
               rcftTypeRC954-FE4E1-REV-A(20012),
               rcftTypeRC954-FX4E1-REV-A(20013),
               rcftTypeRC953-FE4E1-REV-A(20014),
               rcftTypeRC953-FX4E1-REV-A(20015),
