	---以下是远端设备系统的TRAP定义--
	
		rcftRemoteDevExistTrap	NOTIFICATION-TYPE
			OBJECTS{
				 rcftRemoteDeviceExist
			}
			STATUS 	 current
			DESCRIPTION
			"
			      远端板卡在位状态变化告警，当远端板卡由在位到丢失时发送告警，
			      远端板卡由丢失到恢复在位时发送告警恢复. 
 			"
		  	::={rcftRemoteDeviceSysTraps 1}
		  	
		rcftRemoteDevVoltTooHighTrap	NOTIFICATION-TYPE
			OBJECTS{   
			         rcftRemoteSysVoltageStatus
			}
			STATUS	current
			DESCRIPTION
			"
				远端设备电压超出阀值时发送该告警	
			"		  
            ::={rcftRemoteDeviceSysTraps 2}
     
		rcftRemoteDevVoltTooLowTrap	NOTIFICATION-TYPE
			OBJECTS{   
			         rcftRemoteSysVoltageStatus
			}
			STATUS	current
			DESCRIPTION
			"
				远端设备电压超出阀值时发送该告警	
			"		  
            ::={rcftRemoteDeviceSysTraps 3}  
            
   		rcftRemoteDevTmptTrap NOTIFICATION-TYPE
	       OBJECTS {
	
	          rcftRemoteSysTemperature
	       }
	       STATUS  current
	       DESCRIPTION
	       "
	           远端模块所在机箱温度告警及恢复：当远端模块所在机箱温度高于门限值时发送告警，
	           当恢复到门限值范围内时发送告警恢复。       
	       "
	       ::={ rcftRemoteDeviceSysTraps 4}   
	       
	      rcftRemoteDevPowerDownTrap NOTIFICATION-TYPE 
	      	OBJECTS{
				rcftRemoteDevicePowerDown	      	
	      	}
	       STATUS  current
	       DESCRIPTION
	       "
	           远端模块所接远端远端设备的掉电告警及恢复。 
	       "
	       ::={ rcftRemoteDeviceSysTraps 5}   

	      rcftRemoteDevPSChannelTrap NOTIFICATION-TYPE 
	      	OBJECTS{
				rcftRemoteDeviceStatus	      	
	      	}
	       STATUS  current
	       DESCRIPTION
	       "
	           主用通道切换到备用通道告警（没有恢复）。 
	       "
	       ::={ rcftRemoteDeviceSysTraps 6}  
	       	       
 	      rcftRemoteDevSPChannelTrap NOTIFICATION-TYPE 
	      	OBJECTS{
				rcftRemoteDeviceStatus	      	
	      	}
	       STATUS  current
	       DESCRIPTION
	       "
	           备用通道切换到主用通道告警（没有恢复）。 
	       "
	       ::={ rcftRemoteDeviceSysTraps 7}  
            
	---以下是远端设备电口的TRAP定义--
            
            
	    rcftRemoteEthFeLinkTrap NOTIFICATION-TYPE
	        OBJECTS {
	      
	           rcftRemoteEthFeLinkStatus
	        }
	        STATUS             current
	        DESCRIPTION
	        "
	           远端设备电口link状态变化告警及恢复.  
	           当电口link状态由up变down时发送告警，由down变up时发送告警恢复.  
	           The change of the link status of the eth port in the local module
	           will generate this trap.
	        "
	        ::= { rcftRemoteDeviceEthFeTraps 1 }  
	        
	---以下是远端设备光口的TRAP定义--
	        
	     rcftRemoteEthFxPortRLKTrap  NOTIFICATION-TYPE
				OBJECTS{
				          rcftRemoteEthFxPortRLK
				}         
	        STATUS             current
	        DESCRIPTION  
	        "
	          本模块光口接收方向的link状态(1：linkup，2：linkdown).   
	    	  The object indicating the status of the optical port 
	    	  in the receiving direction.
	    	  When the value is 1 the optical port link up,otherwise link down.
	        "
	        ::= { rcftRemoteDeviceEthFxTraps 1 }  
	        
	    rcftRemoteEthFxPortTLKTrap  NOTIFICATION-TYPE
				OBJECTS{
				          rcftRemoteEthFxPortTLK
				}         
	        STATUS             current
	        DESCRIPTION  
	        "
	          远端设备光口发送方向的link状态(1：linkup，2：linkdown).   
	    	  The object indicating the status of the optical port 
	    	  in the receiving direction.
	    	  When the value is 1 the optical port link up,otherwise link down.
	        "
	        ::= { rcftRemoteDeviceEthFxTraps 2 }  
	        
	    rcftRemoteEthFxPortTxPowerTrap NOTIFICATION-TYPE
	       OBJECTS {
	          rcftRemoteEthFxPortTxPowerAbnormal
	       }
	       STATUS     current
	       DESCRIPTION
	       "
	       	本端发送光功率变化告警.
	       "
	       ::={ rcftRemoteDeviceEthFxTraps 3 }
	
	   rcftRemoteEthFxPortRxSensitiveTrap NOTIFICATION-TYPE
	       OBJECTS {
	
	          rcftRemoteEthFxPortRxSensitiveAbnormal
	       }
	       STATUS     current
	       DESCRIPTION
	       "
	         本端接收灵敏度变化告警.
	       "
	       ::={ rcftRemoteDeviceEthFxTraps 4 }
	
	
	   rcftRemoteEthFxPortLaserTrap NOTIFICATION-TYPE
	       OBJECTS {
	
	          rcftRemoteEthFxPortLaserAbnormal
	       }
	       STATUS     current
	       DESCRIPTION
	       "
	         本端激光管工作状态变化告警.
	       "
	       ::={ rcftRemoteDeviceEthFxTraps 5 }
	
	
	   rcftRemoteEthFxPortSDTrap NOTIFICATION-TYPE
	       OBJECTS {
	          rcftRemoteEthFxPortSD
	       }
	       STATUS     current
	       DESCRIPTION
	       "
	         本地光模块SD输出信号异常告警和恢复. 当rcftSlotOSD的状态值发生变化时
	         发送该trap. 是告警还是恢复由绑定的对象rcftSlotOSD的状态值决定：
	         1: 恢复，  2：告警
	       "
	       ::={ rcftRemoteDeviceEthFxTraps 6 }  
	     
	    rcftRemoteEthFxPortLinkTrap  NOTIFICATION-TYPE
				OBJECTS{
				          rcftRemoteEthFxPortLink
				}         
	        STATUS             current
	        DESCRIPTION  
	        "
	          远端设备光口的link状态(1：linkup，2：linkdown).   
	    	  The object indicating the status of the optical port. 
	    	  When the value is 1 the optical port link up,otherwise link down.
	        "
	        ::= { rcftRemoteDeviceEthFxTraps 7 }  
	    
	   rcftRemoteEthFxPortExitTrap   NOTIFICATION-TYPE
			OBJECTS{
				          rcftRemoteEthFxPortExist 
				}         
	        STATUS             current
	        DESCRIPTION  
	        "
	          光模块在位trap。
	          1：在位，2：不在位
	        "
	        ::= { rcftRemoteDeviceEthFxTraps 8 }    
	        
