   --------------------------DS1端口的对象---------------------------------  
      
   rcftRemoteDS1PortTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftRemoteDS1PortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The DS1 PORT table.
       "
       ::= { rcftRemoteDS1PortObjects 1 }
   
    rcftRemoteDS1PortEntry OBJECT-TYPE
       SYNTAX  RcftRemoteDS1PortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the DS1 PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,
        			rcftRemoteDeviceIndex,rcftRemoteDS1PortIndex }
       ::= { rcftRemoteDS1PortTable 1 }
                 
                 
    RcftRemoteDS1PortEntry ::=
       SEQUENCE {
       rcftRemoteDS1PortIndex		 	Integer32,
       rcftRemoteDS1PortAlarmStatus	INTEGER,
       rcftRemoteDS1PortStatus		INTEGER, 
       rcftRemoteDS1PortESCont		INTEGER, 
       rcftRemoteDS1PortSESCont     INTEGER,
       rcftRemoteDS1PortBertStatus	INTEGER,
       rcftRemoteDS1PortFaultPass		INTEGER,
       rcftRemoteDS1PortLoopStatus	INTEGER,
       rcftRemoteDS1PortOrder			INTEGER,
       rcftRemoteDS1PortTranLength		INTEGER,
       rcftRemoteDS1PortFaultPassIndicator		INTEGER,  
       rcftRemoteDS1PortframeType	INTEGER,    
       rcftRemoteDS1PortChannel 	INTEGER
       
       }                               
       
    rcftRemoteDS1PortIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		DS1口的索引
    	"             
        ::= { rcftRemoteDS1PortEntry 1 }   
  
  
    rcftRemoteDS1PortAlarmStatus	OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"   
    		BIT22：从本地设备上看对端DS1电口的LOF
			BIT21：从本地设备上看对端DS1电口的CRC 
			BIT17：从本地设备上看对端DS1电口的LOS
			BIT16：从本地设备上看对端DS1电口的AIS 
		
			BIT8	RED						  1:正常    0：告警
    	 	BIT7    YEL   					  1:正常    0：告警	
			BIT6：LOF 接收信号帧失步告警
			BIT5：CRC CRC校验错告警 
			BIT4    LCV   编码冲突告警       1：正常    0：告警
			BIT3    DMO   发送信号无输出     1：正常    0：告警
			BIT1：LOS 接收信号丢失告警   
			BIT0：AIS 接收信号AIS 告警   
    	"
        ::= { rcftRemoteDS1PortEntry 2 }   
    	
    	                 
	rcftRemoteDS1PortStatus	OBJECT-TYPE
		SYNTAX	INTEGER
		MAX-ACCESS read-write
		STATUS	current
		DESCRIPTION
		"       
				BIT0：线路编码类型   1:B8ZS       0:AMI 
				BIT1: DS1是否成帧    1:成帧       0:非成帧     
				BIT2: DS1是否打开    1：打开      0：关闭
		"
		::= { rcftRemoteDS1PortEntry 3 }   
	
	rcftRemoteDS1PortESCont	  OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS	read-only
		STATUS  current
		DESCRIPTION
		"
			误码秒个数
		"             
        ::= { rcftRemoteDS1PortEntry 4 }    
    
    rcftRemoteDS1PortSESCont	  OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS	read-only
		STATUS  current
		DESCRIPTION
		"
			严重误码秒个数
		"             
        ::= { rcftRemoteDS1PortEntry 5 }  

	
	rcftRemoteDS1PortBertStatus	OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS	read-only
		STATUS	current
		DESCRIPTION
		"
		1: 误码仪使能
		2: 误码仪禁止
		"           
		::= { rcftRemoteDS1PortEntry 6 }  
     
     rcftRemoteDS1PortFaultPass   OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS	read-write
		STATUS	current
		DESCRIPTION 
		"
		1: 故障转移使能
		2: 故障转移禁止
		"
        ::= { rcftRemoteDS1PortEntry 7 }  
         	     
 	  rcftRemoteDS1PortLoopStatus  OBJECT-TYPE
		 SYNTAX  INTEGER
		 MAX-ACCESS	read-only
		 STATUS	current
		 DESCRIPTION 
		 "
		  	1: 本地环回，
			2：无本地环回，
			3：远端环回，
			4：无远端环回，
			5: 无环回，
			6：本地内环使能，
			7：本地外环使能，
			8：本地双向环使能， 
			9：远端内环使能，
			10：远端外环使能，
			11：远端双向环使能，
			12：本地内环禁止，
			13: 本地外环禁止，
			14：本地双向环禁止，
			5： 远端内环禁止，
			16：远端外环禁止，
			17：远端双向环禁止，
		 " 
         ::= { rcftRemoteDS1PortEntry 8 }  
         
         
      rcftRemoteDS1PortOrder  OBJECT-TYPE
		  SYNTAX  INTEGER
		  MAX-ACCESS	read-write
		  STATUS	current
		  DESCRIPTION  
		  "
		 		1：硬复位
				2：软复位
				3：开始查询统计信息
				4：结束查询统计信息
				5：内环使能
				6：内环禁止
				7：外环使能
				8：外环禁止
				9: 无环回
				10: 本地内环使能
				11: 本地外环使能
				12: 本地双向环使能
				13: 远端内环使能
				14: 远端外环使能
				15: 远端双向环使能
				16: 本地内环禁止
				17: 本地外环禁止
				18: 本地双向环禁止
				19: 远端内环禁止
				20: 远端外环禁止
				21: 远端双向环禁止
 				22: 端口BERT测试开始
 				23: 端口BERT测试结束
		" 
         ::= { rcftRemoteDS1PortEntry 9 }   
         
    rcftRemoteDS1PortTranLength   OBJECT-TYPE
		  SYNTAX  INTEGER
		  MAX-ACCESS	read-write
		  STATUS	current
		  DESCRIPTION     
		  "
		     DS1传输距离配置
		  "
         ::= { rcftRemoteDS1PortEntry 10 }   
         		  
     rcftRemoteDS1PortFaultPassIndicator OBJECT-TYPE
		  SYNTAX  INTEGER
		  MAX-ACCESS	read-only
		  STATUS	current
		  DESCRIPTION            
		  "
		     故障转移指示灯
		     １：灭，故障转移禁止
			 ２：绿常亮，故障转移使能，但没有故障发生
			 ３：黄闪亮，故障转移使能，且发生故障导致以太口关闭

		  "    
		  ::= { rcftRemoteDS1PortEntry 11 }  
 
     rcftRemoteDS1PortframeType	  OBJECT-TYPE
		  SYNTAX  INTEGER
		  MAX-ACCESS	read-write
		  STATUS	current
		  DESCRIPTION            
		  "
		     帧格式
			1：SF帧格式
			2：ESF
			3：DM
			4：SLC-96

		  " 
		  ::= { rcftRemoteDS1PortEntry 12 }  
  
     rcftRemoteDS1PortChannel	 OBJECT-TYPE
		  SYNTAX  INTEGER
		  MAX-ACCESS	read-write
		  STATUS	current
		  DESCRIPTION  
		  "
		     成帧时所用的时隙，bit0-bit30每位分别表示时隙１到31的占用情况。
		  "
		  ::= { rcftRemoteDS1PortEntry 13 }     

 
         
         
     --------------------------DS1端口的流量统计------------------------ 

        
         rcftRemoteDS1StatisticTable OBJECT-TYPE
	        SYNTAX SEQUENCE OF RcftRemoteDS1StatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	          局端设备DS1端口的统计表.
	          "
            ::= {rcftRemoteDS1PortPerformance 1}   
                       

	    rcftRemoteDS1StatisticEntry OBJECT-TYPE
	        SYNTAX RcftRemoteDS1StatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	             以rcftRemoteDS1Index为索引
	          "
	        AUGMENTS { rcftRemoteDS1PortEntry }
	        ::= {rcftRemoteDS1StatisticTable 1}

	    RcftRemoteDS1StatisticEntry ::= SEQUENCE {
				rcftRemoteDS1TxPackets       Counter32, 
				rcftRemoteDS1TxBytes           Counter32, 
				rcftRemoteDS1TxFailurePackets   Counter32,					
				rcftRemoteDS1RxPackets         Counter32,
				rcftRemoteDS1RxBytes           Counter32,
				rcftRemoteDS1RxErrorPackets    Counter32,
				rcftRemoteDS1FluxTimer	Counter32		
	         }
					

    	  rcftRemoteDS1TxPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS1口发送以太网包数
						"
					::= { rcftRemoteDS1StatisticEntry 1 }
		
		rcftRemoteDS1TxBytes OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS1口发送以太网字节数
						"
					::= { rcftRemoteDS1StatisticEntry 2 }   
					

	    rcftRemoteDS1TxFailurePackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS1电口发送以太网丢包数
						"
					::= { rcftRemoteDS1StatisticEntry 3 } 
										
		 rcftRemoteDS1RxPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS1口接收以太网包数
						"
					::= { rcftRemoteDS1StatisticEntry 4 }
					
		 rcftRemoteDS1RxBytes OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS1口接收以太网字节数
						"
					::= { rcftRemoteDS1StatisticEntry 5 }
					
	    rcftRemoteDS1RxErrorPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS1电口接收以太网丢包数
						"
					::= { rcftRemoteDS1StatisticEntry 6 }			
		
        rcftRemoteDS1FluxTimer	OBJECT-TYPE
        			SYNTAX		Counter32
        			MAX-ACCESS 	read-only
        			STATUS		current
        			DESCRIPTION
        				"
        					流量统计的时间
        				"			
                    ::= { rcftRemoteDS1StatisticEntry 7 } 

         
         
     --------------------------DS1端口的告警------------------------ 
                                       
       rcftRemoteDS1PortAISTrap	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteDS1PortAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		本端设备上DS1端口的接收信号AIS告警
     	"                
     	::= { rcftRemoteDS1PortTraps 1 }      
     	 		         	
                                       
       rcftRemoteDS1PortLOSTrap	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteDS1PortAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		本端设备上DS1端口的接收信号丢失告警
     	"                
     	::= { rcftRemoteDS1PortTraps 2 }      

       rcftRemoteDS1PortToLAISTrap	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteDS1PortAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		本端设备上DS1端口的接收对端信号AIS告警
     	"                
     	::= { rcftRemoteDS1PortTraps 3 }      
     	 		         	
                                       
       rcftRemoteDS1PortToLLOSTrap	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteDS1PortAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		本端设备上DS1端口的接收对端信号丢失告警
     	"                
     	::= { rcftRemoteDS1PortTraps 4 }                                   
      
	
		rcftRemoteDS1PortLOFTrap	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteDS1PortAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		本端设备上DS1端口的接收信号LOF告警
     	"                
     	::= { rcftRemoteDS1PortTraps 5 }      
     	 		         	
                                       
       rcftRemoteDS1PortCRCTrap	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteDS1PortAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		本端设备上DS1端口的接收信号CRC告警
