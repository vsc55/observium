   --------------------------子卡的对象---------------------------------  
   
     rcftRemoteMoudleTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftRemoteMoudleEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The EthFe PORT of Moudle table.
       "
       ::= { rcftRemoteMoudleObjects 1 }
   
    rcftRemoteMoudleEntry OBJECT-TYPE
       SYNTAX  RcftRemoteMoudleEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the EthFe PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,
       			 rcftRemoteDeviceIndex, rcftRemoteMoudleIndex }
       ::= { rcftRemoteMoudleTable 1 }
                 
                 
    RcftRemoteMoudleEntry ::=
       SEQUENCE {
       rcftRemoteMoudleIndex	Integer32, 
       rcftRemoteMoudleExist		INTEGER,           
       rcftRemoteMoudleType			INTEGER,       
       rcftRemoteMoudleStatus		INTEGER,  
	   rcftRemoteMoudleSigleChipDescr	OCTET STRING, 
	   rcftRemoteMoudleHardWareDescr	OCTET STRING,
	   rcftRemoteMoudleFPGADescr		OCTET STRING,
       rcftRemoteMoudleOrder			INTEGER,
       rcftRemoteMoudleIFOrder			OCTET STRING       	   	      
       }         
  
            
    rcftRemoteMoudleIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		EthFe口的索引
    	"             
        ::= { rcftRemoteMoudleEntry 1 }                
               
    rcftRemoteMoudleExist OBJECT-TYPE
        SYNTAX INTEGER
        {   
            exist(1),
            notExist(2)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
          "
          槽位板卡存在状态(1:存在,2:不存在).  
          "
        ::= {rcftRemoteMoudleEntry 2}
               
               
    rcftRemoteMoudleType OBJECT-TYPE
        SYNTAX      INTEGER
        {
           e-SUBM-2E1-A(6402),               
           e-SUBM-2FV35-A(6422),	
           e-SUBM-2FE-A(6423),     
           e-SUBM-FE4E1-A(6424),         
           e-SUBM-FV35-A(6425),	
           e-SUBM-FV35-B(6427),     
           e-SUBM-2FV35-B(6428)                                                                                       
        }    	
    	MAX-ACCESS	read-write
    	STATUS		current
    	DESCRIPTION
    	"   
   	       模块类型号
   	       6402:   E-SUBM-2E1-A 
   	       6422:   E-SUBM-2FV35-A 
   	       6423:   E-SUBM-2FE-A
   	       6424:   E-SUBM-FE4E1-A
   	       6425:   E-SUBM-FV35-A
   	       6427:   E-SUBM-FV35-B   	       
   	       6428:   E-SUBM-2FV35-B 	       
   	          	                           
   	       注意：为了网管实现的方便，子卡的设备类型号0x1901~0x19FE
   	       因此子卡的设备类型号，将在 6401~6654 之间。                    
   	                           
    	"      
 		::= { rcftRemoteMoudleEntry 3 }               
             
    rcftRemoteMoudleStatus OBJECT-TYPE
    	SYNTAX		INTEGER
    	MAX-ACCESS	read-write
    	STATUS		current
    	DESCRIPTION
    	"   
			BIT0    误码率超过10-3时，是否切断E1     0：不切断   1：切断   	
    	"      
 		::= { rcftRemoteMoudleEntry 4 }       	   
	         
    rcftRemoteMoudleSigleChipDescr OBJECT-TYPE
    	SYNTAX		OCTET STRING (SIZE (0..30))
    	MAX-ACCESS	read-only
    	STATUS		current
    	DESCRIPTION
    	"   
        A textual description of the siglechip program version.   	
    	"      
 		::= { rcftRemoteMoudleEntry 5 } 		            
 		            
    rcftRemoteMoudleHardWareDescr OBJECT-TYPE
    	SYNTAX		OCTET STRING (SIZE (0..30))
    	MAX-ACCESS	read-only
    	STATUS		current
    	DESCRIPTION
    	"   
        A textual description of the siglechip program version.    	
    	"      
 		::= { rcftRemoteMoudleEntry 6 }      
 		
    rcftRemoteMoudleFPGADescr OBJECT-TYPE
    	SYNTAX		OCTET STRING (SIZE (0..30))
    	MAX-ACCESS	read-only
    	STATUS		current
    	DESCRIPTION
    	"   
        A textual description of the FPGA program version.    	
    	"      
 		::= { rcftRemoteMoudleEntry 7 }  		
 		 			   
	rcftRemoteMoudleOrder	OBJECT-TYPE
	  	SYNTAX	INTEGER
		MAX-ACCESS	read-write
		STATUS	current
		DESCRIPTION
		"
			子卡的命令,属于子卡中模块级的命令.  
			
			1: 复位设备
			2: 复位以太网芯片 
			3: 流量统计查询开始
			4: 流量统计查询结束						
		" 	  	  
        ::={ rcftRemoteMoudleEntry 8}    		 			   
 		 		 
	rcftRemoteMoudleIFOrder	OBJECT-TYPE
	  	SYNTAX		OCTET STRING (SIZE (0..30))
		MAX-ACCESS	read-write
		STATUS	current
		DESCRIPTION
		"
			子卡的命令,属于子卡中接口级的命令.
			
			此命令节点采用字节块的方式下发,每一个字节表示一个端口.
			其中,每一个字节的数值表示如下:  
			
			0: 无环回
			1: 误码仪功能使能
			2: 误码仪功能禁止						
			3: 本地E1外环使能
			4: 本地E1内环使能 
			5: 本地V35内环使能
			6: 远端V35内环使能
			7: 本地E1外环禁止
							
		" 	  	  
        ::={ rcftRemoteMoudleEntry 9}  		 		 
 		 		 		 		 		 			         
 		      	 
   rcftRemoteMoudleExistTrap	NOTIFICATION-TYPE
      OBJECTS{
		rcftRemoteMoudleExist      		
      }	 
      STATUS current
     DESCRIPTION
     "
     	模块在位告警告警
     "                
     ::= { rcftRemoteMoudleTraps 1 }    		      	 
		      	          
   --------------------------以太网电口的对象-----------------------------       
    rcftRemoteMoudleEthFeTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftRemoteMoudleEthFeEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The EthFe PORT of Moudle table.
       "
       ::= { rcftRemoteMoudleEthFeObjects 1 }
   
    rcftRemoteMoudleEthFeEntry OBJECT-TYPE
       SYNTAX  RcftRemoteMoudleEthFeEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the EthFe PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,
       			 rcftRemoteDeviceIndex, rcftRemoteMoudleIndex,
       			 rcftRemoteMoudleEthFeIndex }
       ::= { rcftRemoteMoudleEthFeTable 1 }
                 
                 
    RcftRemoteMoudleEthFeEntry ::=
       SEQUENCE {
       rcftRemoteMoudleEthFeIndex	Integer32,   
       rcftRemoteMoudleEthFeStatus			INTEGER,
       rcftRemoteMoudleEthFeRxRestrictSpeed	INTEGER,
       rcftRemoteMoudleEthFeTxRestrictSpeed	INTEGER,
       rcftRemoteMoudleEthFeRestrictSpeedStep INTEGER,
       rcftRemoteMoudleEthFeAlarmStatus	INTEGER            
       }                               
       
    rcftRemoteMoudleEthFeIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		EthFe口的索引
    	"             
        ::= { rcftRemoteMoudleEthFeEntry 1 }                
             
    rcftRemoteMoudleEthFeStatus OBJECT-TYPE
    	SYNTAX		INTEGER
    	MAX-ACCESS	read-write
    	STATUS		current
    	DESCRIPTION
    	" 
    	    
    	    BIT6	以太网电口的流量控制 0：禁止；   1：使能
			BIT5	以太网电口的时钟方式 0：从时钟；   1：主时钟    	                    
			BIT4	以太网电口的自适应	 0：自适应关闭；   1：自适应打开    	
			BIT3	以太网电口的使能	 0：关闭；   1：打开
			BIT2	以太网电口的link状态 0：link down； 1：link up
			BIT1    以太网电口的速率   	 0：10M      1：100M    
			BIT0    以太网电口的双工     0：半双工   1：全双工      	
    	"      
 		::= { rcftRemoteMoudleEthFeEntry 2 }       
 		
 	rcftRemoteMoudleEthFeRxRestrictSpeed  OBJECT-TYPE
    	SYNTAX		INTEGER
    	MAX-ACCESS	read-write
    	STATUS		current
    	DESCRIPTION 
    	"
             接收带宽
    	"
 		::= { rcftRemoteMoudleEthFeEntry 3 }       
	
 	rcftRemoteMoudleEthFeTxRestrictSpeed  OBJECT-TYPE
    	SYNTAX		INTEGER
    	MAX-ACCESS	read-write
    	STATUS		current
    	DESCRIPTION 
    	"
           发送带宽
       	"
 		::= { rcftRemoteMoudleEthFeEntry 4 }    
 		
 	rcftRemoteMoudleEthFeRestrictSpeedStep	OBJECT-TYPE
 		SYNTAX	INTEGER
 		MAX-ACCESS	read-only
 		STATUS		current
 		DESCRIPTION
 		"
 			带宽粒度 
 			0x00：64Kbps；0x01：32Kbps（无）；其余：保留。 		
 		"   
 	    ::= { rcftRemoteMoudleEthFeEntry 5 }                                      
 
    rcftRemoteMoudleEthFeAlarmStatus	OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
			BIT0	本地以太网CRC错误告警		0：告警；        1：正常
    	"
        ::= { rcftRemoteMoudleEthFeEntry 6 }   	 	   	 	 	           
	 	           
    --------------------------以太网电口的流量统计-------------------------- 
    
         rcftRemoteMoudleEthFeStatisticTable OBJECT-TYPE
	        SYNTAX SEQUENCE OF RcftRemoteMoudleEthFeStatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	          局端设备Eth端口的统计表.
	          "
            ::= {rcftRemoteMoudleEthFePerformance 1}

	    rcftRemoteMoudleEthFeStatisticEntry OBJECT-TYPE
	        SYNTAX RcftRemoteMoudleEthFeStatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	             以rcftRemoteMoudleEthFeIndex为索引
	          "
	        AUGMENTS { rcftRemoteMoudleEthFeEntry }
	        ::= {rcftRemoteMoudleEthFeStatisticTable 1}

	    RcftRemoteMoudleEthFeStatisticEntry ::= SEQUENCE {
				rcftRemoteMoudleEthFeTxPackets         Counter32, 
				rcftRemoteMoudleEthFeTxBytes           Counter32, 
				rcftRemoteMoudleEthFeTxFailurePackets  Counter32,					
				rcftRemoteMoudleEthFeRxPackets         Counter32,
				rcftRemoteMoudleEthFeRxBytes           Counter32,
				rcftRemoteMoudleEthFeRxErrorPackets    Counter32,
				rcftRemoteMoudleEthFeFluxTimer		   Counter32		
	         }
					

    	  rcftRemoteMoudleEthFeTxPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							以太网口发送以太网包数
						"
					::= { rcftRemoteMoudleEthFeStatisticEntry 1 }
					
		 rcftRemoteMoudleEthFeTxBytes OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							以太网口发送以太网字节数
						"
					::= { rcftRemoteMoudleEthFeStatisticEntry 2 }   
					

	    rcftRemoteMoudleEthFeTxFailurePackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							以太网电口发送以太网丢包数
						"
					::= { rcftRemoteMoudleEthFeStatisticEntry 3 } 
										
		 rcftRemoteMoudleEthFeRxPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							以太网口接收以太网包数
						"
					::= { rcftRemoteMoudleEthFeStatisticEntry 4 }
					
		 rcftRemoteMoudleEthFeRxBytes OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							以太网口接收以太网字节数
						"
					::= { rcftRemoteMoudleEthFeStatisticEntry 5 }
					
	    rcftRemoteMoudleEthFeRxErrorPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							以太网电口接收以太网丢包数
						"
					::= { rcftRemoteMoudleEthFeStatisticEntry 6 }

        rcftRemoteMoudleEthFeFluxTimer	OBJECT-TYPE
        			SYNTAX		Counter32
        			MAX-ACCESS 	read-only
        			STATUS		current
        			DESCRIPTION
        				"
        					流量统计的时间
        				"			
                    ::= { rcftRemoteMoudleEthFeStatisticEntry 7 }

	                                       
    --------------------------以太网电口的告警-------------------------- 
                                       
       rcftRemoteMoudleEthFeLinkTrap	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteMoudleEthFeStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		局端设备上以太网电口的LINK告警
     	"                
     	::= { rcftRemoteMoudleEthFeTraps 1 }      
     	 		         	               
   --------------------------PDH端口的对象---------------------------------       
                                                                             
    rcftRemoteMoudlePdhTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftRemoteMoudlePdhEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The Pdh PORT table.
       "
       ::= { rcftRemoteMoudlePdhObjects 1 }
   
    rcftRemoteMoudlePdhEntry OBJECT-TYPE
       SYNTAX  RcftRemoteMoudlePdhEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the Pdh PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,
                 rcftRemoteDeviceIndex, rcftRemoteMoudleIndex,
       			 rcftRemoteMoudlePdhIndex }
       ::= { rcftRemoteMoudlePdhTable 1 }
                 
                 
    RcftRemoteMoudlePdhEntry ::=
       SEQUENCE {
       rcftRemoteMoudlePdhIndex		 	Integer32,   
       rcftRemoteMoudlePdhAlarmStatus		INTEGER,  
       rcftRemoteMoudlePdhStatus		 	INTEGER               
       }                               
       
    rcftRemoteMoudlePdhIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		Pdh口的索引
    	"             
        ::= { rcftRemoteMoudlePdhEntry 1 }   
        
     rcftRemoteMoudlePdhAlarmStatus OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS read-only
     	STATUS	current
    	DESCRIPTION
    	"
    		Pdh口的告警状态  
    		其他位：保留
			BIT20	从本地设备上查看到的对端设备掉电告警	 0：告警； 1：正常    		   
			BIT19	从本地设备上查看到的对端设备光口的E-6	 0：告警； 1：正常
			BIT18	从本地设备上查看到的对端设备光口的E-3	 0：告警； 1：正常
			BIT17	从本地设备上查看到的对端设备光口的LOF	 0：告警； 1：正常
			BIT16	从本地设备上查看到的对端设备光口的LOS	 0：告警； 1：正常  
			
			BIT3	E-6 pdh光口的E-6告警 0：告警； 1：正常
			BIT2	E-3	pdh光口的E-3告警 0：告警； 1：正常
			BIT1    LOF pdh光口LOF告警   0：告警   1：正常    
			BIT0    LOS pdh光口LOS告警   0：告警   1：正常    		
		"             
        ::= { rcftRemoteMoudlePdhEntry 2 }   
     	   
     rcftRemoteMoudlePdhStatus OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS read-write
     	STATUS	current
    	DESCRIPTION
    	"		
    	"             
        ::= { rcftRemoteMoudlePdhEntry 3 }  
        
   --------------------------PDH端口的告警---------------------------------     
    
    rcftRemoteMoudlePdhLOSTrap	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteMoudlePdhAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		局端设备上Pdh光口的LOS告警
     	"                
     	::= { rcftRemoteMoudlePdhTraps 1 }   
     	
     rcftRemoteMoudlePdhLOFTrap	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteMoudlePdhAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		局端设备上Pdh光口的LOF告警
     	"                
     	::= { rcftRemoteMoudlePdhTraps 2 }       	                              
     	 	                              
   --------------------------E1端口的对象---------------------------------       
           
    rcftRemoteMoudleE1Table OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftRemoteMoudleE1Entry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The E1 PORT table.
       "
       ::= { rcftRemoteMoudleE1Objects 1 }
   
    rcftRemoteMoudleE1Entry OBJECT-TYPE
       SYNTAX  RcftRemoteMoudleE1Entry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the E1 PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,
                 rcftRemoteDeviceIndex, rcftRemoteMoudleIndex,
       			 rcftRemoteMoudleE1Index }
       ::= { rcftRemoteMoudleE1Table 1 }
                 
                 
    RcftRemoteMoudleE1Entry ::=
       SEQUENCE {
       rcftRemoteMoudleE1Index		 	Integer32,
       rcftRemoteMoudleE1AlarmStatus	INTEGER,
       rcftRemoteMoudleE1Status			INTEGER,
       rcftRemoteMoudleE1TimeSlots		INTEGER,
       rcftRemoteMoudleE1TS0Mode		INTEGER,
       rcftRemoteMoudleE1LoopStatus		INTEGER,
       rcftRemoteMoudleE1ESCnt			INTEGER,
       rcftRemoteMoudleE1SESCnt			INTEGER                                     
       }                               
       
    rcftRemoteMoudleE1Index OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		E1口的索引
    	"             
        ::= { rcftRemoteMoudleE1Entry 1 }   
        
     rcftRemoteMoudleE1AlarmStatus OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS read-only
     	STATUS	current
    	DESCRIPTION
    	"
    		E1口的告警状态  
    		其他位：保留  
    		
    		BIT21  	从本地设备上看到的对端设备E1的CRC告警：0：告警； 1：正常。
    		BIT20  	从本地设备上看到的对端设备E1的LOMF告警：0：告警； 1：正常。
    		BIT19  	从本地设备上看到的对端设备E1的LOF告警：0：告警； 1：正常。
    		BIT18  	从本地设备上看到的对端设备E1的CV告警：0：告警； 1：正常。
    		BIT17  	从本地设备上看到的对端设备E1的AIS告警：0：告警； 1：正常。    		    		    		    		    		
    		BIT16  	从本地设备上看到的对端设备E1的LOS告警：0：告警； 1：正常。
	
    		
    		BIT5	CRC  0: 本地E1 CRC告警；   1：本地E1 CRC正常；
			BIT4	LOMF 0：本地E1 复帧失步；  1：本地E1 复帧正常。
			BIT3	LOF	 0：本地E1 帧失步；	   1：本地E1 帧正常。
			BIT2	CV	 0：本地HDB3 编码违例；1：本地HDB3 编码正常。			
			BIT1	AIS	 0：本地AIS 告警；     1：无本地AIS 告警。
			BIT0	LOS	 0：本地信号消失告警； 1：本地信号正常。
    	"             
        ::= { rcftRemoteMoudleE1Entry 2 }   
     	   
     rcftRemoteMoudleE1Status OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS read-write
     	STATUS	current
    	DESCRIPTION
    	"
    		E1口的状态    
    		其他位：保留  
    		                       
    		BIT14   本地E1口内环          0：禁止；        1：使能                       
    		BIT13   E1是否被网管禁止      0：正常；        1：禁止    		
    		BIT12   E1关断是否由延时引起  0：其他原因；    1：因为延时引起；    		
    		BIT11   E1口是否使用	 	  0：不使用；      1：使用；    		 
    		BIT10   E1口建链是否成功	  0：不成功；      1：成功； 
    		BIT9    对端的E1祯中是否包含CRC	  0：无CRC；      1：有CRC；    		        
    		BIT8    故障转移功能	 0：禁止；      1：使能；    		        
    		BIT7    是否使用SA bit	 0：不使用；    1：使用；     		
    		BIT6    CRC自协商结果	 0：禁止；      1：使能；    		
    		BIT5    误码测试功能	 0：禁止；      1：使能；
  			BIT4    误码测试结果	 0：有误码；      1：正常；    		    
  			BIT3-2  时钟方式      	 00：从时钟； 01：保留； 10：V35终端时钟 11：主时钟      		       
  			BIT1    时隙带宽控制     0：从控；      1：主控；    		
  			BIT0    OPEN             0：本地E1端口关闭；    1：本地E1端口打开； 
    	"             
        ::= { rcftRemoteMoudleE1Entry 3 }     
         			       
         			       
      rcftRemoteMoudleE1TimeSlots  OBJECT-TYPE
      	SYNTAX	INTEGER
      	MAX-ACCESS  read-write
      	STATUS	current
      	DESCRIPTION
      	"
      		E1口在成帧模式下所占用的时隙,bit0-bit30每位分别表示时隙1到时隙31的占用情况。
      		时隙0的占用与否是通过透明成帧模式判断的。
      	"
        ::= { rcftRemoteMoudleE1Entry 4 }    
        
      rcftRemoteMoudleE1TS0Mode  OBJECT-TYPE
      	SYNTAX  INTEGER
      	MAX-ACCESS read-write  
      	STATUS	current
      	DESCRIPTION
		"
			工作模式（TS0）：
				1：透明；
				2：成帧		
		"    
        ::= { rcftRemoteMoudleE1Entry 5 }            			       
         			                			       
      rcftRemoteMoudleE1LoopStatus  OBJECT-TYPE
       	SYNTAX	INTEGER
       	MAX-ACCESS read-only
       	STATUS	current
       	DESCRIPTION
       	"
       	     E1的环回状态：1:本地环回，2：无本地环回，3：远端环回，4：无远端环回，
						   5:无环回，
						   6：本地内环使能，7：本地外环使能，8：本地双向环使能， 
						   9:远端内环使能，10：远端外环使能，11：远端双向环使能，
						   12：本地内环禁止，13:本地外环禁止，14：本地双向环禁止，
						   15：远端内环禁止，16：远端外环禁止，17：远端双向环禁止，       	     			   
       	"     
        ::= { rcftRemoteMoudleE1Entry 6 }        
         			                        
      rcftRemoteMoudleE1ESCnt OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
        局端设备E1误码秒统计计数
        "
        ::= {rcftRemoteMoudleE1Entry 7}   

      rcftRemoteMoudleE1SESCnt OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "
        局端设备E1严重误码秒统计计数
        "
        ::= {rcftRemoteMoudleE1Entry 8}   
                                      	                                                                          
   --------------------------E1端口的流量统计---------------------------------  
                 			                                      
         rcftRemoteMoudleE1StatisticTable OBJECT-TYPE
	        SYNTAX SEQUENCE OF RcftRemoteMoudleE1StatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	          局端设备E1端口的统计表.
	          "
            ::= {rcftRemoteMoudleE1Performance 1}

	    rcftRemoteMoudleE1StatisticEntry OBJECT-TYPE
	        SYNTAX RcftRemoteMoudleE1StatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	             以rcftRemoteMoudleE1Index为索引
	          "
	        AUGMENTS { rcftRemoteMoudleE1Entry }
	        ::= {rcftRemoteMoudleE1StatisticTable 1}

	    RcftRemoteMoudleE1StatisticEntry ::= SEQUENCE {
				rcftRemoteMoudleE1TxPackets         Counter32, 
				rcftRemoteMoudleE1TxBytes           Counter32, 
				rcftRemoteMoudleE1TxFailurePackets  Counter32,					
				rcftRemoteMoudleE1RxPackets         Counter32,
				rcftRemoteMoudleE1RxBytes           Counter32,
				rcftRemoteMoudleE1RxErrorPackets    Counter32,
				rcftRemoteMoudleE1FluxTimer		    Counter32		
	         }
					

    	  rcftRemoteMoudleE1TxPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
