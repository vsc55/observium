-- remote card including RC952-EE1,RC952-FXE1,RC601,RC501,RC511


  RC002-REMOTE-DEVICE-MIB   DEFINITIONS ::= BEGIN
     
   IMPORTS                               
		IpAddress,
		OBJECT-TYPE, MODULE-IDENTITY, NOTIFICATION-TYPE, Integer32,Counter32,Unsigned32
        	FROM SNMPv2-SMI
		DisplayString,
		TimeStamp
			FROM SNMPv2-TC
    	rcftChassisIndex, rcftSlotIndex,rcftMibObjects 
       	FROM RAISECOM-RCFT-MIB;  
     
