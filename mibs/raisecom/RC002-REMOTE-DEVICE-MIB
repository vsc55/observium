     	rcftRemoteDS3E3PortToLOOFTrap	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteDS3E3PortAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		从本地设备上看对端设备电口OOF告警
     	"                
     	::= { rcftRemoteDS3E3PortTraps 18 } 
         
   --------------------------pdh端口的对象---------------------------------  
      
   rcftRemotePdhPortTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftRemotePdhPortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The Pdh PORT table.
       "
       ::= { rcftRemotePdhPortObjects 1 }
   
    rcftRemotePdhPortEntry OBJECT-TYPE
       SYNTAX  RcftRemotePdhPortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the Pdh PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,
        			rcftRemoteDeviceIndex,rcftRemotePdhPortIndex }
       ::= { rcftRemotePdhPortTable 1 }
                 
                 
    RcftRemotePdhPortEntry ::=
       SEQUENCE {
       rcftRemotePdhPortIndex		Integer32, 
       rcftRemotePdhPortModuleType	INTEGER,
       rcftRemotePdhPortAlarmStatus	INTEGER,
       rcftRemotePdhPortStatus		INTEGER,
       rcftRemotePdhPortECSCnt		INTEGER,
       rcftRemotePdhPortSECSCnt		INTEGER,
       rcftRemotePdhPortLoopStatus	INTEGER,
       rcftRemotePdhPortOrder		INTEGER,
       rcftRemotePdhPortBertStatus	INTEGER,
       rcftRemotePdhPortBertErrCode	Unsigned32       
       }                               
       
    rcftRemotePdhPortIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		Pdh口的索引
    	"             
        ::= { rcftRemotePdhPortEntry 1 }   
        
     rcftRemotePdhPortModuleType  OBJECT-TYPE
      	  SYNTAX INTEGER
      	  {   
      	     optical-M(1),
      	     optical-S1(2),
      	     optical-S2(3),
      	     optical-S3(4),
      	     optical-SS13(5), 
      	     optical-SS15(6),
      	     optical-SS23(7),
      	     optical-SS25(8),
      	     --optical-SS3(9),   kuanglili 20090519 根据公司规范取消ss3光头类型
      	     optical-SS34(9),
      	     optical-SS35(10),
      	     optical-S15(12),     --kuanglili 20090909 S15光模块的枚举为12
      	     optical-SFP(15),
      	     optical-SS24(23),       	     
 	     	 optical-S1FC(50), 
 	     	 optical-S1A(51), 
 	     	 optical-S2A(52), 
 	     	 optical-S3A(53),        	     
      	     unknown-type(100)
      	  }   
      	  MAX-ACCESS read-only
      	  STATUS current
      	  DESCRIPTION
      	  "
      	    远端设备pdh光口的光模块类型
      	  "
          ::= { rcftRemotePdhPortEntry 2 }   
  
    rcftRemotePdhPortAlarmStatus	OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"             
			BIT20	从本地设备上查看到的对端设备掉电告警	 0：告警； 1：正常    	
			BIT19	从本地设备上查看到的对端设备光口的E-6	 0：告警； 1：正常
			BIT18	从本地设备上查看到的对端设备光口的E-3	 0：告警； 1：正常
			BIT17	从本地设备上查看到的对端设备光口的LOF	 0：告警； 1：正常
			BIT16	从本地设备上查看到的对端设备光口的LOS	 0：告警； 1：正常  
			
			BIT3	E-6 pdh光口的E-6告警 0：告警； 1：正常
			BIT2	E-3	pdh光口的E-3告警 0：告警； 1：正常
			BIT1    LOF pdh光口LOF告警   0：告警   1：正常    
			BIT0    LOS pdh光口LOS告警   0：告警   1：正常    
    	"
        ::= { rcftRemotePdhPortEntry 3 }   
    	
    	                 
	rcftRemotePdhPortStatus	OBJECT-TYPE
		SYNTAX	INTEGER
		MAX-ACCESS read-write
		STATUS	current
		DESCRIPTION
		"    
			BIT10  光口故障转移           0：禁止   1：使能       
			BIT9   光口所接的对端设备的掉电告警	 0：告警； 1：正常		 
			BIT8   光口ALS开关    	0:禁止       1: 使能		
			BIT7-6 光口的环回状态 0：无环回，1：内环，2：外环 3：双向环     
			BIT5   光口ALS    	0:禁止       1: 使能  
			BIT4   从本地设备上查看到的对端设备光口的工作状态 0:不工作     1：工作  
			BIT3   光口的工作状态 0:不工作     1：工作  
			
			BIT2-1 光口因故障转移功能使能时的开关状态：
			       0：开
			       1：因电-光Faultpass功能使能时关闭
			       2：因光口ALS功能使能关闭
		    BIT0   光口的打开关闭：1:open, 0:close  
		"
		::= { rcftRemotePdhPortEntry 4 }  
		                                       
		                                       
     rcftRemotePdhPortECSCnt	OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-only
     	STATUS	current
     	DESCRIPTION
     	"
     		pdh光口误码秒
     	"   
     	::= { rcftRemotePdhPortEntry 5 }  
     	
     rcftRemotePdhPortSECSCnt	OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-only
     	STATUS	current
     	DESCRIPTION
     	"
     		pdh光口严重误码秒
     	"   
     	::= { rcftRemotePdhPortEntry 6 }  
    
    rcftRemotePdhPortLoopStatus   OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-only
     	STATUS	current
     	DESCRIPTION
     	"
     		环回状态
     		1:本地环回，2：无本地环回，3：远端环回，4：无远端环回，
       	     5:无环回，
       	     6：本地内环使能，7：本地外环使能，8：本地双向环使能， 
       	     9:远端内环使能，10：远端外环使能，11：远端双向环使能，
       	     12：本地内环禁止，13:本地外环禁止，14：本地双向环禁止，
       	     15：远端内环禁止，16：远端外环禁止，17：远端双向环禁止，

     	"   
     	::= { rcftRemotePdhPortEntry 7 }     
    
    rcftRemotePdhPortOrder  OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-write
     	STATUS	current
     	DESCRIPTION     
     	"
     			1：硬复位
				2：软复位
				3：开始查询统计信息
				4：结束查询统计信息
				5：内环使能
				6：内环禁止
				7：外环使能
				8：外环禁止 
				9: 无环回
				10: 本地内环使能
				11: 本地外环使能
				12: 本地双向环使能
				13: 远端内环使能
				14: 远端外环使能
				15: 远端双向环使能
				16: 本地内环禁止
				17: 本地外环禁止
				18: 本地双向环禁止
				19: 远端内环禁止
				20: 远端外环禁止
				21: 远端双向环禁止
				22: 端口BERT测试开始
				23: 端口BERT测试结束

     	"
        ::= { rcftRemotePdhPortEntry 8 } 
                                          
     rcftRemotePdhPortBertStatus  OBJECT-TYPE
     	SYNTAX	INTEGER
     	MAX-ACCESS	read-only
     	STATUS	current
     	DESCRIPTION 
     	"
     	   1: 误码仪使能
		   2: 误码仪禁止

     	"    
        ::= { rcftRemotePdhPortEntry 9 } 

	 rcftRemotePdhPortBertErrCode		OBJECT-TYPE
	    SYNTAX	Unsigned32
	    MAX-ACCESS	read-only
	    STATUS	current
	    DESCRIPTION
	    "
	      误码率方式:误码数，单位是bit
	    "	                         
	    ::= { rcftRemotePdhPortEntry 10}
		         	
    
