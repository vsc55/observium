            ::= {rcftRemoteDeviceEthFxObjects 2}

	    rcftRemoteEthFxStatisticEntry OBJECT-TYPE
	        SYNTAX RcftRemoteEthFxStatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	             以rcftRemoteEthFxIndex为索引
	          "
	        AUGMENTS { rcftRemoteEthFxPortEntry }
	        ::= {rcftRemoteEthFxStatisticTable 1}

	    RcftRemoteEthFxStatisticEntry ::= SEQUENCE {
	           
				rcftRemoteEthFxTxPackets         Counter32, 
				rcftRemoteEthFxTxBytes           Counter32,
				rcftRemoteEthFxRxPackets         Counter32,
				rcftRemoteEthFxRxBytes           Counter32,
				rcftRemoteEthFxRxLostPackets     Counter32,
				rcftRemoteEthFxFluxTimer		 Counter32, 
				rcftRemoteEthFxTxLostPackets     Counter32,
				rcftRemoteEthFx64TxBytes         Counter64,
				rcftRemoteEthFx64RxBytes         Counter64
				
				
	         }
					

    	  rcftRemoteEthFxTxPackets OBJECT-TYPE
					SYNTAX Counter32
