	     rcftRemoteEthFePortConfTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF RcftRemoteEthFePortConfEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
              "
                 The Remote Device Fe Port config table.
              "
             ::= { rcftRemoteDeviceEthFeObjects 3 }
             
   		rcftRemoteEthFePortConfEntry OBJECT-TYPE
              SYNTAX  RcftRemoteEthFePortConfEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
              "
                 An entry in the Fe Port config table.
              "
              INDEX   { rcftChassisIndex, rcftSlotIndex,rcftRemoteDeviceIndex,  rcftRemoteEthFeIndex }
             ::= { rcftRemoteEthFePortConfTable 1 }
             
        RcftRemoteEthFePortConfEntry ::=
          	  SEQUENCE {
          			    rcftRemoteEthFeConfSpeed       INTEGER,
						rcftRemoteEthFeConfDuplex      INTEGER
					} 
					
		 rcftRemoteEthFeConfSpeed OBJECT-TYPE
		       SYNTAX INTEGER
		        {   
		            rcft10Mbps(1),
		            rcft100Mbps(2),
		            rcft1000Mbps(3),
		            rcft10Gbps(4),
		            other(16)
		        }
		        MAX-ACCESS read-write
		        STATUS current
		        DESCRIPTION
		          "
		            远端设备电口的速率配置信息.  
		            The speed of the electronic port. 
		            1、10Mbps
		            2、100Mbps
		            3、1000Mbps
		            4、10Gbps
		            5、other       
		          "					
		          ::= { rcftRemoteEthFePortConfEntry 1 }
					
					
		 rcftRemoteEthFeConfDuplex OBJECT-TYPE
		        SYNTAX INTEGER
		        {   
		            full-duplex(1),
		            half-duplex(2)
		        }
		        MAX-ACCESS read-write
		        STATUS current
		        DESCRIPTION
		          "
		          远端设备电口双工模式配置信息（1:全双工，2:半双工）.   
		          The duplex mode of the electronic port.  
		          1:full duplex
		          2:half duplex      
		          "					
		         ::= { rcftRemoteEthFePortConfEntry 2 }    
