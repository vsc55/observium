     	rcftRemoteDeviceSystemMIB  OBJECT IDENTIFIER ::= { rcftRemoteDeviceMib 1 }	 
    	rcftRemoteDeviceSysObjects OBJECT IDENTIFIER ::= { rcftRemoteDeviceSystemMIB 1 }   
    	rcftRemoteDeviceSysTraps   OBJECT IDENTIFIER	::={ rcftRemoteDeviceSystemMIB 2}
    	
      	rcftRemoteDeviceEthMIB     OBJECT IDENTIFIER ::= { rcftRemoteDeviceMib 2 }     
      	rcftRemoteDeviceEthFeMIB		OBJECT IDENTIFIER ::= {rcftRemoteDeviceEthMIB 1}   
      	rcftRemoteDeviceEthFeObjects	OBJECT IDENTIFIER ::={rcftRemoteDeviceEthFeMIB 1} 
      	rcftRemoteDeviceEthFeTraps		OBJECT IDENTIFIER ::={rcftRemoteDeviceEthFeMIB 2}    
      	
      	rcftRemoteDeviceEthFxMIB		OBJECT IDENTIFIER ::= {rcftRemoteDeviceEthMIB 2}      
      	rcftRemoteDeviceEthFxObjects	OBJECT IDENTIFIER ::={rcftRemoteDeviceEthFxMIB 1} 
      	rcftRemoteDeviceEthFxTraps		OBJECT IDENTIFIER ::={rcftRemoteDeviceEthFxMIB 2} 
      	rcftRemoteDeviceEthFxPerformance	OBJECT IDENTIFIER ::={rcftRemoteDeviceEthFxMIB 3}
      	      	
      	rcftRemoteDeviceE1MIB       OBJECT IDENTIFIER ::= { rcftRemoteDeviceMib 3 } 
      	rcftRemoteDeviceE1Objects	OBJECT IDENTIFIER	::={rcftRemoteDeviceE1MIB 1}   
      	rcftRemoteDeviceE1Traps		OBJECT IDENTIFIER	::={rcftRemoteDeviceE1MIB 2}
      
      	rcftRemoteDeviceSHDSLMIB       	OBJECT IDENTIFIER ::= { rcftRemoteDeviceMib 4 } 
      	rcftRemoteSHDSLPortObjects		OBJECT IDENTIFIER	::={rcftRemoteDeviceSHDSLMIB 1}  
      	rcftRemoteSHDSLPortPerformance  OBJECT IDENTIFIER	::={rcftRemoteDeviceSHDSLMIB 2} 
      	rcftRemoteSHDSLPortTraps		OBJECT IDENTIFIER	::={rcftRemoteDeviceSHDSLMIB 10}
      
      	rcftRemoteDeviceV35MIB       	OBJECT IDENTIFIER ::= { rcftRemoteDeviceMib 5 } 
      	rcftRemoteV35PortObjects		OBJECT IDENTIFIER	::={rcftRemoteDeviceV35MIB 1}  
      	rcftRemoteV35PortPerformance  OBJECT IDENTIFIER	::={rcftRemoteDeviceV35MIB 2} 
      	rcftRemoteV35PortTraps		OBJECT IDENTIFIER	::={rcftRemoteDeviceV35MIB 3}    
       	    
