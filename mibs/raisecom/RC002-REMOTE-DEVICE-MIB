     --------------------------VCG 的告警------------------------ 
                                       
       rcftRemoteVCGGIDTraps 	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteVCGAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		VCG成员的 的VCG-ID 错误 GID告警
     	"                
     	::= { rcftRemoteVCGTraps 1 } 
     	
     	rcftRemoteVCGLOATraps 	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteVCGAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		VCG成员的 的LOA告警 
     		LOA：VCAT使能时，成员相对延时差超过设定范围告警的或。
     	"                
     	::= { rcftRemoteVCGTraps 2 }    
     	
     	rcftRemoteVCGLFDTraps 	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteVCGAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		VCG成员的 的LFD告警    
     		LFD：GFP帧定界告警
     	"                
     	::= { rcftRemoteVCGTraps 3 }
       rcftRemoteVCGCSFTraps 	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteVCGAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		VCG成员的 的CSF告警   
     		CSF：GFP客户信号失效告警
     	"                
     	::= { rcftRemoteVCGTraps 4 } 
     	
     	rcftRemoteVCGTLCTTraps 	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteVCGAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		TLCT：VCG发送全部成员实效告警  
     	"                
     	::= { rcftRemoteVCGTraps 5 } 
 
        rcftRemoteVCGTLCRTraps 	NOTIFICATION-TYPE
      	OBJECTS{
			rcftRemoteVCGAlarmStatus      		
      	}	 
      	STATUS current
     	DESCRIPTION
     	"
     		TLCR：VCG发送全部成员实效告警
     	"                
     	::= { rcftRemoteVCGTraps 6 } 

