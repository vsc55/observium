   --------------------------DS3E3端口的对象---------------------------------  
      
   rcftRemoteDS3E3PortTable OBJECT-TYPE   
       SYNTAX  SEQUENCE OF RcftRemoteDS3E3PortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
         The DS3E3 PORT table.
       "
       ::= { rcftRemoteDS3E3PortObjects 1 }
   
    rcftRemoteDS3E3PortEntry OBJECT-TYPE
       SYNTAX  RcftRemoteDS3E3PortEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
       "
          An entry in the DS3E3 PORT table.
                
       "
       INDEX   { rcftChassisIndex, rcftSlotIndex,
        			rcftRemoteDeviceIndex,rcftRemoteDS3E3PortIndex }
       ::= { rcftRemoteDS3E3PortTable 1 }
                 
                 
    RcftRemoteDS3E3PortEntry ::=
       SEQUENCE {
       rcftRemoteDS3E3PortIndex		 	Integer32,
       rcftRemoteDS3E3PortAlarmStatus	INTEGER,
       rcftRemoteDS3E3PortStatus		INTEGER, 
       rcftRemoteDS3E3PortESCont		INTEGER,
       rcftRemoteDS3E3PortBertStatus	INTEGER,
       rcftRemoteDS3E3PortFaultFass		INTEGER,
       rcftRemoteDS3E3PortLoopStatus	INTEGER,
       rcftRemoteDS3E3PortOrder			INTEGER
       }                               
       
    rcftRemoteDS3E3PortIndex OBJECT-TYPE
    	SYNTAX	   Integer32
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"
    		DS3E3口的索引
    	"             
        ::= { rcftRemoteDS3E3PortEntry 1 }   
  
  
    rcftRemoteDS3E3PortAlarmStatus	OBJECT-TYPE
    	SYNTAX	INTEGER
    	MAX-ACCESS read-only
    	STATUS		current
    	DESCRIPTION
    	"    
    		BIT8：  OOF    从本地设备上看对端设备接收信号帧失步告警 	1：正常    0：告警
            BIT7：  RAI    从本地设备上看对端设备RAI告警				1：正常    0：告警
    		BIT22： LOF    从本地设备上看对端设备电口LOF告警			1：正常	   0：告警  
    		BIT21	CRC	   从本地设备上看对端设备电口CRC校验错告警		1：正常	   0：告警
			BIT20   RLCV   从本地设备上看对端设备电口编码冲突告警       1：正常    0：告警
			BIT19   RDMO   从本地设备上看对端设备电口发送信号无输出     1：正常    0：告警
			BIT18   RLOL   从本地设备上看对端设备电口接收信号失锁告警   1：正常    0：告警
			BIT17   RLOS   从本地设备上看对端设备电口接收信号丢失告警   1：正常    0：告警
			BIT16   RAIS   从本地设备上看对端设备电口接收信号AIS 告警   1：正常    0：告警
            
            BIT8：  OOF   接收信号帧失步告警 1：正常    0：告警
            BIT7：  RAI   RAI告警 			 1：正常    0：告警
            BIT6：  LOF   接收信号帧丢失告警 1：正常    0：告警
            BIT5	CRC	  CRC校验错告警		 1：正常	0：告警
			BIT4    LCV   编码冲突告警       1：正常    0：告警
			BIT3    DMO   发送信号无输出     1：正常    0：告警
			BIT2    LOL   接收信号失锁告警   1：正常    0：告警
			BIT1    LOS   接收信号丢失告警   1：正常    0：告警
			BIT0    AIS   接收信号AIS 告警   1：正常    0：告警
    	"
        ::= { rcftRemoteDS3E3PortEntry 2 }   
    	
    	                 
	rcftRemoteDS3E3PortStatus	OBJECT-TYPE
		SYNTAX	INTEGER
		MAX-ACCESS read-write
		STATUS	current
		DESCRIPTION
		"       
			 BIT10:  线路编码	
			 		0:AMI
			 		1:B3ZS		--------------DS3
			 		1:HDB3      --------------E3                                  
			 BIT9-8：帧结构:	
			 		00:G.751	01:G.832   ------------E3
			 		00:M23		01:C-bit   ------------DS3
			 		  
			 BIT7: 是否成帧				1: 成帧,	 0:非成帧
			 BIT6: DS3/E3传输距离		1：长距，	 0：短距  
			 BIT5：保留
			 BIT4：保留
			 BIT3：保留
			 
			 BIT2-1     端口的环回状态： 0：无环回，1：内环，2：外环，3：双向环
		     BIT0  		端口模式选择     0：E3    1：DS3  
		"
		::= { rcftRemoteDS3E3PortEntry 3 }   
	
	rcftRemoteDS3E3PortESCont	  OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS	read-only
		STATUS  current
		DESCRIPTION
		"
			误码秒个数
		"             
        ::= { rcftRemoteDS3E3PortEntry 4 }  
	
	rcftRemoteDS3E3PortBertStatus	OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS	read-only
		STATUS	current
		DESCRIPTION
		"
		1: 误码仪使能
		2: 误码仪禁止
		"           
		::= { rcftRemoteDS3E3PortEntry 5 }  
     
     rcftRemoteDS3E3PortFaultFass   OBJECT-TYPE
		SYNTAX  INTEGER
		MAX-ACCESS	read-write
		STATUS	current
		DESCRIPTION 
		"
		1: 故障转移使能
		2: 故障转移禁止
		"
        ::= { rcftRemoteDS3E3PortEntry 6 }  
         	     
 	  rcftRemoteDS3E3PortLoopStatus  OBJECT-TYPE
		 SYNTAX  INTEGER
		 MAX-ACCESS	read-only
		 STATUS	current
		 DESCRIPTION 
		 "
		  	1: 本地环回，
			2：无本地环回，
			3：远端环回，
			4：无远端环回，
			5: 无环回，
			6：本地内环使能，
			7：本地外环使能，
			8：本地双向环使能， 
			9：远端内环使能，
			10：远端外环使能，
			11：远端双向环使能，
			12：本地内环禁止，
			13: 本地外环禁止，
			14：本地双向环禁止，
			5： 远端内环禁止，
			16：远端外环禁止，
			17：远端双向环禁止，
		 " 
         ::= { rcftRemoteDS3E3PortEntry 7 }  
         
         
      rcftRemoteDS3E3PortOrder  OBJECT-TYPE
		  SYNTAX  INTEGER
		  MAX-ACCESS	read-write
		  STATUS	current
		  DESCRIPTION  
		  "
		 		1：硬复位
				2：软复位
				3：开始查询统计信息
				4：结束查询统计信息
				5：内环使能
				6：内环禁止
				7：外环使能
				8：外环禁止
				9: 无环回
				10: 本地内环使能
				11: 本地外环使能
				12: 本地双向环使能
				13: 远端内环使能
				14: 远端外环使能
				15: 远端双向环使能
				16: 本地内环禁止
				17: 本地外环禁止
				18: 本地双向环禁止
				19: 远端内环禁止
				20: 远端外环禁止
				21: 远端双向环禁止
 				22: 端口BERT测试开始
 				23: 端口BERT测试结束
		" 
         ::= { rcftRemoteDS3E3PortEntry 8 }  
                  
                    
    --------------------------DS3E3端口的流量统计------------------------ 

        
         rcftRemoteDS3E3StatisticTable OBJECT-TYPE
	        SYNTAX SEQUENCE OF RcftRemoteDS3E3StatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	          局端设备DS3E3端口的统计表.
	          "
            ::= {rcftRemoteDS3E3PortPerformance 1}   
                       

	    rcftRemoteDS3E3StatisticEntry OBJECT-TYPE
	        SYNTAX RcftRemoteDS3E3StatisticEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	          "
	             以rcftRemoteDS3E3Index为索引
	          "
	        AUGMENTS { rcftRemoteDS3E3PortEntry }
	        ::= {rcftRemoteDS3E3StatisticTable 1}

	    RcftRemoteDS3E3StatisticEntry ::= SEQUENCE {
				rcftRemoteDS3E3TxPackets       Counter32, 
				rcftRemoteDS3E3TxBytes           Counter32, 
				rcftRemoteDS3E3TxFailurePackets   Counter32,					
				rcftRemoteDS3E3RxPackets         Counter32,
				rcftRemoteDS3E3RxBytes           Counter32,
				rcftRemoteDS3E3RxErrorPackets    Counter32,
				rcftRemoteDS3E3FluxTimer	Counter32		
	         }
					

    	  rcftRemoteDS3E3TxPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS3E3口发送以太网包数
						"
					::= { rcftRemoteDS3E3StatisticEntry 1 }
		
		rcftRemoteDS3E3TxBytes OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS3E3口发送以太网字节数
						"
					::= { rcftRemoteDS3E3StatisticEntry 2 }   
					

	    rcftRemoteDS3E3TxFailurePackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS3E3电口发送以太网丢包数
						"
					::= { rcftRemoteDS3E3StatisticEntry 3 } 
										
		 rcftRemoteDS3E3RxPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS3E3口接收以太网包数
						"
					::= { rcftRemoteDS3E3StatisticEntry 4 }
					
		 rcftRemoteDS3E3RxBytes OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS3E3口接收以太网字节数
						"
					::= { rcftRemoteDS3E3StatisticEntry 5 }
					
	    rcftRemoteDS3E3RxErrorPackets OBJECT-TYPE
					SYNTAX Counter32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
						"
							DS3E3电口接收以太网丢包数
						"
					::= { rcftRemoteDS3E3StatisticEntry 6 }			
		
        rcftRemoteDS3E3FluxTimer	OBJECT-TYPE
        			SYNTAX		Counter32
        			MAX-ACCESS 	read-only
        			STATUS		current
        			DESCRIPTION
        				"
        					流量统计的时间
        				"			
                    ::= { rcftRemoteDS3E3StatisticEntry 7 } 

		
	                               
