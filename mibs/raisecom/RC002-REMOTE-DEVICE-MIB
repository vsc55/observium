       rcftRemoteEthFxPortCurrentTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF RcftRemoteEthFxPortCurrentEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                 "
                 The Remote Device FX Port table.
                 "
             ::= { rcftRemoteDeviceEthFxPerformance 1 }

   		rcftRemoteEthFxPortCurrentEntry OBJECT-TYPE
              SYNTAX  RcftRemoteEthFxPortCurrentEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                 "
                 An entry in the Fx Port config table.
                 
                 "
              INDEX   { rcftChassisIndex, rcftSlotIndex,rcftRemoteDeviceIndex, rcftRemoteEthFxIndex }
             ::= { rcftRemoteEthFxPortCurrentTable 1 }
             
          RcftRemoteEthFxPortCurrentEntry ::=
              SEQUENCE {                 
		            rcftRemoteEthFxCurrentTemperature	OCTET STRING,
		            rcftRemoteEthFxCurrentVoltage       OCTET STRING,
		            rcftRemoteEthFxCurrentOffsetCurr    OCTET STRING,
		            rcftRemoteEthFxCurrentRecvPower     OCTET STRING,
		            rcftRemoteEthFxCurrentSendPower     OCTET STRING                      								
                   }
		    	
	      rcftRemoteEthFxCurrentTemperature OBJECT-TYPE
	            SYNTAX  OCTET STRING(SIZE (0..20))
	            MAX-ACCESS  read-only
	            STATUS  current
	            DESCRIPTION
	               "this OBJECT indicates the average temperature in the current 15 minute interval."
	             ::= { rcftRemoteEthFxPortCurrentEntry 1 }
	
	      rcftRemoteEthFxCurrentVoltage OBJECT-TYPE
	            SYNTAX  OCTET STRING(SIZE (0..20))
	            MAX-ACCESS  read-only
	            STATUS  current
	            DESCRIPTION
	               "This OBJECT indicates the averaget voltage in the current 15 minute interval."
	             ::= { rcftRemoteEthFxPortCurrentEntry 2 }
		
	      rcftRemoteEthFxCurrentOffsetCurr OBJECT-TYPE
	            SYNTAX  OCTET STRING(SIZE (0..20))
	            MAX-ACCESS  read-only
	            STATUS  current
	            DESCRIPTION
	               "The object indicates the average offset current in the current
	               15 minute interval."
	            ::= { rcftRemoteEthFxPortCurrentEntry 3 }
	
	      rcftRemoteEthFxCurrentRecvPower OBJECT-TYPE
	            SYNTAX  OCTET STRING(SIZE (0..20))
	            MAX-ACCESS  read-only
	            STATUS  current
	            DESCRIPTION
