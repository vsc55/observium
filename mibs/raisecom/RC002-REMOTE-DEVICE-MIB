        rcftRemoteEthFxPortTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF RcftRemoteEthFxPortEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                 "
                 The Remote Device FX Port table.
                 "
             ::= { rcftRemoteDeviceEthFxObjects 1 }

   		rcftRemoteEthFxPortEntry OBJECT-TYPE
              SYNTAX  RcftRemoteEthFxPortEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                 "
                 An entry in the Fx Port config table.
                 
                 "
              INDEX   { rcftChassisIndex, rcftSlotIndex,rcftRemoteDeviceIndex, rcftRemoteEthFxIndex }
             ::= { rcftRemoteEthFxPortTable 1 }
             
          RcftRemoteEthFxPortEntry ::=
              SEQUENCE {
                    rcftRemoteEthFxIndex                        Integer32,
                    rcftRemoteEthFxFlowControl					INTEGER,
                    rcftRemoteEthFxPortRLK                     	INTEGER,
          			rcftRemoteEthFxPortTLK                     	INTEGER,
          			rcftRemoteEthFxPortSD           			INTEGER,
          			rcftRemoteEthFxPortTxPowerAbnormal			INTEGER,
					rcftRemoteEthFxPortRxSensitiveAbnormal     	INTEGER,
					rcftRemoteEthFxPortLaserAbnormal           	INTEGER,
					rcftRemoteEthFxShutDown                  	INTEGER,
					rcftRemoteEthFxModuleType					INTEGER,
					rcftRemoteEthFxFaultPass					INTEGER,
					rcftRemoteEthFxPortLink 					INTEGER,
					rcftRemoteEthFxRxToTxFaultPass				INTEGER, 
					rcftRemoteEthFxTxDisabledByFR				INTEGER,
					rcftRemoteEthFxOrderTimeParameter		 	INTEGER,
					rcftRemoteEthFxOrderModeParameter		 	INTEGER,
					rcftRemoteEthFxOrder					 	INTEGER,
					rcftRemoteEthFxPortExist 		INTEGER,
					rcftRemoteEthFxPortAuto			INTEGER,
					rcftRemoteEthFxModuleMaxSpeed   INTEGER,
					rcftRemoteEthFxTranDistance		INTEGER,
					rcftRemoteEthFxModuleWaveLen	INTEGER, 
					rcftRemoteEthFxPortConnectorType	INTEGER,
					rcftRemoteEthFxPortTransmitMedia	INTEGER,
					rcftRemoteEthFxModuleManufacturer	OCTET STRING,
					rcftRemoteEthFxModuleDescr			OCTET STRING,
					rcftRemoteEthFxPortModuleVersion			OCTET STRING,
					rcftRemoteEthFxModuleSerialNumber	OCTET STRING, 
					rcftRemoteEthFxPortSFPDiagnoInfo	INTEGER,
					rcftRemoteEthFxSFPDiagnoAlarmStatus	INTEGER,
					rcftRemoteEthFxPortStatus		INTEGER,
					rcftRemoteEthFxUntag	INTEGER,
					rcftRemoteEthFxPVID		INTEGER,
					rcftRemoteEthFxPortSFPType INTEGER,
					rcftRemoteEthFxPortSFPInfo	INTEGER,
					rcftRemoteEthFxPortLoopStatus	INTEGER,
					rcftRemoteEthFxPortRxRestrictSpeed	INTEGER,
					rcftRemoteEthFxPortTxRestrictSpeed	INTEGER,
					rcftRemoteEthFxSFPDiagnoWarningStatus	INTEGER,
					rcftRemoteEthFxPortLineOrClient	INTEGER,
                    rcftRemoteEthFxCOS		INTEGER
						
                   }
                   
          rcftRemoteEthFxIndex OBJECT-TYPE
          		SYNTAX  	Integer32
          		MAX-ACCESS  read-only
          		STATUS  	current
          		DESCRIPTION
