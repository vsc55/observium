	   rcftRemoteDeviceManageID OBJECT-TYPE
	      	SYNTAX	INTEGER
	      	MAX-ACCESS read-write
	      	STATUS	current
	      	DESCRIPTION
	      	"
	      	    设备管理ID	1~255
	   		"     
	   		::= { rcftRemoteDeviceSysEntry 65} 

	   rcftRemoteDeviceMibUse OBJECT-TYPE
	      	SYNTAX	INTEGER
	      	{
	      		mib002(1),
	      		rccomlib(2)
	      	}
	      	MAX-ACCESS read-only
	      	STATUS	current
