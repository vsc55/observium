      --   Fe Current table
	     rcftRemoteEthFePortTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF RcftRemoteEthFePortEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
              "
                 The Remote Device Fe Port table.
              "
             ::= { rcftRemoteDeviceEthFeObjects 1 }

   			rcftRemoteEthFePortEntry OBJECT-TYPE
              SYNTAX  RcftRemoteEthFePortEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
              "
                 An entry in the Fe Port config table.
              "
              INDEX   { rcftChassisIndex, rcftSlotIndex,rcftRemoteDeviceIndex,  rcftRemoteEthFeIndex }
             ::= { rcftRemoteEthFePortTable 1 }
             
          RcftRemoteEthFePortEntry ::=
              SEQUENCE {
              			rcftRemoteEthFeIndex                     Integer32,
                    	rcftRemoteEthFeLinkStatus                INTEGER,
          			    rcftRemoteEthFeShutDown                  INTEGER,   
          			    rcftRemoteEthFeAutoNegotiation           INTEGER,
          			    rcftRemoteEthFeSpeed                	 INTEGER,
						rcftRemoteEthFeDuplex                    INTEGER,
						rcftRemoteEthFeFlowControl				 INTEGER,
						rcftRemoteEthFeRestrictSpeed			 INTEGER,
						rcftRemoteEthFeFaultPass				 INTEGER,
						rcftRemoteEthFeDisabledByRemoteTP		 INTEGER,
						rcftRemoteEthFeDisabledByFxToFeFP		 INTEGER,
						rcftRemoteEthFeTxRestrictSpeed			 INTEGER,
						rcftRemoteEthFeTag						 INTEGER,
						rcftRemoteEthFePortStatus				 INTEGER,
						rcftRemoteEthFeRestrictSpeedStep		 INTEGER,
						rcftRemoteEthFeOrderTimeParameter		 INTEGER,
						rcftRemoteEthFeOrderModeParameter		 INTEGER,
						rcftRemoteEthFeOrder					 INTEGER,
						rcftRemoteEthFePortStatusExtend 	INTEGER,  
						rcftRemoteEthFeStormControl			INTEGER,
						rcftRemoteEthFePVID					INTEGER,
						rcftRemoteEthFeDefaultCOS			INTEGER,
						rcftRemoteEthFeQoSPolicy			INTEGER						 
						}

            rcftRemoteEthFeIndex OBJECT-TYPE
					SYNTAX Integer32
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
					"
						远端设备以太网电口端口索引
					"
					::= { rcftRemoteEthFePortEntry 1 }
	   
	   		rcftRemoteEthFeLinkStatus OBJECT-TYPE
					SYNTAX INTEGER
					{
						linkup(1),
						linkdown(2)	
					}
					MAX-ACCESS read-only
					STATUS current
					DESCRIPTION
					"
						远端设备以太网电口的link状态
					"
					::= { rcftRemoteEthFePortEntry 2 }
					
	 		rcftRemoteEthFeShutDown OBJECT-TYPE
					SYNTAX INTEGER
					{
						open(1),
						close(2),
						closebyLocalOtherPortFault(3),
						closebyOppositeFePortFault(4),
						closebyLoopBack(5)
							
					}
					MAX-ACCESS read-write
					STATUS current
					DESCRIPTION
					"
						远端设备以太网电口的开关使能   
						1：电口开；2：电口关；
						3：电口因为本地其它口故障关； 
						4：电口因为对端设备电口故障关
					"
					::= { rcftRemoteEthFePortEntry 3 }
					
		   rcftRemoteEthFeAutoNegotiation OBJECT-TYPE
			        SYNTAX INTEGER
			        {   
			            auto(1),
			            manul(2)
			        }
			        MAX-ACCESS read-write
			        STATUS current
			        DESCRIPTION
			          "
			          远端设备电口速率协商方式（1:自协商,2:手动）.   
			          This object indicating the negoation mode of the electronic port 
			          in the local module.When the value is 1 the eletronic port is 
			          set to auto negotiation,otherwise the eletronic port is set to manual negotiation.
			          "					
          			::= { rcftRemoteEthFePortEntry 4 }
					
		  rcftRemoteEthFeSpeed OBJECT-TYPE
		       SYNTAX INTEGER
		        {   
		            rcft10Mbps(1),
		            rcft100Mbps(2),
		            rcft1000Mbps(3),
		            rcft10Gbps(4),
		            other(16)
		        }
		        MAX-ACCESS read-write
		        STATUS current
		        DESCRIPTION
		          "
		            远端设备电口的速率.  
		            The speed of the electronic port. 
		            1、10Mbps
		            2、100Mbps
		            3、1000Mbps
		            4、10Gbps
		            5、other       
		          "					
		          ::= { rcftRemoteEthFePortEntry 5 }
					
					
		 rcftRemoteEthFeDuplex OBJECT-TYPE
		        SYNTAX INTEGER
		        {   
		            full-duplex(1),
		            half-duplex(2)
		        }
		        MAX-ACCESS read-write
		        STATUS current
		        DESCRIPTION
		          "
		          远端设备电口双工模式（1:全双工，2:半双工）.   
		          The duplex mode of the electronic port.  
		          1:full duplex
		          2:half duplex      
		          "					
		         ::= { rcftRemoteEthFePortEntry 6 }
					

         rcftRemoteEthFeFlowControl OBJECT-TYPE
         		SYNTAX	INTEGER
         		{
         		     enable(1),
         		     disable(2)
         		}
	          MAX-ACCESS  read-write
	          STATUS  current
	          DESCRIPTION
	                 "
	                 	远端设备以太网端口流控功能使能(1:使能，2:禁止)
	                 "
			 ::= { rcftRemoteEthFePortEntry 7 }	
			 		
		rcftRemoteEthFeRestrictSpeed	OBJECT-TYPE
				SYNTAX	INTEGER
				MAX-ACCESS	read-write
				STATUS	current
				DESCRIPTION
					"
						远端设备以太网端口接收限定速率：(n * value；n = 0,1,2....2^12. value = 32kb/s).
					"
			 ::= { rcftRemoteEthFePortEntry 8 }	
			 
		rcftRemoteEthFeFaultPass	OBJECT-TYPE
				SYNTAX INTEGER
				{
					enable(1),
					disable(2)
				}
				MAX-ACCESS	read-write
				STATUS	current
				DESCRIPTION
					"
						远端设备以太网端口故障转移功能(1:使能，2:禁止)			
					"
				 ::= { rcftRemoteEthFePortEntry 9 }		
				 
		rcftRemoteEthFeDisabledByRemoteTP	OBJECT-TYPE
			SYNTAX INTEGER
			{
				open(1),
				close(2)
			}		 	
			MAX-ACCESS	read-only
			STATUS	current
			DESCRIPTION
			"
				远端设备以太网电口关闭的原因是由于对端电口linkdown		
			"
			::= { rcftRemoteEthFePortEntry 10 }		
			 	
		rcftRemoteEthFeDisabledByFxToFeFP	OBJECT-TYPE
			SYNTAX INTEGER
			{
				open(1),
				close(2)
			}		 	
			MAX-ACCESS	read-only
			STATUS	current
			DESCRIPTION
			"
				远端设备以太网电口关闭的原因是由于光到电故障转移功能使能		
			"
			::= { rcftRemoteEthFePortEntry 11 }		
			 	
		rcftRemoteEthFeTxRestrictSpeed		OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS	read-write
			STATUS	current
			DESCRIPTION
			"
				远端设备以太网电口发送限定速率。其最大值视板卡而不同。
			"
			::= { rcftRemoteEthFePortEntry 12 }	
				 	
			 
		rcftRemoteEthFeTag	OBJECT-TYPE
			SYNTAX INTEGER
			{
				tag(1),
				untag(2)
			}
			MAX-ACCESS read-write
			STATUS	current
			DESCRIPTION
			"
				远端设备以太网电口的标签
			"			                
		    ::= { rcftRemoteEthFePortEntry 13 }	
		    
		 rcftRemoteEthFePortStatus	OBJECT-TYPE
		 	SYNTAX INTEGER
		 	MAX-ACCESS read-write
		 	STATUS	current
		 	DESCRIPTION
		 	"     
                BIT5    LOC_FP_EN   1：使能本地其它端口故障向电口转移；0：禁止。
                BIT4    REM_FP_EN	1：使能远端电口故障向电口转移；    0：禁止。
                BIT3-2  MDIX_Valid 	00：电口允许自交叉且工作于SW方式；
                                    01：电口允许自交叉且工作于NIC方式；
                                    10：电口禁止自交叉，设置工作于SW方式；
                                    11：电口禁止自交叉。设置工作于NIC方式。
				BIT1-0  保留		 	
		 	"
	        ::= { rcftRemoteEthFePortEntry 14 }	 
                 
	 	rcftRemoteEthFeRestrictSpeedStep	OBJECT-TYPE
	 		SYNTAX	INTEGER
	 		MAX-ACCESS	read-only
	 		STATUS		current
	 		DESCRIPTION
	 		"
	 			带宽粒度 
	 			0x00：64Kbps；
	 			0x01：32Kbps（无）；
	 			0x02：80Kbps;
				0x03：800Kbps; 
				0x04：8000Kbps;
				其余：保留。 		
	 		"   
			::= { rcftRemoteEthFePortEntry 15 }	 
		
		rcftRemoteEthFeOrderTimeParameter OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS read-write
			STATUS	current
			DESCRIPTION
	  		 "
	  		    给远端模块发送命令时所需要赋的时长参数。 
	  		    	
	  		    1: 测试时长1分钟
	  		    2: 测试时长2分钟
	  		    3: 测试时长4分钟
	  		    4: 测试时长8分钟
	  		    5: 测试时长15分钟
	  		    6: 测试时长30分钟
	  		    7: 测试时长60分钟  
	  		    	
	  		    18: 测试2帧
	  		    19: 测试5帧
	  		    20: 测试10帧
	  		    21: 测试20帧
	  		    22: 测试50帧
	  		    23: 测试100帧 		    	  		    			
	  		"                 
            ::= { rcftRemoteEthFePortEntry 16 }	    
            
      	rcftRemoteEthFeOrderModeParameter OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS read-write
			STATUS	current
			DESCRIPTION
	  		 "
	  		    给远端模块发送命令时所需要赋的模式参数。  	  		    			
	  		    
	  		"                 
            ::= { rcftRemoteEthFePortEntry 17 }            
            
		rcftRemoteEthFeOrder	OBJECT-TYPE
			SYNTAX	INTEGER
			MAX-ACCESS	read-write
			STATUS	current
			DESCRIPTION
			"
			    1：硬复位
				2：软复位
				3：开始查询统计信息
				4：结束查询统计信息
				5：内环使能
				6：内环禁止
				7：外环使能
				8：外环禁止 
				9: 无环回
				10: 本地内环使能
				11: 本地外环使能
				12: 本地双向环使能
				13: 远端内环使能
				14: 远端外环使能
				15: 远端双向环使能
				16: 本地内环禁止
				17: 本地外环禁止
				18: 本地双向环禁止
				19: 远端内环禁止
				20: 远端外环禁止
				21: 远端双向环禁止
				22: 端口BERT测试开始
				23: 端口BERT测试结束
				
								
			注意：rcftRemoteE1Order的枚举值要和rcftRemoteDeviceOrder始终保持完全一致。
			"
		   ::= { rcftRemoteEthFePortEntry 18 }	        
		   
		 
		   rcftRemoteEthFePortStatusExtend	OBJECT-TYPE   
	      	SYNTAX	INTEGER
	      	MAX-ACCESS	read-write
	      	STATUS	current
	      	DESCRIPTION
	      	"     
	      		由于501b/502b/504a视频光端机以太口信息没有遵循002标准子块，因此在这里单独
	      		定义一个节点表示视频光端机以太口信息 
	      		
	      	   bit15-8:  视频光端机以太口的位置信息
	      	   bit15-12：盘 （0代表基盘。002目前只有基盘，固定bit7-4为全0）
			   bit11-8： 槽 （1~15:扩展槽）
	      	 
	      	   BIT4 link	1：linkup		0：linkdown	
	      	   BIT3	开关	1：打开			0：关闭       	            		
	      	   BIT2	自适应  1：使能			0：禁止     	
	      	   BIT1	双工  	1：全双工，		0：半双工  	      	            		
	      	   BIT0	速率，	1：100M，		0：10M  
	      	"
	      	::={rcftRemoteEthFePortEntry 19}   
	      	
	      	rcftRemoteEthFeStormControl			OBJECT-TYPE   
	      	SYNTAX	INTEGER
	      	MAX-ACCESS	read-write
	      	STATUS	current
	      	DESCRIPTION
	      	"     
                广播风暴抑制。
					1：使能，2：禁止。

	      	"
	      	::={rcftRemoteEthFePortEntry 20}   

			rcftRemoteEthFePVID					OBJECT-TYPE   
	      	SYNTAX	INTEGER
	      	MAX-ACCESS	read-write
	      	STATUS	current
	      	DESCRIPTION
	      	"     
	      		PVID。范围：1~4094 
	      	"
	      	::={rcftRemoteEthFePortEntry 21}   

			rcftRemoteEthFeDefaultCOS			OBJECT-TYPE   
	      	SYNTAX	INTEGER
	      	MAX-ACCESS	read-write
	      	STATUS	current
	      	DESCRIPTION
	      	"     
	      		默认COS值。范围：0~7 
	      	"
	      	::={rcftRemoteEthFePortEntry 22}   

			rcftRemoteEthFeQoSPolicy			OBJECT-TYPE   
	      	SYNTAX	INTEGER
	      	MAX-ACCESS	read-write
	      	STATUS	current
	      	DESCRIPTION
	      	"     
	      		 QoS优先级划分策略。
					Bit2：DSCP使能。1：使能，0：禁止
					Bit1：COS使能。1：使能，0：禁止
					Bit0：端口优先级。1：高优先级，0：低优先级。
	      	"
	      	::={rcftRemoteEthFePortEntry 23}   
  
	

			 			                                          
