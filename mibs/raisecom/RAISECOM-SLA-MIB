    ::= { rcSlaThresholdTestResultEntry 3 }     
    
rcSlaPktAverLossRate       OBJECT-TYPE 
	SYNTAX      INTEGER(0..10000)
	MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The average pkt loss rate of all test."
    ::= { rcSlaThresholdTestResultEntry 4 }     
    
