--                    panHwControllerDriveEntry
--                        panHwControllerDriveDrawer
--                        panHwControllerDriveTray
--                        panHwControllerDriveSlot
--                        panHwControllerDriveSN
--                        panHwControllerDriveType
--                        panHwControllerDriveVendor
--                        panHwControllerDriveModel
