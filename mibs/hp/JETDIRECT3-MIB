    notused2   OBJECT IDENTIFIER ::={ netPrinterType 11 }
    hplj4050   OBJECT IDENTIFIER ::={ netPrinterType 12 }
    notused3   OBJECT IDENTIFIER ::={ netPrinterType 13 }
    hpclj4550  OBJECT IDENTIFIER ::={ netPrinterType 14 }
