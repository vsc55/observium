    SYNTAX      TOperator
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Source TCP/UDP port operator."
    DEFVAL { none }
    ::= { aluSecAppOperEntry 5 }

aluSecAppOperMatchDstPortValue1 OBJECT-TYPE
    SYNTAX      TTcpUdpPort
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Destination TCP/UDP port value."
    DEFVAL { 0 }
    ::= { aluSecAppOperEntry 6 }

