enc2111BasicGroup OBJECT-GROUP
	OBJECTS {
		enc2111TrapCtrl,
		enc2111TrapIPIndex,
		enc2111TrapAddr,
		enc2111portNumber,
		enc2111PortIndex,
		enc2111PortName,
		enc2111PortState,
		enc2111PortSwitchCount,
		enc2111PortStartupMode,
		enc2111PortStartupDelay,
