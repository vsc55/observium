	-- 1.3.6.1.4.1.28507.60.2.1.1
	::= { enc2111Groups 1 }

enc2111NotificationGroup NOTIFICATION-GROUP
	NOTIFICATIONS {
		enc2111SwitchEvtPort,
		enc2111InputEvt,
		enc2111TempEvtSen,
		enc2111HygroEvtSen,
		enc2111InputEvtSen,
		enc2111AirPressureEvtSen,
		enc2111DewPtDiffEvtSen,
		enc2111POEEvt }
	STATUS  current
	DESCRIPTION ""
	-- 1.3.6.1.4.1.28507.60.2.1.2
	::= { enc2111Groups 2 }

