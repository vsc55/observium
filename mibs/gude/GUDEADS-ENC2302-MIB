	-- 1.3.6.1.4.1.28507.70.2.1.1
	::= { enc2302Groups 1 }

enc2302NotificationGroup NOTIFICATION-GROUP
	NOTIFICATIONS {
		enc2302SwitchEvtPort,
		enc2302InputEvt,
		enc2302TempEvtSen,
		enc2302HygroEvtSen,
		enc2302InputEvtSen,
		enc2302AirPressureEvtSen,
		enc2302DewPtDiffEvtSen }
	STATUS  current
	DESCRIPTION ""
	-- 1.3.6.1.4.1.28507.70.2.1.2
	::= { enc2302Groups 2 }

