enc2302BasicGroup OBJECT-GROUP
	OBJECTS {
		enc2302TrapCtrl,
		enc2302TrapIPIndex,
		enc2302TrapAddr,
		enc2302portNumber,
		enc2302PortIndex,
		enc2302PortName,
		enc2302PortState,
		enc2302PortSwitchCount,
		enc2302PortStartupMode,
		enc2302PortStartupDelay,
		enc2302PortRepowerTime,
		enc2302ActiveInputs,
		enc2302InputIndex,
		enc2302Input,
		enc2302SensorIndex,
		enc2302TempSensor,
		enc2302HygroSensor,
		enc2302InputSensor,
		enc2302AirPressure,
		enc2302DewPoint,
		enc2302DewPointDiff }
	STATUS  current
	DESCRIPTION
