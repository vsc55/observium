	DESCRIPTION
	  "Probcause,AlarmType - NMS completion"
	REVISION "201411210000Z"
	DESCRIPTION
	  "Labels cosmetic updates"
	REVISION "201605230000Z"
	DESCRIPTION
	  "New AlarmType – synthesisAlarm (12)"
        ::= { ekinops 30 }



