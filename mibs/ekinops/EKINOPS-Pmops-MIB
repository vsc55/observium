PmopsSwitchMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "different switch mode for port"
    SYNTAX INTEGER {
		switchManual1(1),
		switchManual2(2),
		switchAuto(4),
		switchAutoRevertive(8)
           }


