          rm10010RinvLineIndex 
              Integer32,
          rm10010RinvxfpLine
              DisplayString
        }


    rm10010RinvLineIndex OBJECT-TYPE
        SYNTAX  Integer32(1..16)
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
              "Index for XFP table"
        ::= { rm10010RinvLineEntry 1 }


