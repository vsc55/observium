	::= { rm10010CtrlaccessLoopTable 1 }


Rm10010CtrlaccessLoopEntry ::=
	SEQUENCE {
	rm10010CtrlaccessLoopIndex
		INTEGER,
	rm10010CtrlaccessLoopPortn
		EkiState
	}


