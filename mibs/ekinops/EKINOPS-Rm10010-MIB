		--&FILTNAME    "RM10010 Client HW Fail ON (Hidden)"
		--&ACTIONS     { info }
		--&MATCH       { rm10010AlmFailAccPortn "*", rm10010AlmHwFailAccPortn "on", rm10010trapPortNumber "$3", rm10010trapBoardNumber "$4"}
		--&MESG        "Client Port $3 Down on RM10010 in Slot $4 ($A)"
