	SYNTAX Rm10010AlmclientSfpWarnDdmEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
        "
        Row definition for the rm10010AlmclientSfpWarnDdm 
        table
        "
	INDEX   { rm10010AlmclientSfpWarnDdmIndex }
	::= { rm10010AlmclientSfpWarnDdmTable 1 }


