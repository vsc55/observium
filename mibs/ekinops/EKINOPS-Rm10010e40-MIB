      ::= { rm10010e40riTable 1 }

rm10010e40RinvSfpEntry OBJECT-TYPE
        SYNTAX  Rm10010e40RinvSfpEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
              "Row definition for the SFP table"
        INDEX   { rm10010e40RinvSfpIndex }
        ::= { rm10010e40RinvSfpTable 1 }
    


