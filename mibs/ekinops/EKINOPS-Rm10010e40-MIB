	rm10010e40AlmFailAccPortn
		EkiOnOff,
	rm10010e40AlmSfpDdmAlmPortn
		EkiOnOff,
	rm10010e40AlmSfpDdmWarningPortn
		EkiOnOff,
	rm10010e40AlmDwCaisPortn
		EkiOnOff,
	rm10010e40AlmClientRemoteOosPortn
		EkiOnOff,
	rm10010e40AlmClientLocalOosPortn
		EkiOnOff,
	rm10010e40AlmDwLsdPortn
		EkiOnOff,
	rm10010e40AlmHwFailAccPortn
		EkiOnOff,
	rm10010e40AlmDdmAbsentPortn
		EkiOnOff,
	rm10010e40AlmSfpAbsentPortn
		EkiOnOff
	}


