pm20002maAlmsynthAlmPortEntry OBJECT-TYPE
	SYNTAX Pm20002maAlmsynthAlmPortEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
        "
        Row definition for the pm20002maAlmsynthAlmPort 
        table
        "
	INDEX   { pm20002maAlmsynthAlmPortIndex }
	::= { pm20002maAlmsynthAlmPortTable 1 }


Pm20002maAlmsynthAlmPortEntry ::=
	SEQUENCE {
	pm20002maAlmsynthAlmPortIndex
		INTEGER,
	pm20002maAlmUpCsfPortn
		EkiOnOff,
	pm20002maAlmFailAccPortn
		EkiOnOff,
	pm20002maAlmSfpDdmAlmPortn
		EkiOnOff,
	pm20002maAlmSfpDdmWarningPortn
		EkiOnOff,
	pm20002maAlmDwCaisPortn
		EkiOnOff,
	pm20002maAlmClientRemoteOosPortn
		EkiOnOff,
	pm20002maAlmClientLocalOosPortn
		EkiOnOff,
	pm20002maAlmDwLsdPortn
		EkiOnOff,
	pm20002maAlmHwFailAccPortn
		EkiOnOff,
	pm20002maAlmDdmAbsentPortn
		EkiOnOff,
	pm20002maAlmSfpAbsentPortn
		EkiOnOff
	}


pm20002maAlmsynthAlmPortIndex OBJECT-TYPE
	SYNTAX  INTEGER(0..128)
	MAX-ACCESS  read-only
	STATUS current
	DESCRIPTION
        "
        Index definition for the pm20002maAlmsynthAlmPort 
        table
        "
	::= { pm20002maAlmsynthAlmPortEntry 1 }


pm20002maAlmUpCsfPortn OBJECT-TYPE
	SYNTAX  EkiOnOff
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
        "
        Client Signal Fail Inserted :This 
        OID indicates a CSF has been inserted 
        in the upstream client port. <Help> 
        This status indicates if a Client 
        Signal Fail is currently inserted 
        on the line signal for this client 
        port <Condtype> UP_CSF_1 <Probcause> 
        informationalStatus(1000) <Alarmtype> 
        other (1) <Polarity> (2) 
        "
	::= { pm20002maAlmsynthAlmPortEntry 17 }


