pm20002maClientTrapCritGoesOn NOTIFICATION-TYPE
	OBJECTS {pm20002maAlmFailAccPortn, pm20002maAlmHwFailAccPortn, pm20002matrapPortNumber, pm20002matrapBoardNumber}
	STATUS current
		--&FILTNAME    "PM20002MA Client Fail ON"
		--&ACTIONS     { log, critical }
		--&MATCH       { pm20002maAlmFailAccPortn "on", pm20002maAlmHwFailAccPortn "*", pm20002matrapPortNumber "$3", pm20002matrapBoardNumber "$4"}
		--&MESG        "Client Port $3 Down on PM20002MA in Slot $4 ($A)"
		
		--&FILTNAME    "PM20002MA Client HW Fail ON (Hidden)"
		--&ACTIONS     { info }
		--&MATCH       { pm20002maAlmFailAccPortn "*", pm20002maAlmHwFailAccPortn "on", pm20002matrapPortNumber "$3", pm20002matrapBoardNumber "$4"}
		--&MESG        "Client Port $3 Down on PM20002MA in Slot $4 ($A)"
	DESCRIPTION "Critical client default is activated"
	::= { pm20002matraps 44 }

pm20002maClientTrapCritGoesOff NOTIFICATION-TYPE
	OBJECTS {pm20002maAlmFailAccPortn, pm20002maAlmHwFailAccPortn, pm20002matrapPortNumber, pm20002matrapBoardNumber}
	STATUS current
		--&FILTNAME    "PM20002MA Client Fail OFF"
		--&ACTIONS     { log, normal }
		--&CLEARS      { "PM20002MA Client Fail ON" }
		--&MATCH       { pm20002maAlmFailAccPortn "on", pm20002maAlmHwFailAccPortn "*", pm20002matrapPortNumber "$3", pm20002matrapBoardNumber "$4"}
		--&MESG        "Client Port $3 Up on PM20002MA in Slot $4 ($A)"
		
		--&FILTNAME    "PM20002MA Client HW Fail OFF (Hidden)"
		--&ACTIONS     { info }
		--&MATCH       { pm20002maAlmFailAccPortn "*", pm20002maAlmHwFailAccPortn "on", pm20002matrapPortNumber "$3", pm20002matrapBoardNumber "$4"}
		--&MESG        "Client Port $3 Up on PM20002MA in Slot $4 ($A)"
	DESCRIPTION "Critical client default disappeared"
	::= { pm20002matraps 45 }






--- *****************************************************
---     HW TRAPS
--- *****************************************************

pm20002maPowerTrapUrgentGoesOn NOTIFICATION-TYPE
	OBJECTS {pm20002maAlmDefFuseB, pm20002maAlmDefFuseA , pm20002matrapBoardNumber}
	STATUS current
		--&FILTNAME    "PM20002MA Fuse A Fail ON"
		--&ACTIONS     { log, major }
		--&MATCH       { pm20002maAlmDefFuseB "*", pm20002maAlmDefFuseA "on", pm20002matrapBoardNumber "$3"}
		--&MESG        "Power Input A Fail on PM20002MA in Slot $3 ($A)"
	
		--&FILTNAME    "PM20002MA Fuse B Fail ON"
		--&ACTIONS     { log, major }
		--&MATCH       { pm20002maAlmDefFuseB "on", pm20002maAlmDefFuseA "*", pm20002matrapBoardNumber "$3"}
		--&MESG        "Power Input B Fail on PM20002MA in Slot $3 ($A)"
	DESCRIPTION "Urgent power supply default is activated"
	::= { pm20002matraps 50 }

pm20002maPowerTrapUrgentGoesOff NOTIFICATION-TYPE
	OBJECTS {pm20002maAlmDefFuseB, pm20002maAlmDefFuseA , pm20002matrapBoardNumber}
	STATUS current
		--&FILTNAME    "PM20002MA Fuse A&B Fail OFF"
		--&ACTIONS     { log, normal}
		--&CLEARS      { "PM20002MA Fuse A Fail ON", "PM20002MA Fuse B Fail ON" }
		--&MATCH       { pm20002maAlmDefFuseB "on", pm20002maAlmDefFuseA "on", pm20002matrapBoardNumber "$3"}
		--&MESG        "Power Inputs A and B Present on PM20002MA in Slot $3 ($A)"
	DESCRIPTION "Urgent power supply default disappeared"
	::= { pm20002matraps 51 }






END
