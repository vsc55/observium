  ::= { pm20002mari 6 }


pm20002maRinvSfpTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF Pm20002maRinvSfpEntry
      MAX-ACCESS  not-accessible
      STATUS  current
      DESCRIPTION
              "List of the SFPs on the selected module"
      ::= { pm20002mariTable 1 }

pm20002maRinvSfpEntry OBJECT-TYPE
        SYNTAX  Pm20002maRinvSfpEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
              "Row definition for the SFP table"
        INDEX   { pm20002maRinvSfpIndex }
        ::= { pm20002maRinvSfpTable 1 }
    


Pm20002maRinvSfpEntry::=
        SEQUENCE {
          pm20002maRinvSfpIndex 
              Integer32,
          pm20002maRinvsfp
              DisplayString
        }


    pm20002maRinvSfpIndex OBJECT-TYPE
        SYNTAX  Integer32(1..16)
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
              "Index for SFP table"
        ::= { pm20002maRinvSfpEntry 1 }


    pm20002maRinvsfp OBJECT-TYPE
        SYNTAX  DisplayString
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
              "Inventory for the SFP: This OID contains the inventory for the SFP"
        ::= { pm20002maRinvSfpEntry 2 }




pm20002maRinvLineTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF Pm20002maRinvLineEntry
      MAX-ACCESS  not-accessible
      STATUS  current
      DESCRIPTION
              "List of the XFPs on the selected module"
      ::= { pm20002mariTable 2 }

