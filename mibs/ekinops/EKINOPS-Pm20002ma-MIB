--- *****************************************************
---     LINE TRAPS
--- *****************************************************

pm20002maLineTrapNotUrgentGoesOn NOTIFICATION-TYPE
	OBJECTS {pm20002maAlmLineDdmWarningPortn, pm20002matrapLineNumber, pm20002matrapBoardNumber}
	STATUS current
	--&FILTNAME    "PM20002MA Line Not Urgent ON"
	--&ACTIONS     { log, minor }
	--&MATCH       { pm20002maAlmLineDdmWarningPortn "on", pm20002matrapLineNumber "$2", pm20002matrapBoardNumber "$3"}
	--&MESG        "DDM Warning appears on Line Port $2 of PM20002MA in Slot $3 ($A)"
	DESCRIPTION "Not urgent Line default is activated"
	::= { pm20002matraps 30 }

pm20002maLineTrapNotUrgentGoesOff NOTIFICATION-TYPE
	OBJECTS {pm20002maAlmLineDdmWarningPortn, pm20002matrapLineNumber, pm20002matrapBoardNumber}
	STATUS current
	--&FILTNAME    "PM20002MA Line Not Urgent OFF"
	--&ACTIONS     { log, normal }
	--&CLEARS      { "PM20002MA Line Not Urgent ON" }
	--&MATCH       { pm20002maAlmLineDdmWarningPortn "on", pm20002matrapLineNumber "$2", pm20002matrapBoardNumber "$3"}
	--&MESG        "DDM Warning disappears on Line Port $2 of PM20002MA in Slot $3 ($A)"
	DESCRIPTION "Not urgent Line default disappeared"
	::= { pm20002matraps 31 }

pm20002maLineTrapUrgentGoesOn NOTIFICATION-TYPE
	OBJECTS {pm20002maAlmLineDdmAlmPortn, pm20002matrapLineNumber, pm20002matrapBoardNumber}
	STATUS current
	--&FILTNAME    "PM20002MA Line Urgent ON"
	--&ACTIONS     { log, major }
	--&MATCH       { pm20002maAlmLineDdmAlmPortn "on", pm20002matrapLineNumber "$2", pm20002matrapBoardNumber "$3"}
	--&MESG        "DDM Alarm appears on Line Port $2 of PM20002MA in Slot $3 ($A)"
	DESCRIPTION "Urgent Line default is activated"
	::= { pm20002matraps 32 }

pm20002maLineTrapUrgentGoesOff NOTIFICATION-TYPE
	OBJECTS {pm20002maAlmLineDdmAlmPortn, pm20002matrapLineNumber, pm20002matrapBoardNumber}
	STATUS current
	--&FILTNAME    "PM20002MA Line Urgent OFF"
	--&ACTIONS     { log, normal }
	--&CLEARS      { "PM20002MA Line Urgent ON" }
	--&MATCH       { pm20002maAlmLineDdmAlmPortn "on", pm20002matrapLineNumber "$2", pm20002matrapBoardNumber "$3"}
	--&MESG        "DDM Alarm disappears on Line Port $2 of PM20002MA in Slot $3 ($A)"
	DESCRIPTION "Urgent Line default disappeared"
	::= { pm20002matraps 33 }

pm20002maLineTrapCritGoesOn NOTIFICATION-TYPE
	OBJECTS {pm20002maAlmLineFailPortn , pm20002maAlmLineHwFailPortn , pm20002matrapLineNumber, pm20002matrapBoardNumber}
	STATUS current
	--&FILTNAME    "PM20002MA Line Fail ON"
	--&ACTIONS     { log, critical }
	--&MATCH       { pm20002maAlmLineFailPortn "on", pm20002maAlmLineHwFailPortn "*", pm20002matrapLineNumber "$3", pm20002matrapBoardNumber "$4"}
	--&MESG        "Line Port $3 Down on PM20002MA in Slot $4 ($A)"
	
	--&FILTNAME    "PM20002MA Line HW Fail ON (Hidden)"
	--&ACTIONS     { info }
	--&MATCH       { pm20002maAlmLineFailPortn "*", pm20002maAlmLineHwFailPortn "on", pm20002matrapLineNumber "$3", pm20002matrapBoardNumber "$4"}
	--&MESG        "Line Port $3 Down on PM20002MA in Slot $4 ($A)"
	DESCRIPTION "Critical Line default is activated"
	::= { pm20002matraps 34 }

pm20002maLineTrapCritGoesOff NOTIFICATION-TYPE
	OBJECTS {pm20002maAlmLineFailPortn , pm20002maAlmLineHwFailPortn , pm20002matrapLineNumber, pm20002matrapBoardNumber}
	STATUS current
	--&FILTNAME    "PM20002MA Line Fail OFF"
	--&ACTIONS     { log,  normal }
	--&CLEARS      { "PM20002MA Line Fail ON" }
	--&MATCH       { pm20002maAlmLineFailPortn "on", pm20002maAlmLineHwFailPortn "*", pm20002matrapLineNumber "$3", pm20002matrapBoardNumber "$4"}
	--&MESG        "Line Port $3 Up on PM20002MA in Slot $4 ($A)"

	--&FILTNAME    "PM20002MA Line HW Fail OFF (Hidden)"
	--&ACTIONS     { info }
	--&MATCH       { pm20002maAlmLineFailPortn "*", pm20002maAlmLineHwFailPortn "on", pm20002matrapLineNumber "$3", pm20002matrapBoardNumber "$4"}
	--&MESG        "Line Port $3 Up on PM20002MA in Slot $4 ($A)"
	DESCRIPTION "Critical Line default disappeared"
	::= { pm20002matraps 35 }






