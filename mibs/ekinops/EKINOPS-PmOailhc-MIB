	--&MATCH       { pmoailhcAlmLineFail "on", pmoailhcAlmLineHwFail "*", pmoailhctrapBoardNumber "$3"}
	--&MESG        "Critical Alarm appears on Pre-Amp input or Booster Output of PMOAILHC in slot $3 ($A)"
	
	--&FILTNAME    "PMOAILHC Line HW Fail ON (Hidden)"
	--&ACTIONS     { info }
	--&MATCH       { pmoailhcAlmLineFail "*", pmoailhcAlmLineHwFail "on", pmoailhctrapBoardNumber "$3"}
	--&MESG        "Critical Alarm appears on Pre-Amp input or Booster Output of PMOAILHC in slot $3 ($A)"
	DESCRIPTION "Critical Line default is activated"
