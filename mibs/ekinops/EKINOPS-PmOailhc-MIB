pmoailhcRinvInLine1Table OBJECT-TYPE
      SYNTAX  SEQUENCE OF PmoailhcRinvInLine1Entry
      MAX-ACCESS  not-accessible
      STATUS  current
      DESCRIPTION
              "List of the InLine1s on the selected module"
      ::= { pmoailhcri 6 }

pmoailhcRinvInLine1Entry OBJECT-TYPE
        SYNTAX  PmoailhcRinvInLine1Entry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
              "Row definition for the InLine1s table"
        INDEX   { pmoailhcRinvInLine1Index }
        ::= { pmoailhcRinvInLine1Table 1 }
    


PmoailhcRinvInLine1Entry::=
        SEQUENCE {
          pmoailhcRinvInLine1Index 
              Integer32,
          pmoailhcRinvInLine1
              DisplayString
        }


    pmoailhcRinvInLine1Index OBJECT-TYPE
        SYNTAX  Integer32(1..16)
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
              "Index for InLine1 table"
        ::= { pmoailhcRinvInLine1Entry 1 }


    pmoailhcRinvInLine1 OBJECT-TYPE
        SYNTAX  DisplayString
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
              "Inventory for the InLine1: This OID contains the inventory for the InLine1"
        ::= { pmoailhcRinvInLine1Entry 2 }




pmoailhcRinvInLine2Table OBJECT-TYPE
      SYNTAX  SEQUENCE OF PmoailhcRinvInLine2Entry
      MAX-ACCESS  not-accessible
