PmmcBitRate ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "different rates for a client and line ports"
    SYNTAX INTEGER {
		rateSTM16OC48(0),
		rate1GBE(1),
		rateSTM4OC12(2),
		rateSTM1OC3(3),
		rate2GFC(4),
		rate1GFC(5),
		rateFastEthernet(7),
		rateSTM16FEC(8),
		rateSTM4FEC(9),
		rateSTM1FEC(10)
           }

