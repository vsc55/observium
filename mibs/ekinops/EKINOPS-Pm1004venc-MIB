--- *****************************************************
---     LINE TRAPS
--- *****************************************************

pm1004vencLineTrapNotUrgentGoesOn NOTIFICATION-TYPE
	OBJECTS {pm1004vencAlmLineDdmWarningPortn, pm1004venctrapLineNumber, pm1004venctrapBoardNumber}
	STATUS current
	DESCRIPTION "Not urgent Line default is activated"
	::= { pm1004venctraps 30 }

pm1004vencLineTrapNotUrgentGoesOff NOTIFICATION-TYPE
	OBJECTS {pm1004vencAlmLineDdmWarningPortn, pm1004venctrapLineNumber, pm1004venctrapBoardNumber}
	STATUS current
	DESCRIPTION "Not urgent Line default disappeared"
	::= { pm1004venctraps 31 }

