	OBJECTS {pm1004vencAlmLineDdmAlmPortn, pm1004venctrapLineNumber, pm1004venctrapBoardNumber}
	STATUS current
	DESCRIPTION "Urgent Line default is activated"
	::= { pm1004venctraps 32 }

pm1004vencLineTrapUrgentGoesOff NOTIFICATION-TYPE
	OBJECTS {pm1004vencAlmLineDdmAlmPortn, pm1004venctrapLineNumber, pm1004venctrapBoardNumber}
	STATUS current
	DESCRIPTION "Urgent Line default disappeared"
	::= { pm1004venctraps 33 }

pm1004vencLineTrapCritGoesOn NOTIFICATION-TYPE
	OBJECTS {pm1004vencAlmLineFailPortn , pm1004vencAlmLineHwFailPortn , pm1004venctrapLineNumber, pm1004venctrapBoardNumber}
	STATUS current
	DESCRIPTION "Critical Line default is activated"
	::= { pm1004venctraps 34 }

