PmdgeGrid ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Different grids on line ports"
    SYNTAX INTEGER {
		dgegrid200(200),
		dgegrid100(100),
		dgegrid50(50)
           }

PmdgePlanNumber ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Different values for plans"
    SYNTAX INTEGER {
		workingPlan(0),
		savedPlan1(1),
		savedPlan2(2),
		usedPlan(7)
           }

PmdgeChannel ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Channel selection on line ports"
    SYNTAX INTEGER {

           }

