	OBJECTS {pmdgeAlmDefFuseB, pmdgeAlmDefFuseA , pmdgetrapBoardNumber}
	STATUS current
	DESCRIPTION "Urgent power supply default disappeared"
	::= { pmdgetraps 51 }






