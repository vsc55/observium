    ::= { fabricNameCfg 2 }



fabricBindAll OBJECT-TYPE
    SYNTAX  INTEGER {
        other(1),
        bind(2) 
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "If this is set to bind(2) the binding of all attached members
         will be performed at the next apply.
         other(1) is returned when the object is read."
    ::= {switchNumCfg 3 }


