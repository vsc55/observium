    SYNTAX INTEGER {
        unmapped(0),
        mapped(1)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "The port configuration,
        either unmapped(0) for ports that are not mapped, or
        mapped(1) for mapped ports."
    ::= { portMapEntry 3 }

pmapPortAlias OBJECT-TYPE
    SYNTAX  DisplayString
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The Port Alias."
    ::= { portMapEntry 4 }

