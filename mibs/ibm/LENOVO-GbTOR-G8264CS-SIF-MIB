    failoverCurCfgTriggerMconTrunk     OCTET STRING,
    failoverCurCfgTriggerMconKey       DisplayString,
    failoverCurCfgFabricPathMonitor    INTEGER,
    failoverCurCfgTriggerState         INTEGER
    }

