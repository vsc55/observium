	wlcWlanIfStatTxBytes		Counter32,
	wlcWlanIfStatTxDropped		Counter32,
	wlcWlanIfStatRxPackets		Counter32,
	wlcWlanIfStatRxBytes		Counter32,
	wlcWlanIfStatRxErrors		Counter32,
	wlcWlanIfStatAssociated		INTEGER,
	wlcWlanIfStatOperState	        TruthValue	
    }
    
    wlcWlanIfStatIndex OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
