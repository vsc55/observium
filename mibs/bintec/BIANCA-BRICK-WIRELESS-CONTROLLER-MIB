    wlcVSSStatBSMovements OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
	    "Number of clients moved to other band by band steering mechanism."
    ::= { wlcVSSStatEntry 8 }

    wlcVSSStatClearCounter OBJECT-TYPE
	SYNTAX INTEGER { counting(1), clearing(2) }
	MAX-ACCESS read-write
