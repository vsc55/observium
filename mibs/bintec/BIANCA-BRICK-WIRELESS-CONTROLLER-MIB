	wlcWlanHWTxPowerSteps		DisplayString,
	wlcWlanHWShortRetries		TruthValue,
	wlcWlanHWLongRetries		TruthValue,
	wlcWlanHWFragmentationThreshold	TruthValue,
	wlcWlanHWNoSpatialRx		INTEGER,
	wlcWlanHWNoSpatialTx		INTEGER,
	wlcWlanHWBands			BITS,
	wlcWlanHWModes			BITS,
	wlcWlanHWBandwidth		TruthValue,
	wlcWlanHWShortGI		TruthValue,
	wlcWlanHWGreenfield		TruthValue,
	wlcWlanHWRxSTBC			TruthValue,
	wlcWlanHWTxSTBC			TruthValue,
        wlcWlanHWSkipModes              BITS
    }
    
    wlcWlanHWIndex OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
	    "Unique WLAN hardware index.
	    "
    ::= { wlcWlanHWEntry 1 }
	    
