	    wlanLinkTestRxFramesRemote48    Counter32,
	    wlanLinkTestRxFramesRemote36    Counter32,
	    wlanLinkTestRxFramesRemote24    Counter32,
	    wlanLinkTestRxFramesRemote18    Counter32,
	    wlanLinkTestRxFramesRemote12    Counter32,
