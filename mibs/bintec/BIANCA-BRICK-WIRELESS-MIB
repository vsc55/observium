    wlanStationTxDiscards OBJECT-TYPE
        SYNTAX Counter32
	UNITS  "bytes"
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
	    "Count of discarded bytes due to bandwidth limitation.
             see wlanVSSStationTxShaping"
	DEFVAL { 0 }
	::= { wlanStationEntry 87 }

