	UNITS "dBm"
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
            "This attribute shows the received signal strength at the 
            first receiver in dBm. The value 0 indicates no connection."
	DEFVAL { 0 }
    ::= { wlanStationEntry 55 }
    
    wlanStationRSSI2 OBJECT-TYPE
	SYNTAX  WlanRSSI
	UNITS "dBm"
	MAX-ACCESS  read-only
