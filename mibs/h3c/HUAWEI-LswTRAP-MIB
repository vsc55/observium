      ::= { hwsLswTRAPMibObject 32 }

      hwMemoryUsageEarlyWarningNotification NOTIFICATION-TYPE
      OBJECTS {
            hwLswFrameIndex,
            hwLswSlotIndex,
            hwLswCpuIndex,
