    ::= { h3cDot11StationTraps 10 }

h3cDot11StationAuditTrap NOTIFICATION-TYPE
    OBJECTS
      {
          h3cDot11StationAuditTrapOpType,
          h3cDot11StationTrapStaMAC,
          h3cDot11StationTrapStaIPAddress,
          h3cDot11StationTrapAPMacAddress,
          h3cDot11StationTrapAPIPAddress,
