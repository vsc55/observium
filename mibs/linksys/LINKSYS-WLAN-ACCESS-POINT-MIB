                denyList(1),
                acceptList(2)        
                }         
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "Mode for the MAC access list."
    DEFVAL      { denyList }        
    ::= { apWgBridgeDwstrmEntry 20 }

