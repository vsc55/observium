        apQosDsPolicyMapAttrMarkIpDscp                  Integer32,
        apQosDsPolicyMapAttrMarkIpPrecedence            Integer32,
        apQosDsPolicyMapAttrPoliceSimpleRate            Unsigned32,
        apQosDsPolicyMapAttrPoliceSimpleBurstSize       Integer32,
