     SYNTAX SEQUENCE OF OswDCPowerEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
 	     "DC Power Information Table"
     ::= { oswIdent 7 }

oswDCPowerEntry OBJECT-TYPE
     SYNTAX OswDCPowerEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	     "DC Power Information Table Head"
     INDEX { oswDCPowerIndex }
     ::= { oswDCPowerTable 1 }

OswDCPowerEntry ::=SEQUENCE
     {
          oswDCPowerIndex
               INTEGER,
          oswDCPowerVoltage
               INTEGER,
          oswDCPowerCurrent
               INTEGER,
          oswDCPowerName
               DisplayString
     }

