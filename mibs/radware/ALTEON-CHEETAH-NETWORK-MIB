vrrpNewCfgVirtRtrVrGrpTckIslPort OBJECT-TYPE
        SYNTAX  INTEGER {
                include(1),
                exclude(2)
                }
        MAX-ACCESS  read-create
        STATUS  current
        DESCRIPTION
            "Include or exclude ISL ports in tracking."
        ::= { vrrpNewCfgVirtRtrVrGrpTableEntry 21 }

