    SYNTAX  INTEGER{
        ipv4(1),
        ipv6(2)
    }
    MAX-ACCESS  read-create
    STATUS  current
    DESCRIPTION
        "The floating IP version."
    ::= { haFloatIpNewCfgEntry 2 }

