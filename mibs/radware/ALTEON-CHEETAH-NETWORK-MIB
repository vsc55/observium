ipTunnelNewCfgIndex OBJECT-TYPE
    SYNTAX  Integer32 
    MAX-ACCESS  read-create
    STATUS  current  
    DESCRIPTION
        "The tunnel Index."
    ::= { ipTunnelNewCfgEntry 2 }

ipTunnelNewCfgState OBJECT-TYPE
    SYNTAX  INTEGER {
		enabled(2),
		disabled(3)
	}
    MAX-ACCESS  read-create
    STATUS  current
    DESCRIPTION
        "Enable or Disable the IP tunnel interface."
    ::= { ipTunnelNewCfgEntry 3 }

