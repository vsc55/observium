  SYNTAX  INTEGER {
		disabled(1),
--        vxlan(2),
          ipinip(3),
          gre(4)
  }
    MAX-ACCESS read-only
    STATUS  current
    DESCRIPTION
        "The type of IP tunnel interface."
    ::= { ipTunnelCurCfgEntry 14 }

