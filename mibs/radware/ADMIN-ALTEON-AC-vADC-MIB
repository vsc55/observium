    SYNTAX  INTEGER  {
        ipv4(4),
        ipv6(6)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The vADC allowed network ip version.
         Note:This mib is supported only for VX instance of Virtualization."
    ::= { vADCCurCfgNetTableEntry 3 }

