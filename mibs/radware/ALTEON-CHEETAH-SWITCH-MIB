    SYNTAX  INTEGER 
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "GEL: set pending throughput (Mbps)."
    ::= { agLMLicenseOper 2 }

lmLicOperSubscrAddOn OBJECT-TYPE   
     SYNTAX  INTEGER {   
         ignore(0),   
         perform(2),   
         secure(3)   
        }   
     MAX-ACCESS  read-write   
     STATUS  current   
     DESCRIPTION   
         "GEL: set/ignore pending subsciption add-on."   
     ::= { agLMLicenseOper 5} 

lmLicOperRmkey OBJECT-TYPE
    SYNTAX  INTEGER {
        all(0),       
        perform(2),
        secure(3)
--        idle(6),
--        inprogress(7),
--        complete(8),
--        failed(9) 
       }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "GEL: Enter software feature to be removed."
    ::= { agLMLicenseOper 3}

lmLicOperSynclic OBJECT-TYPE
    SYNTAX  INTEGER  {
        start(0)
--        idle(1),
--        inprogress(2),
--        complete(3),
--        failed(4)
       }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "GEL: Synchronize license with the license server (use new features configured via SNMP if exist)."
--         When this object is read the current state is returned.
--         'idle' indicates that there is no communication with license server in progess.
--         'complete' indicates that the last operation is completed.
--         Setting the value to 'start', will write all the changes if found, and start new syncronize.
--         If succesfull will return the state 'complete' else it will return 'failed'.
--         TBD_LM - In case of failure, to read the reason for failure use the lmLicOperStateTable.
--         Once the operation is completed or the agLicOperStatusTable is read in case
--         of failure, the reason buffer needs to be freed and the lmLicOperState 
--         should be set back to 'idle' state so others can issue the sync command via SNMP."
    ::= { agLMLicenseOper 4}
-- GEL License manager Oper end

