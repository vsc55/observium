    SYNTAX  Integer32 
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The utilization of this VATD over 1 second. It shows the percentage."
    ::= { vatdStatsCpuUtilTableEntry 2 }
 
vatdStatsCpuUtil64Seconds OBJECT-TYPE
    SYNTAX  Integer32 
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The utilization of this VATD over 64 seconds. It shows the percentage."
    ::= { vatdStatsCpuUtilTableEntry 3 }

