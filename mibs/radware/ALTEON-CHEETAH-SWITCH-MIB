    agAccessNewHttpsCfgIntermCertsHashAlgo              INTEGER,
    agAccessNewHttpsCfgIntermCertsCountryName           DisplayString,
    agAccessNewHttpsCfgIntermCertsProvinceName          DisplayString,
    agAccessNewHttpsCfgIntermCertsLocalityName          DisplayString,
    agAccessNewHttpsCfgIntermCertsOrganizationName      DisplayString,
    agAccessNewHttpsCfgIntermCertsOrganizationUnitName  DisplayString,
    agAccessNewHttpsCfgIntermCertsEMail                 DisplayString,
    agAccessNewHttpsCfgIntermCertsValidityPeriod        INTEGER,
    agAccessNewHttpsCfgIntermCertsDelete                INTEGER,
    agAccessNewHttpsCfgIntermCertsGenerate              INTEGER,
    agAccessNewHttpsCfgIntermCertsStatus                INTEGER,
    agAccessNewHttpsCfgIntermCertsKeyType               INTEGER,
    agAccessNewHttpsCfgIntermCertsKeySizeEc             INTEGER,
    agAccessNewHttpsCfgIntermCurveNameEc                INTEGER,
    agAccessNewHttpsCfgIntermCertsKeySizeCommon         INTEGER
    }

