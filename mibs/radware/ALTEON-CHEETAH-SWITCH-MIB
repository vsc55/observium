agLastSyncInfoTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AgLastSyncInfoTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "The table of Last Sync Information."
    ::= { agentInfo 13 }

agLastSyncInfoTableEntry OBJECT-TYPE
    SYNTAX  AgLastSyncInfoTableEntry
    MAX-ACCESS not-accessible
    STATUS  current
    DESCRIPTION
        "A row in the Last Sync Information table."
    INDEX  { lastSyncInfoIdx }
    ::= { agLastSyncInfoTable 1 }

AgLastSyncInfoTableEntry ::=
    SEQUENCE {
        lastSyncInfoIdx                Integer32,
        lastSyncPeerIp                 DisplayString,
        lastSyncType                   INTEGER,
        lastSyncStatus                 INTEGER,
        lastSyncTime                   DisplayString,
        lastSyncFailReason             DisplayString,
        lastSuccessfulSyncType         INTEGER,
        lastSuccessfulSyncTime         DisplayString
    }

lastSyncInfoIdx OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Last sync table peer index."
    ::= { agLastSyncInfoTableEntry 1 }

lastSyncPeerIp OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Last sync peer ip address."
    ::= { agLastSyncInfoTableEntry 2 }

lastSyncType OBJECT-TYPE
    SYNTAX  INTEGER {
        none(1),
        manual(2),
        auto(3)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Last sync type - none(1),manual(2) and auto(3)."
    ::= { agLastSyncInfoTableEntry 3 }

lastSyncStatus OBJECT-TYPE
    SYNTAX  INTEGER {
        idle(1),
        in-progress(2),
        success(3),
        failure(4)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Status of last sync sent- idle(1), in-progress(2),success(3) and failure(4)."
    ::= { agLastSyncInfoTableEntry 4 }

lastSyncTime OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(0..127))
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Timestamp for last sync sent."
    ::= { agLastSyncInfoTableEntry 5 }

lastSyncFailReason OBJECT-TYPE
    SYNTAX  DisplayString
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Failure reason for last sync."
    ::= { agLastSyncInfoTableEntry 6 }

lastSuccessfulSyncType OBJECT-TYPE
    SYNTAX  INTEGER {
        none(1),
        manual(2),
        auto(3)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Type of the last successful sync sent - none(1), manual(2), auto(3)."
    ::= { agLastSyncInfoTableEntry 7 }

lastSuccessfulSyncTime OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(0..127))
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Timestamp for last successful sync sent."
    ::= { agLastSyncInfoTableEntry 8 }

-- end sync stat

