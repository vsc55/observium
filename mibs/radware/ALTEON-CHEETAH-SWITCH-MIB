-- Pblade Capacity info table
switchCapPbladeInfoTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF SwitchCapPbladeInfoTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "Pblade Capacity information table"
    ::= { switchCapPbladeInfo 1 }

switchCapPbladeInfoTableEntry OBJECT-TYPE
    SYNTAX  SwitchCapPbladeInfoTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "A row in the Pblade Capacity information table"
    INDEX  { switchCapPbladeInfoPbladeId }
    ::= { switchCapPbladeInfoTable 1 }

SwitchCapPbladeInfoTableEntry ::= SEQUENCE {
    switchCapPbladeInfoPbladeId         INTEGER,
    switchCapPbladeInfoRamMax           INTEGER,
    switchCapPbladeInfoRamCur           INTEGER
    }

switchCapPbladeInfoPbladeId OBJECT-TYPE
    SYNTAX  INTEGER (3..6)
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Pblade slot number"
    ::= { switchCapPbladeInfoTableEntry 1 }

switchCapPbladeInfoRamMax OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Pblade total RAM size (GB)"
    ::= { switchCapPbladeInfoTableEntry 2 }

switchCapPbladeInfoRamCur OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Pblade used RAM size (GB)"
    ::= { switchCapPbladeInfoTableEntry 3 }

agLicenseInfoTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AgLicenseInfoTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "The table of license Information."
    ::= { agLicenseInfo 1 }

agLicenseInfoTableEntry OBJECT-TYPE
    SYNTAX  AgLicenseInfoTableEntry
    MAX-ACCESS not-accessible
    STATUS  current
    DESCRIPTION
        "A row in the license Information table."
    INDEX  { licenseInfoIdx }
    ::= { agLicenseInfoTable 1 }

AgLicenseInfoTableEntry ::=
    SEQUENCE {
        licenseInfoIdx                        Integer32,
        softwareKey                           DisplayString,
        licenseType                           INTEGER,
        remainingDays                         INTEGER,
        remainingDaysLeft                     DisplayString,
        licenseSize                           Integer32,
        licenseAllocated                      Integer32,
        timeBasedLicenseStatus                DisplayString
    }

licenseInfoIdx OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        " The license index."
    ::= { agLicenseInfoTableEntry 1 }

softwareKey OBJECT-TYPE
    SYNTAX DisplayString(SIZE(0..31))
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The Sofware Key."
    ::= { agLicenseInfoTableEntry 2 }

licenseType OBJECT-TYPE
    SYNTAX INTEGER {
        permanent(1),
        temporary(2),
        removed(3),
        expired(4),
        subscription(5)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The License Type information."
    ::= { agLicenseInfoTableEntry 3 }

remainingDays OBJECT-TYPE
    SYNTAX INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The number of days left for temporary license."
    ::= { agLicenseInfoTableEntry 4 }

remainingDaysLeft OBJECT-TYPE
SYNTAX DisplayString(SIZE(0..255))
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
    "The number of days/hours left for temporary license."
    ::= { agLicenseInfoTableEntry 5 }

licenseSize OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
