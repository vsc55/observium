agNewCfgSyslogSessLogMode OBJECT-TYPE
    SYNTAX  INTEGER {
        syslog(1),
        disk(2),
        both(3)
        }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "This object is used to select syslog session mode.
        syslog(1)  - send the session log details to syslog server only.
        disk(2)    - write the session log details to disk(file).
        both(3)    - session log details send to syslog server and write to disk(file)"
    ::= { agSyslog 94 }

agCurCfgSyslogSessLogMode OBJECT-TYPE
    SYNTAX  INTEGER {
        syslog(1),
        disk(2),
        both(3)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "This object is used to select syslog session mode.
        syslog(1)  - send the session log details to syslog server only.
        disk(2)    - write the session log details to disk(file).
        both(3)    - session log details send to syslog server and write to disk(file)"
    ::= { agSyslog 95 }

