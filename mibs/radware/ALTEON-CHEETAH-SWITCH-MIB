    SYNTAX  INTEGER {
        ipv4(4),
        ipv6(6)
        }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "The type of the IP address of the peer (IPv4 | IPv6)"
    ::= { agSysPeerNewCfgEntry 8 }

