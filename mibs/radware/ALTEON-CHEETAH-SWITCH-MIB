vatdStatsCpuUtilTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF VatdStatsCpuUtilTableEntry 
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "The table of VATD CPU utilization."
    ::= { vatdStats 1 }

vatdStatsCpuUtilTableEntry OBJECT-TYPE
    SYNTAX  VatdStatsCpuUtilTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "A row in the VATD CPU utilization table."
    INDEX   { vatdStatsCpuUtilVatdIndex }
    ::= { vatdStatsCpuUtilTable 1 }

VatdStatsCpuUtilTableEntry ::= SEQUENCE {
    vatdStatsCpuUtilVatdIndex    Integer32,
    vatdStatsCpuUtil1Second    Integer32,
    vatdStatsCpuUtil64Seconds  Integer32
    }

