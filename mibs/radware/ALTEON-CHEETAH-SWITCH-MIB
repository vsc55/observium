-- fan slot information table

hwFanInfoTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF HwFanInfoTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "The table of fan slot information."
    ::= { hardware 31 }

hwFanInfoTableEntry OBJECT-TYPE
    SYNTAX  HwFanInfoTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "A row in the fan information table."
    INDEX   { hwFanInfoIndex }
    ::= { hwFanInfoTable 1 }

HwFanInfoTableEntry ::= SEQUENCE {
    hwFanInfoIndex              Integer32,
    hwFanInfoStatus		INTEGER,
    hwFanInfoTotalCount		INTEGER,
    hwFanInfoCountFailed	INTEGER,
    hwFanInfoIsCritical     INTEGER
}

hwFanInfoIndex OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "fan slot index."
    ::= { hwFanInfoTableEntry 1 }

hwFanInfoStatus OBJECT-TYPE
    SYNTAX  INTEGER {
        notRelevant(0),
        ok(1),
        failed(2),
        unplugged(3),
		empty(4)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
		"The status of the fan slot."
    ::= { hwFanInfoTableEntry 2 }
	
hwFanInfoTotalCount OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
		"The total count of fans per slot."
    ::= { hwFanInfoTableEntry 3 }

hwFanInfoCountFailed OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
		"The total count of failed fans."
    ::= { hwFanInfoTableEntry 4 }

hwFanInfoIsCritical OBJECT-TYPE
    SYNTAX INTEGER {
        no(0),
        yes(1),
        notRelevant(2)
    }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
		"A flag indicates whether the fan is critical or not."
    ::= { hwFanInfoTableEntry 5 }

