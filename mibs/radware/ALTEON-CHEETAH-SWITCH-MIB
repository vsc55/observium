spMaintStatsClear OBJECT-TYPE
    SYNTAX INTEGER {
         ok(1),
         clear(2)
         }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "This is an action object to clear maintenance statistics
         on the particular SP.
         ok(1) is returned when read."
    ::= { spMaintStatsTableEntry 43 }


spMemUsageStatsTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF SpMemUsageStatsTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "The table of SP memory statistics.
        Note: This MIB table is not supported for VX instance of Virtualization."
    ::= { spStats 4 }

spMemUsageStatsTableEntry OBJECT-TYPE
    SYNTAX  SpMemUsageStatsTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "A row in the SP memory statistics table."
    INDEX   { spMemUsageStatsIndex }
    ::= { spMemUsageStatsTable 1 }

SpMemUsageStatsTableEntry ::= SEQUENCE {
    spMemUsageStatsIndex             Integer32,
    spMemUsageStatsCurrentMemory     Integer32,
    spMemUsageStatsHiWaterMark       Integer32,
    spMemUsageStatsMaxMemory       Integer32
    }

spMemUsageStatsIndex OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION
        "The SP index."
    ::= { spMemUsageStatsTableEntry 1 }

spMemUsageStatsCurrentMemory OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION
        "The current memory usage of SP in KiloBytes."
    ::= { spMemUsageStatsTableEntry 2 }

spMemUsageStatsHiWaterMark OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION
        "The high watermark of memory usage in KiloBytes."
    ::= { spMemUsageStatsTableEntry 3 }

spMemUsageStatsMaxMemory   OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION
        "Allowed Max in KiloBytes."
    ::= { spMemUsageStatsTableEntry 4 }

-- SP mem usage start

spMemUseStatsTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF SpMemUseStatsTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "The table of SP memory statistics.
        Note: This MIB table is not supported for VX instance of Virtualization."
    ::= { spStats 5 }

spMemUseStatsTableEntry OBJECT-TYPE
    SYNTAX  SpMemUseStatsTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "A row in the SP memory statistics table."
    INDEX   { spMemUseStatsIndex }
    ::= { spMemUseStatsTable 1 }

SpMemUseStatsTableEntry ::= SEQUENCE {
	spMemUseStatsIndex             	Integer32,
	spMemUseStatsInitProcessSize 	Counter32,	
	spMemUseStatsInitMaxProcessSize Counter32,
	spMemUseStatsInitSizeTo1Margin  Counter32,
	spMemUseStatsInitSizeTo2Margin  Integer32,	
	spMemUseStatsAllowMemIncrease   Counter32,	
	spMemUseStatsCurProcSize     	Counter32,
	spMemUseStatsCurProcShareSize  	Counter32,	
	spMemUseStatsCurProcResidentSize  Counter32,
	spMemUseStatsCurFrontEndSessions  Counter32,
	spMemUseStatsAvgFrontEndSessions  Counter32,
	spMemUseStatsPeakUsageFrom1stMargin  	Counter32,
	spMemUseStatsMemPressStat		Counter32,
	spMemUseStatsMemUseFrom1stMargin	Counter32,
	spMemUseStatsCurProcCacheRelativeSize 	Counter32,
	spMemUseStatsCurProcDynCertRelativeSize Counter32,	
	spMemUseStatsMemPressActiveTime		Counter32,
	spMemUseStatsMaxAllowConnections	Counter32,
	spMemUseStatsProcOver1stMargin		INTEGER,
	spMemUseStatsProcOver2ndMargin		INTEGER,
	spMemUseStatsMemTo2ndMargin		Integer32,
        spMemUseStatsCurExtraProcessRelativeSize Counter32,
        spMemUseStatsCurQatSlabsRelativeSize 	Counter32
    }

spMemUseStatsIndex OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The SP index."
    ::= { spMemUseStatsTableEntry 1 }
    		
-- Memory stats per SP

spMemUseStatsInitProcessSize   OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Initial process size (RES-SHR)(Mb)."
    ::= { spMemUseStatsTableEntry 2 } 	
	
