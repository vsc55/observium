    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Current process resident size (RES)(Mb)."
    ::= { spMemUseStatsTableEntry 9 } 

spMemUseStatsCurFrontEndSessions   OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Current front-end sessions."
    ::= { spMemUseStatsTableEntry 10 } 	
	
spMemUseStatsAvgFrontEndSessions   OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Average front-end session size(Kb)."
    ::= { spMemUseStatsTableEntry 11 } 

spMemUseStatsPeakUsageFrom1stMargin   OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Peak memory used from 1st watermark(%)."
    ::= { spMemUseStatsTableEntry 12 } 		
	
spMemUseStatsMemPressStat   OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Memory pressure status (On/Off)."
    ::= { spMemUseStatsTableEntry 13 } 	
	
spMemUseStatsMemUseFrom1stMargin   OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Memory used from 1st watermark (%)."
    ::= { spMemUseStatsTableEntry 14 } 		
	
spMemUseStatsCurProcCacheRelativeSize   OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Current process cache relative size (Mb)."
    ::= { spMemUseStatsTableEntry 15 } 
	
