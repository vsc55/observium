SlbNewCfgEnhVirtServicesSixthPartEntry ::= SEQUENCE {

    slbNewCfgEnhVirtServSixthPartIndex            DisplayString,
    slbNewCfgEnhVirtServiceSixthPartIndex         Integer32,
