    SYNTAX INTEGER {
        other(1),
        delete(2)
        }
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "DNS SOA new config zone entry delete operation by setting (2), (1) is ignored"
    ::= { gslbNewDnsSoaZoneEntry 6 }

-- DNS SOA End

-- DNS CAA Start
