    SYNTAX  DisplayString (SIZE(1..127))
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "DNS CAA config Record entry alphanumeric index."
    ::= { gslbCurDnsCaaRecordEntry 1 }

