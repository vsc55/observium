        issue(0),
        issuewild(1),
        iodef(2)
    }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "DNS CAA config Record entry Record Type."
    ::= { gslbCurDnsCaaRecordEntry  3 }

