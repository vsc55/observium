    SYNTAX  INTEGER {
        layer3Only(1),
        disabled(2)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Enable or disable layer3 only balancing."
    ::= { slbCurCfgEnhVirtualServerEntry 3 }

