slbCurCfgSlbTso OBJECT-TYPE
    SYNTAX  INTEGER {
        enabled(1),
        disabled(2)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Enable/Disable TCP Segmentation Offload.
         Note:This mib is supported only on VA Platform."
    ::= { slbGeneralCfg 108 }

