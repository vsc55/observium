    SYNTAX  INTEGER {
        allow(1),
        deny(2),
        redirect(3),
        nat(4),      -- network address translation
        goto(5),
	outbound-llb(6)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
