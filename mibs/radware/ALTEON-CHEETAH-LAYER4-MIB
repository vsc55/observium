SlbCurDataclassCfgDataClassesEntry ::= SEQUENCE {
    slbCurDataclassCfgDataClassesId          DisplayString,
    slbCurDataclassCfgDataClassesDataType    INTEGER,


    slbCurDataclassCfgDataClassesName        DisplayString
    }

