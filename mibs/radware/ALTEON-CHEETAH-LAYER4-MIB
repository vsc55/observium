    SYNTAX INTEGER{
        low(0),
        medium(1),
        high(2),
        user-defined(3),
        user-defined-expert(4),
        main(5)
        }
    MAX-ACCESS  read-create
    STATUS  current
    DESCRIPTION
        "Allowed cipher-suites in backend SSL."
    ::= {slbNewSslCfgSSLPolEntry  14 }

