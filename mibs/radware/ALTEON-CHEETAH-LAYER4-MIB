    SYNTAX  INTEGER {
            regular(1),
            read-only(2)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Group config type either regular or read-only."
    ::= { slbNewSslCfgGroupsEntry  9 }

