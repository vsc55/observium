    SYNTAX  INTEGER (0..4090)
    MAX-ACCESS  read-create
    STATUS  current
    DESCRIPTION
        "The ingress VLAN to be associated with IDS server."
    ::= { slbNewCfgEnhRealServerSecondPartEntry 21 }


