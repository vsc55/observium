    gslbCurDnsCaaRecordDomainName   DisplayString,
    gslbCurDnsCaaRecordType         INTEGER,
    gslbCurDnsCaaRecordValue        DisplayString,
    gslbCurDnsCaaRecordTtl          INTEGER
    }

