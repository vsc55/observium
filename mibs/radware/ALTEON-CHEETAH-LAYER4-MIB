    SYNTAX  INTEGER {
        enabled(1),
        disabled(2)
        }
    MAX-ACCESS read-create
    STATUS  current
    DESCRIPTION
        "Enable or Disable override maximum connections limit."
    ::= { slbNewCfgEnhGroupEntry 37 }



