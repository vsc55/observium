      "Table containing the list of immediate chacks"
    ::=  {rsIDSImmediateChecks 1}

	rsIDSImmediateChecksPacketTraceStatus OBJECT-TYPE
	SYNTAX INTEGER {
			enable(1),
			disable(2)
			}
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
      "Packet Trace status for Immediate Checks "
    DEFVAL {2}
    ::=  {rsIDSImmediateChecks 2}
	
