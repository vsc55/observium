    SYNTAX INTEGER {
			enable(1),
			disable(2)}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
       "This variable indicates the per-attack packet report"
	DEFVAL  {disable}
    ::=  {rsIDSServAttackEntry 5}

