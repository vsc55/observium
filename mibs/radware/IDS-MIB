      minor(1),    
      low(2),
      medium(3),
      high(4)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
      "Http Flood Server detection sensitiviy."
    DEFVAL {3}
    ::=  { rsHTTPFServerEntry 3 }


