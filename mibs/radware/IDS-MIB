    SYNTAX INTEGER {
		 report-only(0),
	     drop(10),
	     rst-src(11),
     	 rst-dst(12),
    	 rst-bi(13)}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
       "This variable indicates the attack report mode"
    ::=  { rsIDSServAttackEntry 4}

