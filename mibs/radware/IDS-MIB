    SYNTAX INTEGER {
			enable(1),
			disable(2)
			}
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
      "SIP Server Uri in Tcp"
    DEFVAL {2}
    ::= {rsIDSServProtection 8}

-- Immediate Checks

