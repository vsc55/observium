    SYNTAX INTEGER {
			enable(1),
			disable(2)
			}
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
      "SIP Server Originated Sessions"
    DEFVAL {2}
    ::= {rsIDSServProtection 7}

