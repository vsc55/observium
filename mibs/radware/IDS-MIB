    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
      "the log file polling time frequency"
    ::= {rsIDS 34}

 rsIDSSendLogFile OBJECT-TYPE
    SYNTAX DisplayString
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
        "The file name include path where the device will put the
         log file. The default destination address will be the sender
         address."
    ::= { rsIDS 35 }

