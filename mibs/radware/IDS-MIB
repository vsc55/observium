    SYNTAX INTEGER {
			no-report(0),
			report(1),
			block(2)}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "The immediate check attack action."
    ::=  {rsIDSImmediateChecksAttackEntry 3}

rsIDSImmChkRisk OBJECT-TYPE
    SYNTAX INTEGER {
	  info(1),
	  low(2),
      medium(3),
	  high(4)}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
       "The immediate check report mode."
    ::=  {rsIDSImmediateChecksAttackEntry 4}

