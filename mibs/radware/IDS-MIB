	rsIDSImmChkId INTEGER,
	rsIDSImmChkName DisplayString,
	rsIDSImmChkAction INTEGER,
	rsIDSImmChkRisk INTEGER,
	rsIDSImmChkStatus RowStatus,
	rsIDSImmChkReportAction INTEGER
}

