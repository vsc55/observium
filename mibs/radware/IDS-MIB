    SYNTAX INTEGER {
		  info(1),
		  low(2),
       	  medium(3),
		  high(4)}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
       "This variable indicates the risk of the attack"
	DEFVAL  {medium}
    ::=  {rsIDSServAttackEntry 6}

