rsIDSLogAttackStatus OBJECT-TYPE
    SYNTAX INTEGER {
	  -- discrete --
      started(1),
      terminated(2),
			occur(3),          
	  -- limited --
      sampled(6),
	  -- aggregated --
      occured(11),
	  -- aggressive --
	  ongoing(16)
   }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The status of the attack(started/terminated/occured...)"
    ::=  {rsIDSLogEntry 5}

