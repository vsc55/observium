   SYNTAX INTEGER {
			allow(1),
			deny(2)}
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"Reassembly Overlap Status"
	DEFVAL { allow }
    ::= { rsIDSIpFragments 6 }
    
    
