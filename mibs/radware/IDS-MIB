   SYNTAX INTEGER {
		 report-only(0),
	     drop(10)}
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"Reassembly Overlap mode"
	DEFVAL { report-only }
    ::= { rsIDSIpFragments 7 }

   
    
