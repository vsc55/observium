    SYNTAX INTEGER {
--        ip (1),
        tcp (2),
        udp (3)
--        icmp (4),
--        nonIp (5)
    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
       "This variable indicates the protocol that the application port relate to"
	DEFVAL  {tcp}
    ::=  { rsIDSConnectionLimitAttackEntry 4}

