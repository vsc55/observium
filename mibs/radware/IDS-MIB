 SYNTAX INTEGER {
      enable(1),
      disable(2)
   }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
      "packet trace status"
   DEFVAL  {disable}
    ::= {rsIDSServProtProfilesEntry 8}	

