    SYNTAX INTEGER {
		  high(0),
       	  medium(1),
		  low(2),
		  minor(3)
	}
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
      "The sensitivity of the attack in the profile."
    ::=  { rsIDSServProtProfilesEntry 5 }

