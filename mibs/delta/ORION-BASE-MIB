        SYNTAX INTEGER {
                unknown(1),
                eventOR(2) }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Type of the input event."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.8.2.1.4 --  
 ::= {  dcEventProcessingEventEntry 4 }

-- ====================================================================
-- LVD Config table
-- ====================================================================
dcLvdTable OBJECT-TYPE
        SYNTAX SEQUENCE OF DcLvdEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
                "Table lists all available LVDs."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.8.4 --  
 ::= { dcConfig 4 }

dcLvdEntry OBJECT-TYPE
        SYNTAX DcLvdEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
                "A particular LVD entry."
        INDEX { dcLvdIndex }
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.8.4.1 --  
 ::= { dcLvdTable 1 }

DcLvdEntry ::= SEQUENCE {
        dcLvdIndex Integer32,
        dcLvdName DisplayString,   
        dcLvdDisconnectDelay DisplayString,
		dcLvdType INTEGER, 
		dcLvdVoltageThreshold Integer32,	
		dcLvdVoltageHysteresis Integer32,
		dcLvdControlEvent DisplayString,	
		dcLvdMonitoringEvent DisplayString}

dcLvdIndex OBJECT-TYPE
        SYNTAX Integer32 (1..96)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
                "Table index."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.8.4.1.1 --  
 ::= { dcLvdEntry 1 }

dcLvdName OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..16))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Name of the LVD."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.8.4.1.2 --  
 ::= { dcLvdEntry 2 }

dcLvdDisconnectDelay OBJECT-TYPE
        SYNTAX DisplayString (SIZE (8))
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
                "Time delay from the activation of the source event, after 
                which the LVD event is switched. The value is in the format 
                'hh:mm:ss'."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.8.4.1.3 -- 
 ::= {  dcLvdEntry 3 }
 
 dcLvdType OBJECT-TYPE
        SYNTAX INTEGER {
                eventControlled(1),
                utControlled(2) }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Type of the LVD."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.8.4.1.4 --  
 ::= {  dcLvdEntry 4 }
 
dcLvdVoltageThreshold OBJECT-TYPE
        SYNTAX Integer32 
        UNITS "10 mV"
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
                "Voltage threshold for U and t controlled type LVD. Only available in U and t Controlled Type LVDs."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.8.4.1.5 -- 
 ::= {  dcLvdEntry 5 }
 
dcLvdVoltageHysteresis OBJECT-TYPE
        SYNTAX Integer32 
        UNITS "10 mV"
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
                "Voltage Hysteresis for U and t controlled type LVD. Only available in U and t Controlled Type LVDs."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.8.4.1.6 -- 
 ::= {  dcLvdEntry 6 }
 
  dcLvdControlEvent OBJECT-TYPE
        SYNTAX DisplayString (SIZE (0..35))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Control (previously named Source) Event for the LVD action. Only available in Event Controlled Type LVDs."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.8.4.1.7 -- 
 ::= {  dcLvdEntry 7 }
 
 dcLvdMonitoringEvent OBJECT-TYPE
        SYNTAX DisplayString (SIZE (0..35))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Monitoring Event. Only available in Event Controlled Type LVDs."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.8.4.1.8 -- 
 ::= {  dcLvdEntry 8 }

-- ====================================================================
-- EventDefinition table
-- ====================================================================
dcEventDefinitionTable OBJECT-TYPE
        SYNTAX SEQUENCE OF DcEventDefinitionEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
                "Table that shows twenty threshold event assignments that can be 
                 configured via WEB interface."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.8.5 --  
