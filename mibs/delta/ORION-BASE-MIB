-- ====================================================================
-- Event controlled charge table
-- ====================================================================
dcEventControlledChargeTable OBJECT-TYPE
        SYNTAX SEQUENCE OF DcEventControlledChargeEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
                "Lists all the configured Event Controlled Charges elements."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.7.4 --  
 ::= { dcEvtCtrlCharge 4 }

dcEventControlledChargeTableEntry OBJECT-TYPE
        SYNTAX DcEventControlledChargeEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
                ""
        INDEX { dcEventControlledChargeTableIndex }
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.7.4.1 --  
 ::= { dcEventControlledChargeTable 1 }

DcEventControlledChargeEntry ::= SEQUENCE {
				dcEventControlledChargeTableIndex Integer32,
				dcEventControlledChargeTablePriority Unsigned32,
				dcEventControlledChargeTableName DisplayString,
				dcEventControlledChargeTableActivationInput DisplayString,
				dcEventControlledChargeTableStatus INTEGER,
				dcEventControlledChargeTableType INTEGER,
				dcEventControlledChargeTableVoltage Integer32,
				dcEventControlledChargeTableMaxIBatt Integer32,
				dcEventControlledChargeTableTempCompEnabled INTEGER }

dcEventControlledChargeTableIndex OBJECT-TYPE
        SYNTAX Integer32 (1..10000)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
                "Table index."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.7.4.1.1 --   
 ::= { dcEventControlledChargeTableEntry 1 }

dcEventControlledChargeTablePriority OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Priority of the event controlled charge instance. Used as index for this table."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.7.4.1.2 -- 
 ::= { dcEventControlledChargeTableEntry 2 }

dcEventControlledChargeTableName OBJECT-TYPE
        SYNTAX DisplayString (SIZE (32))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Event control charge name."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.7.4.1.3 -- 
 ::= { dcEventControlledChargeTableEntry 3 }
 
 dcEventControlledChargeTableActivationInput OBJECT-TYPE
        SYNTAX DisplayString (SIZE (32))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Event used to activate the controlled charge."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.7.4.1.4 -- 
 ::= { dcEventControlledChargeTableEntry 4 }
 
