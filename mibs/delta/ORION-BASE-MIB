-- file: ORION-BASE-MIB.mib
-- This MIB file include couple special unicode(UTF-8) characters: ACUTE ACCENT and DEGREE SIGN

ORION-BASE-MIB DEFINITIONS ::= BEGIN

IMPORTS
        DisplayString,
		TEXTUAL-CONVENTION
                FROM SNMPv2-TC
        OBJECT-GROUP,
        NOTIFICATION-GROUP,
        MODULE-COMPLIANCE
                FROM SNMPv2-CONF
        modules,
        orion
                FROM GLOBAL-REG
		InetAddress
				FROM INET-ADDRESS-MIB
        enterprises,
        MODULE-IDENTITY,
        OBJECT-TYPE,
        NOTIFICATION-TYPE,
        Gauge32,
        Integer32,
        IpAddress,
        Unsigned32
                FROM SNMPv2-SMI;

orionBaseMibModule MODULE-IDENTITY
        LAST-UPDATED "201604291257Z"
        ORGANIZATION "Delta Energy Systems (Switzerland) AG"
        CONTACT-INFO
                "postal: Delta Energy Systems (Switzerland) AG
                         Freiburgstrasse 251,
                         CH-3018 Bern-Buempliz
                 web:    www.deltapowersolutions.com
                 email:  adrian.pluess@delta-es.com"
        DESCRIPTION
                "MIB for managing a DC power system with ORION. V6.60B22"

 -- 1.3.6.1.4.1.20246.2.1.1.2 --  
 ::= { modules 2 }
	
-- root for items in the controller MIB module

