-- ====================================================================
-- PVC table
-- ====================================================================
dcPVCTable OBJECT-TYPE
        SYNTAX SEQUENCE OF DcPVCEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
                "Table that shows all currently active PVC modules."
-- 1.3.6.1.4.1.20246.2.3.1.1.1.2.11.4 -- 
 ::= { dcPVC 4 }

dcPVCEntry OBJECT-TYPE
        SYNTAX DcPVCEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
                "A particular PVC entry."
        INDEX { dcPVCIndex } 
  -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.11.4.1 -- 
 ::= { dcPVCTable 1 }

DcPVCEntry ::= SEQUENCE {
        dcPVCIndex         Integer32, 
        dcPVCIdentifier    DisplayString, 
        dcPVCSlotState     INTEGER, 
        dcPVCMainStatus    INTEGER,
        dcPVCSubStatus     INTEGER,
        dcPVCConfiguration INTEGER,
        dcPVCIout          Integer32,
        dcPVCUout          Integer32,
        dcPVCIin           Integer32,
        dcPVCUin           Integer32}

dcPVCIndex OBJECT-TYPE
        SYNTAX Integer32 (1..32)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
                "Table index."
   -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.11.4.1.1 -- 
 ::= { dcPVCEntry 1 }
                               
dcPVCIdentifier OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..16))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Unique PVC identifier. If positioning active the 
                position number scheme is used otherwise the PVC 
                instance is used to identify a PVC."
   -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.11.4.1.2 --  
 ::= { dcPVCEntry 2 }

dcPVCSlotState OBJECT-TYPE
        SYNTAX INTEGER {
                noPos(1),
                empty(2),
                lost(3),
                new(4),
                off(5),
                on(6) }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Specifies state of slots."
   -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.11.4.1.3 --  
 ::= { dcPVCEntry 3 }
 
dcPVCMainStatus OBJECT-TYPE
        SYNTAX INTEGER {
                unknown(1),
                on(2),
                remoteOff(3),
                off(4),
                temporaryInternalOff(5),
                latchedInternalOff(6),
                error(7) }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Specifies status of PVCs."
   -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.11.4.1.4 -- 
 ::= { dcPVCEntry 4 }
 
 dcPVCSubStatus OBJECT-TYPE
        SYNTAX INTEGER {
                unknown(1),
                notAvailable(2),
                mppt(3),
                voltageMode(4),
                outputCurrentLimit(5),
                outputPowerLimit(6),
                inputCurrentLimit(7),
                inputPowerLimit(8),
                inputVoltageOutsideRange(9),
                sunset(10),
                breakerOpen(11),
                startUpDelay(12),
                otp(13),
                ovp(14),
                fanFailure(15) }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Specifies sub-status of PVCs."
   -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.11.4.1.5 -- 
 ::= { dcPVCEntry 5 }
 
 dcPVCConfiguration OBJECT-TYPE
        SYNTAX INTEGER {
                unknown(1),
                ok(2),
                default(3),
                outsideLimts(4),
                invalid(5) }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Specifies configuration of PVCs."
   -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.11.4.1.6 -- 
 ::= { dcPVCEntry 6 }
 
 dcPVCIout OBJECT-TYPE
        SYNTAX Integer32
        UNITS "100 mA"
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "The measured PVC output current. The max. value 2147483647 
                is used to indicate an invalid value."
   -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.11.4.1.7 -- 
 ::= { dcPVCEntry 7 }
 
 dcPVCUout OBJECT-TYPE
        SYNTAX Integer32
        UNITS "10 mV"
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "The measured PVC output voltage. The max. value 2147483647 
                is used to indicate an invalid value."
   -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.11.4.1.8 -- 
 ::= { dcPVCEntry 8 }
 
  dcPVCIin OBJECT-TYPE
        SYNTAX Integer32
        UNITS "100 mA"
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "The measured PVC input current. The max. value 2147483647 
                is used to indicate an invalid value."
   -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.11.4.1.9 -- 
 ::= { dcPVCEntry 9 }
 
 dcPVCUin OBJECT-TYPE
        SYNTAX Integer32
        UNITS "100 mV"
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "The measured PVC input voltage. The max. value 2147483647 
                is used to indicate an invalid value."
   -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.11.4.1.10 -- 
 ::= { dcPVCEntry 10 }
    
