dcBatteryLithiumEntry OBJECT-TYPE
        SYNTAX DcBatteryLithiumEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
                "A particular Lithium battery entry."
        INDEX { dcBatteryLithiumIndex }
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.3.4.1 --  
 ::= { dcBatteryLithiumTable 1 }

DcBatteryLithiumEntry ::= SEQUENCE {
        dcBatteryLithiumIndex            Integer32,
        dcBatteryLithiumName             DisplayString,
        dcBatteryLithiumMainState        INTEGER,
        dcBatteryLithiumSubState         INTEGER,
        dcBatteryLithiumCurrent          Integer32,
        dcBatteryLithiumStateOfCharge    Gauge32,
        dcBatteryLithiumInstallationDate DisplayString,
        dcBatteryLithiumSoH              Gauge32 }

dcBatteryLithiumIndex OBJECT-TYPE
        SYNTAX Integer32 (1..16)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
                "Table index."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.3.4.1.1 --  
 ::= { dcBatteryLithiumEntry 1 }
 
dcBatteryLithiumName OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..16))        
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Name of the Lithium battery."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.3.4.1.2 --
 ::= { dcBatteryLithiumEntry 2 }

dcBatteryLithiumMainState OBJECT-TYPE
        SYNTAX INTEGER {
          invalid(0),     
          unknown(1),
          missing(2),
          ok(3),
          warning(4),
          alarm(5),
          error(6),
          remoteOff(7)}
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Lithium Battery Main State."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.3.4.1.3 -- 
 ::= { dcBatteryLithiumEntry 3 } 
 
dcBatteryLithiumSubState OBJECT-TYPE
        SYNTAX INTEGER {
                invalid(0), 
                na(1),                      
                unknown(2),
                float(3),
                charge(4),
                discharge(5),
                highvoltage(6),
                lowvoltage(7),
                temperature(8),
                highcurrent(9),
                internalfailure(10),
                lowSoC(11)}
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Lithium Battery Sub-State."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.3.4.1.4 -- 
 ::= { dcBatteryLithiumEntry 4 } 
 
dcBatteryLithiumCurrent OBJECT-TYPE
        SYNTAX Integer32        
        UNITS "100 mA"
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Lithium battery current"
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.3.4.1.5 --  
 ::= { dcBatteryLithiumEntry 5 }
 
dcBatteryLithiumStateOfCharge OBJECT-TYPE
        SYNTAX Gauge32        
        UNITS "%"
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Lithium battery state of charge"
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.3.4.1.6 --  
 ::= { dcBatteryLithiumEntry 6 }
 
dcBatteryLithiumInstallationDate OBJECT-TYPE
        SYNTAX DisplayString (SIZE (1..11))        
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "Installation Date of the Lithium battery."
 -- 1.3.6.1.4.1.20246.2.3.1.1.1.2.5.3.4.1.7 --
 ::= { dcBatteryLithiumEntry 7 }
   
