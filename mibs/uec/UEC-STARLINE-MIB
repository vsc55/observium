            ::= { cpmDcGeneral 7 }

        cpmDcEnergyReset  OBJECT-TYPE
            SYNTAX      DisplayString   -- 1 character max
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Energy reset. Set to 1 to issue reset energy command."
            ::= { cpmDcGeneral 8 }

        --  End of General.


