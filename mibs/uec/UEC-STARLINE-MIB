                ::= { cpmDcDigitalIoEntry 4 }

            cpmDcDigitalIoLevel  OBJECT-TYPE
                SYNTAX      DisplayString   -- 1 character max
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
                    "The active level of the digital I/O."
                ::= { cpmDcDigitalIoEntry 5 }

            cpmDcDigitalIoAlarm  OBJECT-TYPE
                SYNTAX      DisplayString   -- 1 character max
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
