                ::= { cpmDcDigitalIoEntry 1 }

            cpmDcDigitalIoName  OBJECT-TYPE
                SYNTAX      DisplayString   -- 15 characters max
                MAX-ACCESS  read-write
                STATUS      current
                DESCRIPTION
