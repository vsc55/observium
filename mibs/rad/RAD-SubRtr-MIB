    rtrSourceAddressApp                 INTEGER,
    rtrSourceAddressType                InetAddressType,
    rtrSourceAddress                    InetAddress,
    rtrSourceAddressIfIndex             Unsigned32,
    rtrSourceAddressRowStatus           RowStatus 
 }

