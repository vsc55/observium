    SYNTAX      INTEGER
    {
        other       (1),
        trap        (2),
        loopback    (3),
        clock       (4),
        managementAll (255)
    }
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object indicates the Application Type."
    ::= { rtrSourceAddressEntry 1 }

