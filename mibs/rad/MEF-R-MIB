    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
      "A conceptual row where each row contains one flow attributes."
    INDEX { flowIdx1,flowIdx2 }
    ::= {  flowTable 1 }

FlowEntry ::= SEQUENCE {
    flowIdx1                      Unsigned32,
    flowIdx2                      Unsigned32,
