uniClassifierKey OBJECT-TYPE
    SYNTAX      BITS {
        vlan            (0),
        innerVlan       (1),
        pBit            (2),
        ipPrecedence    (3),
        ipDscp          (4),
        srcIpAddr       (5),
        destIpAddr      (6),
        legacy          (7),
        dscp            (8),
        macSrcAddr      (9),
        macDestAddr     (10),
        etherType       (11),
        myMac           (12),
        untagged        (13)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
            "This bit map defines port classifier capabilities."
    ::= { uniEntry 36 }


